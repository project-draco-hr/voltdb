{
  List<AbstractPlanNode> pn=null;
  try {
    pn=aide.compile(sql,paramCount,singlePartition);
  }
 catch (  NullPointerException ex) {
    ex.printStackTrace();
    fail();
  }
catch (  Exception ex) {
    ex.printStackTrace();
    fail();
  }
  assertTrue(pn != null);
  assertFalse(pn.isEmpty());
  return pn;
}
