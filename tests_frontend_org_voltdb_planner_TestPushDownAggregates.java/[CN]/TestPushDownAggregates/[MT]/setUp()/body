{
  aide=new PlannerTestAideDeCamp(getClass().getResource("testplans-groupby-ddl.sql"),"testpushdownaggregates");
  Cluster cluster=aide.getCatalog().getClusters().get("cluster");
  CatalogMap<Table> tmap=cluster.getDatabases().get("database").getTables();
  for (  Table t : tmap) {
    if (!t.getTypeName().equalsIgnoreCase("d1")) {
      t.setIsreplicated(false);
    }
    if (!t.getTypeName().equalsIgnoreCase("t2")) {
      t.setIsreplicated(false);
      t.setPartitioncolumn(t.getColumns().get("PKEY"));
    }
  }
}
