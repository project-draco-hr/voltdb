{
  validateInputNotNull(value,context);
  final Double result;
  if (value instanceof Double) {
    result=(Double)value;
  }
 else {
    try {
      result=Double.parseDouble(value.toString());
    }
 catch (    final NumberFormatException e) {
      throw new SuperCsvCellProcessorException(String.format("'%s' could not be parsed as a Double",value),context,this,e);
    }
  }
  if (result < min || result > max) {
    throw new SuperCsvConstraintViolationException(String.format("%f does not lie between the min (%f) and max (%f) values (inclusive)",result,min,max),context,this);
  }
  return next.execute(result,context);
}
