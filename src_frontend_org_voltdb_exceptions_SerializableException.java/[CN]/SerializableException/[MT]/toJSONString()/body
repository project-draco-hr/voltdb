{
  try {
    JSONStringer js=new JSONStringer();
    js.object();
    js.key("type");
    js.value(getExceptionType().ordinal());
    js.key("message");
    js.value(m_message);
    return js.toString();
  }
 catch (  Exception e) {
    return "{ error: \"Unable to serialize exception.\" }";
  }
}
