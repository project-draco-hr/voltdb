{
  executionList.add(listener,exec);
  if (hasListeners.compareAndSet(false,true)) {
    if (delegate.isDone()) {
      executionList.execute();
      return;
    }
    adapterExecutor.execute(new Runnable(){
      @Override public void run(){
        try {
          getUninterruptibly(delegate);
        }
 catch (        Throwable e) {
        }
        executionList.execute();
      }
    }
);
  }
}
