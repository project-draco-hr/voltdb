{
  ByteBuffer buf=ByteBuffer.allocate(4 + (m_tokens.size() * (8 + TokenCompressor.COMPRESSED_SIZE)));
  buf.putInt(m_tokens.size());
  for (  Map.Entry<Long,Integer> e : m_tokens.entrySet()) {
    long token=e.getKey();
    int pid=e.getValue();
    buf.putLong(token);
    buf.putInt(pid);
  }
  return buf.array();
}
