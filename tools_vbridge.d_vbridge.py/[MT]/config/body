@VOLT.Multi_Command(description='Manipulate and view configuration properties.', modifiers=[VOLT.Modifier('get', run_config_get, 'Show one or more configuration properties.', arg_name='KEY'), VOLT.Modifier('reset', run_config_reset, 'Reset configuration properties to default values.'), VOLT.Modifier('set', run_config_set, 'Set one or more configuration properties (use KEY=VALUE format).', arg_name='KEY_VALUE')])
def config(runner):
    runner.go()
