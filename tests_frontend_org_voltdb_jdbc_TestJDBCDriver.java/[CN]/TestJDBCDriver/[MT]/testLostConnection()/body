{
  CallableStatement cs=conn.prepareCall("{call Oopsy(?)}");
  conn.close();
  conn=null;
  try {
    server.shutdown();
  }
 catch (  InterruptedException e) {
  }
  server=null;
  cs.setLong(1,99);
  try {
    cs.execute();
  }
 catch (  SQLException e) {
    assertEquals(e.getSQLState(),SQLError.CONNECTION_FAILURE);
  }
}
