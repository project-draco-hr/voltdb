{
  checkValidHSQLPreprocessing("create table foo ...",HSQLDDLVerb.CREATE,HSQLDDLNoun.TABLE,"foo");
  checkValidHSQLPreprocessing("create table PANTS (ID int, RENAME varchar(50));",HSQLDDLVerb.CREATE,HSQLDDLNoun.TABLE,"pants");
  checkValidHSQLPreprocessing("create table PANTS (\n ID int,\n RENAME varchar(50)\n);",HSQLDDLVerb.CREATE,HSQLDDLNoun.TABLE,"pants");
  checkValidHSQLPreprocessing("create view PANTS (ID int, RENAME varchar(50));",HSQLDDLVerb.CREATE,HSQLDDLNoun.VIEW,"pants");
  checkValidHSQLPreprocessing("create index PANTS (ID int, RENAME varchar(50));",HSQLDDLVerb.CREATE,HSQLDDLNoun.INDEX,"pants");
  checkInvalidHSQLPreprocessing("create tabel PANTS (ID int, RENAME varchar(50));");
  checkInvalidHSQLPreprocessing("craete table PANTS (ID int, RENAME varchar(50));");
  checkInvalidHSQLPreprocessing("create role pants with pockets;");
  checkInvalidHSQLPreprocessing("create role\n pants\n with cuffs;\n");
  checkValidHSQLPreprocessing("drop table pants;",HSQLDDLVerb.DROP,HSQLDDLNoun.TABLE,"pants");
  checkValidHSQLPreprocessing("drop view pants;",HSQLDDLVerb.DROP,HSQLDDLNoun.VIEW,"pants");
  checkValidHSQLPreprocessing("drop index pants;",HSQLDDLVerb.DROP,HSQLDDLNoun.INDEX,"pants");
  checkInvalidHSQLPreprocessing("dorp table pants;");
  checkInvalidHSQLPreprocessing("drop tabel pants;");
  checkValidHSQLPreprocessing("alter table pants add column blargy blarg;",HSQLDDLVerb.ALTER,HSQLDDLNoun.TABLE,"pants");
  checkValidHSQLPreprocessing("alter table pants add constraint blargy blarg;",HSQLDDLVerb.ALTER,HSQLDDLNoun.TABLE,"pants");
  checkValidHSQLPreprocessing("alter index pants",HSQLDDLVerb.ALTER,HSQLDDLNoun.INDEX,"pants");
  checkValidHSQLPreprocessing("alter table pants rename to shorts;",HSQLDDLVerb.ALTER,HSQLDDLNoun.TABLE,"pants");
  checkValidHSQLPreprocessing("alter index pants rename to shorts;",HSQLDDLVerb.ALTER,HSQLDDLNoun.INDEX,"pants");
  checkValidHSQLPreprocessing("alter table pants alter column rename to shorts;",HSQLDDLVerb.ALTER,HSQLDDLNoun.TABLE,"pants");
  checkInvalidHSQLPreprocessing("altre table pants blargy blarg;");
  checkInvalidHSQLPreprocessing("alter tabel pants blargy blarg;");
}
