  validateOptimalPlan("SELECT * from Tmanykeys WHERE T_D1 = ?              ORDER BY T_D0");
  validateOptimalPlan("SELECT * from Tmanykeys WHERE T_D2 = ?              ORDER BY T_D0");
  validateOptimalPlan("SELECT * from Tmanykeys                             ORDER BY T_D0");
}
