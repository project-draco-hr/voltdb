{
  HashMap<Long,Datum> all=new HashMap<Long,Datum>();
  for (  Datum d : historyS)   all.put(d.timestamp,d);
  for (  Datum d : historyM)   all.put(d.timestamp,d);
  for (  Datum d : historyL)   all.put(d.timestamp,d);
  StringBuilder sb=new StringBuilder();
  for (  Datum d : all.values())   sb.append(d.toLine()).append("\n");
  return sb.toString();
}
