{
  String type="TABLE";
  if (table.getMaterializer() != null) {
    type="VIEW";
  }
 else   if (m_database.getConnectors() != null && m_database.getConnectors().get("0") != null && m_database.getConnectors().get("0").getTableinfo() != null && m_database.getConnectors().get("0").getTableinfo().getIgnoreCase(table.getTypeName()) != null) {
    type="EXPORT";
  }
  return type;
}
