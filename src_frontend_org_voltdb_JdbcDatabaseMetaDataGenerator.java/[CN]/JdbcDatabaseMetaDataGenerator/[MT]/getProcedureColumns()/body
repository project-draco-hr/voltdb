{
  VoltTable results=new VoltTable(PROCEDURECOLUMNS_SCHEMA);
  for (  Procedure proc : m_database.getProcedures()) {
    for (    ProcParameter param : proc.getParameters()) {
      results.addRow(null,null,proc.getTypeName(),param.getTypeName(),java.sql.DatabaseMetaData.procedureColumnIn,getColumnSqlDataType(VoltType.get((byte)param.getType())),getColumnSqlTypeName(VoltType.get((byte)param.getType())),getParamPrecisionAndRadix(param)[0],getParamLength(param),getColumnDecimalDigits(VoltType.get((byte)param.getType())),getParamPrecisionAndRadix(param)[1],java.sql.DatabaseMetaData.procedureNullableUnknown,getProcedureColumnRemarks(param,proc),null,null,null,getParamCharOctetLength(param),param.getIndex() + 1,"",proc.getTypeName());
    }
  }
  return results;
}
