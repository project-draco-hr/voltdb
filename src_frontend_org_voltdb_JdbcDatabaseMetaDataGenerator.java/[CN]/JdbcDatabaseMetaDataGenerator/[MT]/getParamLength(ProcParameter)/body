{
  int length=0;
  VoltType type=VoltType.get((byte)param.getType());
switch (type) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case TIMESTAMP:
case FLOAT:
case DECIMAL:
    length=type.getLengthInBytesForFixedTypes();
  break;
case STRING:
case VARBINARY:
length=VoltType.MAX_VALUE_LENGTH;
break;
default :
}
return length;
}
