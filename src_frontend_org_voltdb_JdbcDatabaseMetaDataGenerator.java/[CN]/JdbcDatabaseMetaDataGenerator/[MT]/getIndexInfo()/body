{
  VoltTable results=new VoltTable(INDEXINFO_SCHEMA);
  for (  Table table : m_database.getTables()) {
    for (    Index index : table.getIndexes()) {
      results.addRow(null,null,table.getTypeName(),index.getUnique() ? 1 : 0,null,index.getTypeName(),null,null,null,null,null,null,null);
    }
  }
  return results;
}
