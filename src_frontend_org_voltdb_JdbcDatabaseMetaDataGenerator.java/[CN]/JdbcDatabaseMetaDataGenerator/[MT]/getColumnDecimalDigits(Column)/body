{
  Integer num_dec_digits=null;
  VoltType type=VoltType.get((byte)column.getType());
switch (type) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case TIMESTAMP:
case FLOAT:
case STRING:
case VARBINARY:
    num_dec_digits=null;
  break;
case DECIMAL:
num_dec_digits=VoltDecimalHelper.kDefaultScale;
break;
default :
}
return num_dec_digits;
}
