{
  VoltTable results=new VoltTable(COLUMN_SCHEMA);
  for (  Table table : m_database.getTables()) {
    for (    Column column : table.getColumns()) {
      results.addRow(null,null,table.getTypeName(),column.getTypeName(),getColumnSqlDataType(column),getColumnSqlTypeName(column),getColumnSizeAndRadix(column)[0],null,getColumnDecimalDigits(column),getColumnSizeAndRadix(column)[1],getNullable(column),null,null,null,null,null,null,null,null,null,null,null,"NO");
    }
  }
  return results;
}
