{
  PendingStatsRequest psr=m_pendingRequests.remove(requestId);
  if (psr == null) {
    return;
  }
  hostLog.warn("Stats request " + requestId + " timed out, sending error to client");
  ClientResponseImpl response=new ClientResponseImpl(ClientResponse.GRACEFUL_FAILURE,Byte.MIN_VALUE,null,new VoltTable[0],"Stats request hit sixty second timeout before all responses were received");
  response.setClientHandle(psr.clientData);
  psr.c.writeStream().enqueue(response);
}
