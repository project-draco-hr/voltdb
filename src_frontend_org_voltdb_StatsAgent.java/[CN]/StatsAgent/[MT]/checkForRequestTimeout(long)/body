{
  PendingStatsRequest psr=m_pendingRequests.remove(requestId);
  if (psr == null) {
    return;
  }
  hostLog.warn("Stats request " + requestId + " timed out, sending error to client");
  sendErrorResponse(psr.c,ClientResponse.GRACEFUL_FAILURE,"Stats request hit sixty second timeout before all responses were received",psr.clientData);
}
