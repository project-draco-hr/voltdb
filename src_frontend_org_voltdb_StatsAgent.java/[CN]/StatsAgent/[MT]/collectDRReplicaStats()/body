{
  VoltTable[] stats=null;
  VoltTable[] statusStats=collectDRReplicaStatusStats();
  VoltTable[] perfStats=collectDRReplicaPerformanceStats();
  if (statusStats != null && perfStats != null) {
    stats=new VoltTable[2];
    stats[0]=statusStats[0];
    stats[1]=perfStats[0];
  }
  return stats;
}
