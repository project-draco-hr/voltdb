{
  List<Long> catalogIds=Arrays.asList(new Long[]{0L});
  Long now=System.currentTimeMillis();
  VoltTable[] stats=null;
  VoltTable ssStats=getStats(SysProcSelector.SNAPSHOTSTATUS,catalogIds,false,now);
  if (ssStats != null) {
    stats=new VoltTable[1];
    stats[0]=ssStats;
  }
  return stats;
}
