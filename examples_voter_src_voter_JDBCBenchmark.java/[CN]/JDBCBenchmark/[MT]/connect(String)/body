{
  System.out.println("Connecting to VoltDB...");
  Class.forName("org.voltdb.jdbc.Driver");
  String url="jdbc:voltdb://" + config.servers;
  client=DriverManager.getConnection(url,"","");
  periodicStatsContext=((IVoltDBConnection)client).getClientStatsContext();
  fullStatsContext=((IVoltDBConnection)client).getClientStatsContext();
}
