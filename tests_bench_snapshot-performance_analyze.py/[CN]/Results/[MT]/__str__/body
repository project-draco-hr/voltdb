def __str__(self):

    def format(label, value):
        return ('%20s: %s' % (label, str(value)))
    if (self.snapshot_count == 0):
        duration = 0.0
    else:
        duration = (self.snapshot_total_duration / self.snapshot_count)
    return '\n'.join([('===== Results (%s) =====' % self.runname), format('Throughput', self.throughput), format('Latency', self.latency), format('Snapshots', self.snapshot_count), format('Snapshot duration', ('%0.2f' % duration))])
