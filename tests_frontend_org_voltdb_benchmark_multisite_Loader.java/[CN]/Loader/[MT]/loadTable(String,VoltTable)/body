{
  SyncCallback cb=new SyncCallback();
  try {
    m_voltClient.callProcedure(cb,"@LoadMultipartitionTable",tablename,table);
  }
 catch (  IOException e) {
    e.printStackTrace();
    System.exit(-1);
  }
  try {
    cb.waitForResponse();
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
  ;
}
