{
  Thread C=new Thread(){
    @Override public void run(){
      generateCustomers();
    }
  }
;
  Thread F=new Thread(){
    @Override public void run(){
      generateFlights();
    }
  }
;
  Thread R=new Thread(){
    @Override public void run(){
      generateReservations();
    }
  }
;
  C.start();
  F.start();
  R.start();
  try {
    C.join();
    F.join();
    R.join();
    m_voltClient.drain();
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
    System.exit(-1);
  }
  System.err.println("Multisite loader done.");
}
