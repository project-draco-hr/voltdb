{
  try {
    final ClientResponseImpl error=handleRead(message,this,c);
    if (error != null) {
      ByteBuffer buf=ByteBuffer.allocate(error.getSerializedSize());
      error.flattenToBuffer(buf).flip();
      c.writeStream().enqueue(buf);
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
