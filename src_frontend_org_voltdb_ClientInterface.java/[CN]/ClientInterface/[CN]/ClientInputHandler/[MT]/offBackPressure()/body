{
  if (VoltDB.instance().isIV2Enabled()) {
    return new Runnable(){
      @Override public void run(){
        if (!m_acg.get().hasBackPressure()) {
          m_connection.enableReadSelection();
        }
      }
    }
;
  }
  return new Runnable(){
    @Override public void run(){
      m_backpressureLock.lock();
      try {
        if (!m_hasDTXNBackPressure) {
          m_connection.enableReadSelection();
        }
      }
  finally {
        m_backpressureLock.unlock();
      }
    }
  }
;
}
