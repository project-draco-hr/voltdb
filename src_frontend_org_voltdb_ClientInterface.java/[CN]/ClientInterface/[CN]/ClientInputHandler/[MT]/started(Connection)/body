{
  m_connection=c;
  if (VoltDB.instance().isIV2Enabled()) {
    m_connectionSpecificStuff.put(c.connectionId(),Pair.of(new ClientInterfaceHandleManager(m_isAdmin,c),m_acg.get()));
    m_acg.get().addMember(this);
    if (!m_acg.get().hasBackPressure()) {
      c.enableReadSelection();
    }
  }
}
