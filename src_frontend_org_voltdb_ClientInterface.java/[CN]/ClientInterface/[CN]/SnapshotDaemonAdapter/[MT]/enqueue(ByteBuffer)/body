{
  ClientResponseImpl resp=new ClientResponseImpl();
  b.position(4);
  try {
    resp.initFromBuffer(b);
  }
 catch (  IOException ioe) {
    hostLog.error("Unable to deserialize ClientResponse from snapshot",ioe);
    return;
  }
  m_snapshotDaemon.processClientResponse(resp,resp.getClientHandle());
}
