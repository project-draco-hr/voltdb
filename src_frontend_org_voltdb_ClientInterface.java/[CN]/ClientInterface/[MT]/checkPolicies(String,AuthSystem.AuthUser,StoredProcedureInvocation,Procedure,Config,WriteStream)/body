{
  List<InvocationAcceptancePolicy> policies=m_policies.get(name);
  if (policies != null) {
    for (    InvocationAcceptancePolicy policy : policies) {
      if (catProc != null) {
        if (!policy.shouldAccept(user,task,catProc,s)) {
          return false;
        }
      }
 else {
        if (!policy.shouldAccept(user,task,sysProc,s)) {
          return false;
        }
      }
    }
  }
  return true;
}
