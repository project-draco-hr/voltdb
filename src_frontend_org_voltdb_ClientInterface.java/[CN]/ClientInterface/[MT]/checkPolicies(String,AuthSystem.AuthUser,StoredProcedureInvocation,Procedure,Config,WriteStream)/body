{
  List<InvocationAcceptancePolicy> policies=m_policies.get(name);
  if (policies != null) {
    for (    InvocationAcceptancePolicy policy : policies) {
      if (catProc != null) {
        return policy.shouldAccept(user,task,catProc,s);
      }
 else {
        return policy.shouldAccept(user,task,sysProc,s);
      }
    }
  }
  return true;
}
