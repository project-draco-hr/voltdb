{
  assert(VoltDB.instance().isIV2Enabled());
  final long initiatorHSId=m_iv2Masters.get(partitionId);
  MultiPartitionParticipantMessage mppm=new MultiPartitionParticipantMessage(initiatorHSId,m_cartographer.getHSIdForMultiPartitionInitiator(),txnId,false);
  m_mailbox.send(initiatorHSId,mppm);
}
