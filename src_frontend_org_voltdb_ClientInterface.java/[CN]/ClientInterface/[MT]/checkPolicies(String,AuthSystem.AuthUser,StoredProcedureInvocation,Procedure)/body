{
  List<InvocationAcceptancePolicy> policies=m_policies.get(name);
  ClientResponseImpl error=null;
  if (policies != null) {
    for (    InvocationAcceptancePolicy policy : policies) {
      if ((error=policy.shouldAccept(user,task,catProc)) != null) {
        return error;
      }
    }
  }
  return null;
}
