{
  int[] involvedPartitions=null;
  try {
    CatalogMap<Table> tables=m_catalogContext.get().database.getTables();
    int partitionParamType=getLoadSinglePartitionTablePartitionParamType(tables,task);
    byte[] valueToHash=(byte[])task.getParameterAtIndex(0);
    involvedPartitions=new int[]{TheHashinator.getPartitionForParameter(partitionParamType,valueToHash)};
  }
 catch (  Exception e) {
    authLog.warn(e.getMessage());
    return new ClientResponseImpl(ClientResponseImpl.UNEXPECTED_FAILURE,new VoltTable[0],e.getMessage(),task.clientHandle);
  }
  assert(involvedPartitions != null);
  createTransaction(handler.connectionId(),task,catProc.getReadonly(),catProc.getSinglepartition(),catProc.getEverysite(),involvedPartitions,buf.capacity(),System.currentTimeMillis());
  return null;
}
