{
  int[] allPartitions=new int[partitionCount];
  int index=0;
  for (  Integer partition : cartographer.getPartitions()) {
    if (partition != MpInitiator.MP_INIT_PID) {
      allPartitions[index++]=partition;
    }
  }
  final ClientInterface ci=new ClientInterface(clientIntf,clientPort,adminIntf,adminPort,context,messenger,replicationRole,cartographer,allPartitions);
  return ci;
}
