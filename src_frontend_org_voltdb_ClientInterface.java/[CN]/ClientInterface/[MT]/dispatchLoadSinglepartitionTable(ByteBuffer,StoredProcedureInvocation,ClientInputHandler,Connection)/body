{
  int[] involvedPartitions=null;
  try {
    CatalogMap<Table> tables=m_catalogContext.get().database.getTables();
    Object valueToHash=LoadSinglepartitionTable.partitionValueFromInvocation(tables,task);
    involvedPartitions=new int[]{TheHashinator.hashToPartition(valueToHash)};
  }
 catch (  Exception e) {
    authLog.warn(e.getMessage());
    return new ClientResponseImpl(ClientResponseImpl.UNEXPECTED_FAILURE,new VoltTable[0],e.getMessage(),task.clientHandle);
  }
  assert(involvedPartitions != null);
  createTransaction(handler.connectionId(),handler.m_hostname,handler.isAdmin(),task,false,true,false,involvedPartitions,involvedPartitions.length,ccxn,buf.capacity(),System.currentTimeMillis(),false);
  return null;
}
