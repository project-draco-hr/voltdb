{
  if (VoltDB.instance().getMode() != OperationMode.PAUSED || handler.isAdmin()) {
    return true;
  }
  if (procedure.getSystemproc() && (invocation.procName.equals("@AdHoc") || invocation.procName.equals("@AdHocSpForTest"))) {
    return true;
  }
 else {
    return procedure.getReadonly();
  }
}
