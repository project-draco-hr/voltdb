{
  ParameterSet params=task.getParams();
  if ((params.m_params.length != 0) && (((String)params.toArray()[0]).equals("DR"))) {
    try {
      VoltDB.instance().getStatsAgent().collectStats(ccxn,task.clientHandle,"DR");
      return null;
    }
 catch (    Exception e) {
      return errorResponse(ccxn,task.clientHandle,ClientResponse.UNEXPECTED_FAILURE,null,e,true);
    }
  }
 else {
    int[] involvedPartitions=m_allPartitions;
    createTransaction(handler.connectionId(),handler.m_hostname,handler.isAdmin(),task,sysProc.getReadonly(),sysProc.getSinglepartition(),sysProc.getEverysite(),involvedPartitions,involvedPartitions.length,ccxn,buf.capacity(),System.currentTimeMillis());
    return null;
  }
}
