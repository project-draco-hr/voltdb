{
  ParameterSet params=task.getParams();
  byte[] catalogBytes=null;
  if (params.m_params[0] instanceof String) {
    catalogBytes=Encoder.hexDecode((String)params.m_params[0]);
  }
 else   if (params.m_params[0] instanceof byte[]) {
    catalogBytes=(byte[])params.m_params[0];
  }
  String deploymentString=(String)params.m_params[1];
  LocalObjectMessage work=new LocalObjectMessage(new CatalogChangeWork(m_siteId,task.clientHandle,handler.connectionId(),handler.m_hostname,handler.isAdmin(),ccxn,catalogBytes,deploymentString));
  try {
    m_mailbox.send(Long.MIN_VALUE,work);
  }
 catch (  MessagingException ex) {
    return new ClientResponseImpl(ClientResponseImpl.GRACEFUL_FAILURE,new VoltTable[0],"Failed to process UpdateApplicationCatalog request." + " No data was read or written.",task.clientHandle);
  }
  return null;
}
