{
  MessengerSite site=m_hostMessenger.getSite(siteId);
  if (site == null) {
    return;
  }
  Mailbox mailbox=site.getMailbox(mailboxId);
  if (mailbox == null) {
    System.err.printf("Message (%s) sent to unknown mailbox id: %d:%d @ (%s:%d)\n",message.getClass().getSimpleName(),siteId,mailboxId,m_ipAddress.toString(),m_tcpPort);
    assert(false);
    return;
  }
  mailbox.deliver(message);
}
