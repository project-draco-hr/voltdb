{
  truncationTxnId=txnId;
  long lastExecuted=lastExecutedTxnId;
  if (truncationTxnId <= lastExecuted) {
    InitiateTaskMessage txn;
    while ((txn=inflight.peek()) != null) {
      if (txn.getTransactionId() <= truncationTxnId) {
        inflight.remove();
      }
 else {
        break;
      }
    }
  }
}
