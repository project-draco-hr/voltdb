{
  prt(err);
  timer.cancel();
  try {
    client.drain();
  }
 catch (  NoConnectionsException e) {
    e.printStackTrace();
  }
catch (  InterruptedException e) {
    e.printStackTrace();
  }
  try {
    printResults();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  try {
    client.close();
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
}
