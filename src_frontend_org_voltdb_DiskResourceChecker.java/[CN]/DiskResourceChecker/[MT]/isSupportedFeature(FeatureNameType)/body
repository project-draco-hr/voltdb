{
  LicenseApi licenseApi=VoltDB.instance().getLicenseApi();
switch (featureName) {
case COMMANDLOG:
case COMMANDLOGSNAPSHOT:
case SNAPSHOTS:
    return licenseApi.isCommandLoggingAllowed();
case DROVERFLOW:
  return licenseApi.isDrReplicationAllowed();
case EXPORTOVERFLOW:
return MiscUtils.isPro();
default :
return false;
}
}
