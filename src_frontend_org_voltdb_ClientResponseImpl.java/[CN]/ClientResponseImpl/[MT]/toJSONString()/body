{
  JSONStringer js=new JSONStringer();
  try {
    js.object();
    js.key("results");
    js.array();
    for (    VoltTable o : results) {
      js.value(o);
    }
    js.endArray();
    js.endObject();
  }
 catch (  JSONException e) {
    e.printStackTrace();
    throw new RuntimeException("Failed to serialized a parameter set to JSON.",e);
  }
  return js.toString();
}
