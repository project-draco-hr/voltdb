{
  hostLog.debug("STARTING: " + this);
  if (!m_txnState.isReadOnly()) {
    siteConnection.truncateUndoLog(m_completeMsg.isRollback(),m_txnState.getBeginUndoToken(),m_txnState.m_spHandle,m_txnState.m_spHandle,m_txnState.getUndoLog());
  }
  if (!m_completeMsg.isRestart()) {
    doCommonSPICompleteActions();
    logToDR();
    hostLog.debug("COMPLETE: " + this);
  }
 else {
    m_txnState.setBeginUndoToken(Site.kInvalidUndoToken);
    hostLog.debug("RESTART: " + this);
  }
}
