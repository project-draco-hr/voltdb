{
  assert(m_children.size() == 0);
  m_hasSignificantOutputSchema=true;
  if (m_outputSchema == null) {
    m_outputSchema=new NodeSchema();
    m_outputSchema.addColumn(new SchemaColumn(m_outputExpression.getTableName(),m_outputExpression.getColumnName(),m_outputExpression.getColumnAlias(),m_outputExpression));
  }
}
