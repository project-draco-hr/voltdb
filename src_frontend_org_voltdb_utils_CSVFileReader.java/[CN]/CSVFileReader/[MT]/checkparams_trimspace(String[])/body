{
  if (lineValues.length != m_columnCount) {
    return "Error: Incorrect number of columns. " + lineValues.length + " found, "+ m_columnCount+ " expected.";
  }
  for (int i=0; i < lineValues.length; i++) {
    if (lineValues[i] == null) {
      if (m_config.blank.equalsIgnoreCase("error")) {
        return "Error: blank item";
      }
 else       if (m_config.blank.equalsIgnoreCase("empty")) {
        lineValues[i]=m_blankStrings.get(m_columnTypes[i]);
      }
    }
 else {
      if (m_config.nowhitespace && (lineValues[i].charAt(0) == ' ' || lineValues[i].charAt(lineValues[i].length() - 1) == ' ')) {
        return "Error: White Space Detected in nowhitespace mode.";
      }
 else {
        lineValues[i]=lineValues[i].trim();
      }
      if (lineValues[i].equals("NULL") || lineValues[i].equals(Constants.CSV_NULL) || lineValues[i].equals(Constants.QUOTED_CSV_NULL)) {
        lineValues[i]=null;
      }
    }
  }
  return null;
}
