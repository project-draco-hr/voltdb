{
  final Client client=getClient();
  final LocalSingleProcessServerELT config=(LocalSingleProcessServerELT)getServerConfig();
  for (int i=0; i < 10; i++) {
    final Object[] rowdata=TestSQLTypesSuite.m_midValues;
    if (i % 2 == 0)     config.addRow("ALLOW_NULLS",convertValsToRow(i,rowdata));
    final Object[] params=convertValsToParams(i,rowdata);
    client.callProcedure("Insert",params);
  }
  quiesceAndVerifyFalse(client);
}
