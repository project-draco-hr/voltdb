{
switch (type) {
case Types.SQL_DECIMAL:
case Types.SQL_DOUBLE:
case Types.SQL_FLOAT:
case Types.SQL_NUMERIC:
case Types.SQL_REAL:
case Types.SQL_SMALLINT:
case Types.TINYINT:
    return Boolean.FALSE;
case Types.SQL_BIGINT:
case Types.SQL_INTEGER:
  return Boolean.TRUE;
default :
return null;
}
}
