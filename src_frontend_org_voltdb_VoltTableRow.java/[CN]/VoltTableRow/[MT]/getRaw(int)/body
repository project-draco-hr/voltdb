{
  byte[] retval;
  int pos;
  int offset=getOffset(columnIndex);
  VoltType type=getColumnType(columnIndex);
  int length=type.getLengthInBytesForFixedTypesWithoutCheck();
switch (type) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case TIMESTAMP:
case FLOAT:
case DECIMAL:
    retval=new byte[length];
  pos=m_buffer.position();
m_buffer.position(offset);
m_buffer.get(retval);
m_buffer.position(pos);
return retval;
case STRING:
case VARBINARY:
length=m_buffer.getInt(offset);
if (length == VoltTable.NULL_STRING_INDICATOR) {
length=0;
}
length+=4;
retval=new byte[length];
pos=m_buffer.position();
m_buffer.position(offset);
m_buffer.get(retval);
m_buffer.position(pos);
return retval;
default :
throw new RuntimeException("Unknown type");
}
}
