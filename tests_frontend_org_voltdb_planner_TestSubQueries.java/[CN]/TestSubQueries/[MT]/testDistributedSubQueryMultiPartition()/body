{
{
    failToCompile("select D1, P2.D FROM (SELECT A, D D1 FROM P1 WHERE A=1) TEMP1, P2 WHERE TEMP1.A = P2.A ","Statements are too complex in set operation or statement with sub-query using multiple partitioned tables.");
  }
{
    failToCompile("select A1, A2 FROM (SELECT A A1 FROM P1) TEMP1, (SELECT A A2 FROM P2) TEMP2 WHERE TEMP1.A1 = TEMP2.A2 ","Statements are too complex in set operation or statement with sub-query using multiple partitioned tables.");
  }
}
