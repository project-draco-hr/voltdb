{
  StringBuilder sb=new StringBuilder();
  sb.append(String.format("0-%dms by %dms: [",msPerBucket * numberOfBuckets,msPerBucket));
  for (int i=0; i < numberOfBuckets; i++) {
    sb.append(buckets[i]);
    if (i == (numberOfBuckets - 1)) {
      sb.append("]");
    }
 else {
      sb.append(", ");
    }
  }
  return sb.toString();
}
