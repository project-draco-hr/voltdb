{
switch (type) {
case NULL:
    return null;
case BIGINT:
  return Longs.fromByteArray(value);
case STRING:
return new String(value,Charsets.UTF_8);
case INTEGER:
return Ints.fromByteArray(value);
case SMALLINT:
return Shorts.fromByteArray(value);
case TINYINT:
return value[0];
default :
throw new RuntimeException("TheHashinator#bytesToValue failed to convert a non-partitionable type.");
}
}
