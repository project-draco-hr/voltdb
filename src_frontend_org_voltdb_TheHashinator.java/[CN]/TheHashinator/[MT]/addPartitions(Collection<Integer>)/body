{
  HashinatorType type=getConfiguredHashinatorType();
switch (type) {
case LEGACY:
    throw new RuntimeException("Legacy hashinator doesn't support adding partitions");
case ELASTIC:
  return ElasticHashinator.addPartitions(instance.get().getSecond(),newPartitions,ElasticHashinator.DEFAULT_TOKENS_PER_PARTITION);
}
throw new RuntimeException("Should not reach here");
}
