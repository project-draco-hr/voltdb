{
  assert(logString.isTxnStart());
  m_txnId=logString.getTxnId();
  m_closed=false;
  m_rollback=false;
  m_multipart=logString.isMultiPart();
  m_readonly=logString.isReadOnly();
  m_coord=logString.isCoordinator();
  m_selfFail=false;
  m_otherFail=false;
  m_failedSites=new HashSet<Integer>();
}
