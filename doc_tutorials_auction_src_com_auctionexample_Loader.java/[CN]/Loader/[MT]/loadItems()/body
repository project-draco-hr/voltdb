{
  ArrayList<Integer> itemIds=new ArrayList<Integer>();
  URL url=Loader.class.getResource("datafiles/items.txt");
  String[] myOptions={"--file=" + url.getPath(),"--procedure=InsertIntoItemAndBid","--maxerrors=50","--user=program","--password=pass","--port="};
  try {
    CSVLoader.main(myOptions);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  CSVReader csvReader=new CSVReader(new FileReader(url.getPath()));
  String[] line=null;
  try {
    while ((line=csvReader.readNext()) != null)     itemIds.add(Integer.parseInt(line[0]));
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  return itemIds;
}
