{
  ArrayList<Integer> categoryIds=new ArrayList<Integer>();
  URL url=Loader.class.getResource("datafiles/categories.txt");
  String[] myOptions={"--file=" + url.getPath(),"--procedure=InsertIntoCategory","--maxerrors=50","--user=program","--password=pass","--port="};
  try {
    CSVLoader.main(myOptions);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  InterruptedException e) {
    e.printStackTrace();
  }
  ArrayList<String> list=CSVLoader.getFirstIds();
  for (  String id : list) {
    categoryIds.add(Integer.parseInt(id));
  }
  return categoryIds;
}
