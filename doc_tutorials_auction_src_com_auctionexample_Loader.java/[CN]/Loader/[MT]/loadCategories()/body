{
  ArrayList<Integer> categoryIds=new ArrayList<Integer>();
  String userHome=System.getProperty("user.home");
  String[] myOptions={"--inputfile=" + userHome + "/workspace/voltdb/doc/tutorials/auction/src/com/auctionexample/datafiles/categories.txt","--procedurename=InsertIntoCategory","--abortfailurecount=50"};
  CSVLoader loader=new CSVLoader(myOptions);
  while (loader.readNext()) {
    categoryIds.add(Integer.parseInt(loader.insertLine(null)[0]));
  }
  loader.flush();
  return categoryIds;
}
