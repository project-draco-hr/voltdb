{
  ArrayList<Integer> categoryIds=new ArrayList<Integer>();
  String userHome=System.getProperty("user.home");
  String[] myOptions={"--file=" + userHome + "/workspace/voltdb/doc/tutorials/auction/src/com/auctionexample/datafiles/categories.txt","--procedure=InsertIntoCategory","--maxerrors=50","--user=program","--password=pass","--port="};
  new CSVLoader(myOptions);
  while (CSVLoader.readNext()) {
    categoryIds.add(Integer.parseInt(CSVLoader.insertLine(null,3)[0]));
  }
  CSVLoader.drain();
  CSVLoader.produceFiles();
  CSVLoader.flush();
  return categoryIds;
}
