{
  ArrayList<Integer> userIds=new ArrayList<Integer>();
  String userHome=System.getProperty("user.home");
  String[] myOptions={"--file=" + userHome + "/workspace/voltdb/doc/tutorials/auction/src/com/auctionexample/datafiles/users.txt","--procedure=InsertIntoUser","--maxerrors=50","--user=program","--password=pass","--port="};
  new CSVLoader(myOptions);
  while (CSVLoader.readNext()) {
    userIds.add(Integer.parseInt(CSVLoader.insertLine(null,8)[0]));
  }
  CSVLoader.drain();
  CSVLoader.produceFiles();
  CSVLoader.flush();
  return userIds;
}
