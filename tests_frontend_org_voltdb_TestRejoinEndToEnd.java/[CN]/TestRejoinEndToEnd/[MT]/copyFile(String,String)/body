{
  File inputFile=new File(fromPath);
  File outputFile=new File(toPath);
  try {
    FileReader in=new FileReader(inputFile);
    FileWriter out=new FileWriter(outputFile);
    int c;
    while ((c=in.read()) != -1)     out.write(c);
    in.close();
    out.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
    fail();
  }
}
