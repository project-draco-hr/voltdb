{
  m_es=Executors.newFixedThreadPool(m_instances.size(),getThreadFactory("SocketStreamImporter","SocketImporterImpl",ImportHandlerProxy.MEDIUM_STACK_SIZE));
  for (  SocketImporterImpl impl : m_instances) {
    final SocketImporterImpl importer=impl;
    m_es.submit(new Runnable(){
      @Override public void run(){
        importer.readyForData();
      }
    }
);
  }
}
