{
  for (  ClientConnectionHandler s : m_clients) {
    try {
      s.stopClient();
    }
 catch (    Exception e) {
      warn(e,"Error closing socket client connection");
    }
  }
  m_clients.clear();
  try {
    m_serverSocket.close();
    m_serverSocket=null;
  }
 catch (  IOException ex) {
    warn(ex,"Error closing socket importer server socket");
  }
}
