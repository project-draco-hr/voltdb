{
  this.Active=false;
  try {
    System.out.println("Closing Client Connection[" + this.Id + "] :: "+ this.Socket.getInetAddress());
  }
 catch (  Exception x) {
  }
  try {
    this.Cache.close();
  }
 catch (  Exception x) {
  }
  try {
    this.in.close();
  }
 catch (  Exception x) {
  }
  try {
    this.out.close();
  }
 catch (  Exception x) {
  }
  try {
    this.Socket.close();
  }
 catch (  Exception x) {
  }
  return false;
}
