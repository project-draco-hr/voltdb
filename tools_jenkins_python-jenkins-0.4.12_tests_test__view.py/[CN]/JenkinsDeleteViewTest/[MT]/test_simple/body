@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_simple(self, jenkins_mock):
    jenkins_mock.side_effect = [None, jenkins.NotFoundException()]
    self.j.delete_view(u'Test View')
    self.assertEqual(jenkins_mock.call_args_list[0][0][0].get_full_url(), self.make_url('view/Test%20View/doDelete'))
    self._check_requests(jenkins_mock.call_args_list)
