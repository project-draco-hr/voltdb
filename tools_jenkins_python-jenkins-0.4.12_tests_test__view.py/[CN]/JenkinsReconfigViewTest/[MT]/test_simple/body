@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_simple(self, jenkins_mock):
    jenkins_mock.side_effect = [json.dumps({'name': 'Test View', }), None]
    self.j.reconfig_view(u'Test View', self.config_xml)
    self.assertEqual(jenkins_mock.call_args[0][0].get_full_url(), self.make_url('view/Test%20View/config.xml'))
    self._check_requests(jenkins_mock.call_args_list)
