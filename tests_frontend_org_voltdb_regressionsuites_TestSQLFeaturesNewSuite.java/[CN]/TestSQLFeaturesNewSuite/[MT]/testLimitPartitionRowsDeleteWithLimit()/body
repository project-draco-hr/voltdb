{
  if (isHSQL())   return;
  Client client=getClient();
  VoltTable vt;
  for (int i=0; i < 50; ++i) {
    vt=client.callProcedure("@AdHoc","INSERT INTO events_capped VALUES (NOW, " + i + ")").getResults()[0];
    validateTableOfScalarLongs(vt,new long[]{1});
  }
  vt=client.callProcedure("@AdHoc","select info from events_capped order by when_occurred asc").getResults()[0];
  validateTableOfScalarLongs(vt,new long[]{45,46,47,48,49});
}
