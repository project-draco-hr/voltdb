{
  String sql="SELECT DECODE(COALESCE(" + col1 + ","+ col2+ "),COALESCE("+ col2+ ","+ col1+ "), 0, 1) "+ "FROM C_NULL WHERE ID = 1;";
  ClientResponse cr=cl.callProcedure("@AdHoc",sql);
  assertEquals(cr.getStatus(),ClientResponse.SUCCESS);
  VoltTable result=cr.getResults()[0];
  result.advanceRow();
  long rv=result.getLong(0);
  assertEquals(rv,0);
  sql="SELECT DECODE(COALESCE(" + col1 + ","+ col2+ "),"+ col1+ ", 0, 1) "+ "FROM C_NULL WHERE ID = 1;";
  cr=cl.callProcedure("@AdHoc",sql);
  assertEquals(cr.getStatus(),ClientResponse.SUCCESS);
  result=cr.getResults()[0];
  result.advanceRow();
  rv=result.getLong(0);
  assertEquals(rv,0);
}
