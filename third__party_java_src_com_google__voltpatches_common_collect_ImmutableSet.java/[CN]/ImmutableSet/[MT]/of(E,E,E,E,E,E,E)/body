{
  final int paramCount=6;
  Object[] elements=new Object[paramCount + others.length];
  elements[0]=e1;
  elements[1]=e2;
  elements[2]=e3;
  elements[3]=e4;
  elements[4]=e5;
  elements[5]=e6;
  System.arraycopy(others,0,elements,paramCount,others.length);
  return construct(elements.length,elements);
}
