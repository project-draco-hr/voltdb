  client.callProcedure("DoNothing2",1);
  client.callProcedure("DoNothing3",1);
  boolean exceptionThrown=false;
  try {
    client.callProcedure("DoNothing4",1);
  }
 catch (  ProcCallException e) {
    e.printStackTrace();
    exceptionThrown=true;
  }
  assertTrue(exceptionThrown);
