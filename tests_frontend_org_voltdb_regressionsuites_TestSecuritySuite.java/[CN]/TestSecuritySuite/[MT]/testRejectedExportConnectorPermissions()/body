{
  boolean caught=false;
  ExportTestClient eclient=new ExportTestClient(1);
  try {
    eclient.connectToExportServers("user2","password");
  }
 catch (  IOException e) {
    caught=true;
  }
  assertTrue(caught);
  try {
    eclient.disconnectFromExportServers();
  }
 catch (  IOException ignored) {
  }
}
