{
  boolean replicated=(counter % 5) == 0;
  boolean batched=(counter % 11) == 0;
  long rwMix=counter % 3;
  String sql="";
  if (batched) {
    if (rwMix == 0) {
      sql+=nextWriteAdHocStmt(replicated);
    }
    if (rwMix == 1) {
      sql+=nextReadAdHocStmt(replicated);
    }
  }
  sql+=nextWriteAdHocStmt(replicated);
  if (batched) {
    if (rwMix == 2) {
      sql+=nextReadAdHocStmt(replicated);
    }
  }
  counter++;
  return sql;
}
