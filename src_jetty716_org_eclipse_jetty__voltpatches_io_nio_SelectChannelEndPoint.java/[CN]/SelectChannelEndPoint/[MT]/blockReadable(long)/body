{
synchronized (this) {
    long start=_selectSet.getNow();
    try {
      _readBlocked=true;
      while (isOpen() && _readBlocked) {
        try {
          updateKey();
          this.wait(timeoutMs);
          timeoutMs-=_selectSet.getNow() - start;
          if (_readBlocked && timeoutMs <= 0)           return false;
        }
 catch (        InterruptedException e) {
          Log.warn(e);
        }
      }
    }
  finally {
      _readBlocked=false;
    }
  }
  return true;
}
