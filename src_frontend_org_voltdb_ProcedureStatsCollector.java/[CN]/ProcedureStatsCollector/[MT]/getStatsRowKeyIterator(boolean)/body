{
  m_interval=interval;
  return new Iterator<Object>(){
    boolean givenNext=false;
    @Override public boolean hasNext(){
      if (!m_interval) {
        if (m_invocations == 0) {
          return false;
        }
      }
 else       if (m_invocations - m_lastInvocations == 0) {
        return false;
      }
      return !givenNext;
    }
    @Override public Object next(){
      if (!givenNext) {
        givenNext=true;
        return new Object();
      }
      return null;
    }
    @Override public void remove(){
    }
  }
;
}
