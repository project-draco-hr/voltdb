{
  m_interval=interval;
  return new Iterator<Object>(){
    boolean givenNext=false;
    @Override public boolean hasNext(){
      if (!getInterval()) {
        if (getInvocations() == 0) {
          return false;
        }
      }
 else       if (getInvocations() - getLastInvocations() == 0) {
        return false;
      }
      return !givenNext;
    }
    @Override public Object next(){
      if (!givenNext) {
        givenNext=true;
        return new Object();
      }
      return null;
    }
    @Override public void remove(){
    }
  }
;
}
