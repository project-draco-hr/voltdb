{
  if (iv2log.isTraceEnabled()) {
    String logmsg=new String("createTxn %s ciHandle %s initHSId %s proc %s");
    iv2log.trace(String.format(logmsg,CoreUtils.hsIdToString(msg.getInitiatorHSId()),ClientInterfaceHandleManager.handleToString(msg.getClientInterfaceHandle()),CoreUtils.hsIdToString(msg.getCoordinatorHSId()),msg.getStoredProcedureInvocation().getProcName()));
  }
}
