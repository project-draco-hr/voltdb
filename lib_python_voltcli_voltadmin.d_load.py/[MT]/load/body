@VOLT.Client(description='Load (restore) a VoltDB database snapshot.', cli_options=(VOLT.CLIValue('-c', '--catalog', 'catalog', 'the application catalog jar file path', required=True), VOLT.CLIValue('-i', '--id', 'unique_id', 'the unique snapshot identifier', required=True)))
def load(runner):
    proc = VOLT.VoltProcedure(runner.client, '@SnapshotRestore', [VOLT.FastSerializer.VOLTTYPE_STRING, VOLT.FastSerializer.VOLTTYPE_STRING])
    response = proc.call(params=(runner.opts.directory, runner.opts.unique_id))
    print response
