{
  InaugurationFuture result=new InaugurationFuture();
  try {
    m_babySitter=new BabySitter(m_zk,LeaderElector.electionDirForPartition(m_partitionId),m_replicasChangeHandler,true);
    declareReadyAsLeader();
  }
 catch (  Exception e) {
    result.setException(e);
  }
  result.m_doneLatch.countDown();
  return result;
}
