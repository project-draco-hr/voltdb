{
  try {
    makeBabySitter();
    if (m_missingStartupSites.getCount() > 0) {
      prepareForStartup();
    }
 else {
      prepareForFaultRecovery();
    }
  }
 catch (  Exception e) {
    tmLog.error(m_whoami + "failed leader promotion:",e);
    m_promotionResult.setException(e);
    m_promotionResult.done();
  }
  return m_promotionResult;
}
