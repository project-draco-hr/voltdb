{
  final CountDownLatch onCompletion=new CountDownLatch(1);
  final Runnable r=new Runnable(){
    @Override public void run(){
      try {
        processRejoin(rejoiningSite);
      }
  finally {
        onCompletion.countDown();
      }
    }
  }
;
  m_mailbox.deliver(new LocalObjectMessage(r));
  onCompletion.await();
}
