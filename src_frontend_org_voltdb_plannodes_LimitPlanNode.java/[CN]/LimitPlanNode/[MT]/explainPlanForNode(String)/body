{
  String retval="";
  if (m_limit >= 0) {
    retval+="LIMIT " + String.valueOf(m_limit) + " ";
  }
  if (m_offset > 0) {
    retval+="OFFSET " + String.valueOf(m_offset) + " ";
  }
  if (retval.length() > 0) {
    return retval.substring(0,retval.length() - 1);
  }
 else {
    return "LIMIT with parameter";
  }
}
