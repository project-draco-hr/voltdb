def get_error_log():
    outfilename = os.path.join(HTTPListener.Global.PATH, get_most_recent_log())
    try:
        rfile = open(outfilename, 'r')
        read_it = rfile.read()
        myLine = ''
        for line in read_it.splitlines():
            if ('ERROR:' in line):
                myLine = line
                break
        return myLine
    except:
        return 'error'
    pass
