{
  File retval=null;
  InputStreamReader inp=new InputStreamReader(System.in);
  BufferedReader br=new BufferedReader(inp);
  while (retval == null) {
    System.out.println("Please choose a dump:\n");
    for (int i=0; i < dumps.size(); i++)     System.out.printf(" %d) %s\n",i + 1,dumps.get(i).getName());
    System.out.print("> ");
    String choice=null;
    try {
      choice=br.readLine();
    }
 catch (    IOException e) {
      e.printStackTrace();
      System.err.println("Unable to choose a dump.");
      System.exit(-1);
    }
    int numChoice=Integer.parseInt(choice.trim());
    if ((numChoice > 0) && (numChoice <= dumps.size()))     retval=dumps.get(numChoice - 1);
 else     System.out.println("Choice not recognized. Please try again.\n");
  }
  return retval;
}
