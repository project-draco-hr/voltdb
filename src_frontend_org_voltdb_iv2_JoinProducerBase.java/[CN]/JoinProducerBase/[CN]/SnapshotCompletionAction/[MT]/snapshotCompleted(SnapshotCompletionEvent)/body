{
  if (event.nonce.equals(m_snapshotNonce)) {
    JOINLOG.debug(m_whoami + "counting down snapshot monitor completion. " + "Snapshot txnId is: "+ event.multipartTxnId);
    m_completionAction.setSnapshotTxnId(event.multipartTxnId);
    deregister();
    kickWatchdog(true);
    m_completionMonitorAwait.set(event);
  }
 else {
    JOINLOG.debug(m_whoami + " observed completion of irrelevant snapshot nonce: " + event.nonce);
  }
  return null;
}
