{
  validateInputNotNull(value,context);
  if (!(value instanceof Date)) {
    throw new SuperCsvCellProcessorException(Date.class,value,context,this);
  }
  final SimpleDateFormat formatter;
  try {
    formatter=new SimpleDateFormat(dateFormat);
  }
 catch (  IllegalArgumentException e) {
    throw new SuperCsvCellProcessorException(String.format("'%s' is not a valid date format",dateFormat),context,this,e);
  }
  String result=formatter.format((Date)value);
  return next.execute(result,context);
}
