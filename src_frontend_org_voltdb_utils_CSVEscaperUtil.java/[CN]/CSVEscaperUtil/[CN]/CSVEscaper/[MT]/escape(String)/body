{
  if (!contains(s,m_csvEscapeChars)) {
    return s;
  }
  StringBuilder sb=new StringBuilder(s.length() + (int)(s.length() * .10));
  sb.append('"');
  for (int ii=0; ii < s.length(); ii++) {
    char c=s.charAt(ii);
    if (c == '"') {
      sb.append("\"\"");
    }
 else {
      sb.append(c);
    }
  }
  sb.append('"');
  return sb.toString();
}
