{
  try {
    final Queue<VoltMessage> dq=m_messages.get(s.getId());
    assert(dq != null);
synchronized (dq) {
      while (dq.isEmpty()) {
        dq.wait();
      }
      final VoltMessage message=dq.poll();
      assert(message != null);
      return message;
    }
  }
 catch (  InterruptedException e) {
  }
  return null;
}
