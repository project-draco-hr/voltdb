{
  final FileOutputStream fos=new FileOutputStream(outfile,true);
  try {
    final CSVTableSaveFile converter=new CSVTableSaveFile(infile,escaper,partitions);
    try {
      while (true) {
        final byte bytes[]=converter.read();
        if (bytes.length == 0) {
          break;
        }
        fos.write(bytes);
      }
    }
  finally {
      try {
        converter.close();
      }
  finally {
        fos.getFD().sync();
      }
    }
  }
  finally {
    fos.close();
  }
}
