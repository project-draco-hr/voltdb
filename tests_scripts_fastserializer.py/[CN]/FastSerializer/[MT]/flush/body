def flush(self):
    if (self.socket == None):
        print 'ERROR: not connected to server.'
        exit((-1))
    if (self.dump_file != None):
        self.dump_file.write(self.wbuf)
        self.dump_file.write('\n')
    self.socket.sendall(self.wbuf.tostring())
    self.wbuf = array.array('c')
