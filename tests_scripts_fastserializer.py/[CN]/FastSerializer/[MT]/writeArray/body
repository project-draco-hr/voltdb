def writeArray(self, type, array):
    if ((not array) or (len(array) == 0) or (not type)):
        return
    if (type not in self.WRITER):
        print 'ERROR: Unsupported date type (', type, ').'
        exit((-2))
    self.writeInt16(len(array))
    for i in array:
        self.WRITER[type](i)
