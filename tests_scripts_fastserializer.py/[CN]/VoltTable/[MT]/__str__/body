def __str__(self):
    result = ''
    result += ('column count: %d\n' % len(self.columns))
    result += ('row count: %d\n' % len(self.tuples))
    result += 'cols: '
    result += ', '.join(map((lambda x: str(x)), self.columns))
    result += '\n'
    result += 'rows -\n'
    result += '\n'.join(map((lambda x: str(map((lambda y: ('NULL' if (y == None) else y)), x))), self.tuples))
    return result
