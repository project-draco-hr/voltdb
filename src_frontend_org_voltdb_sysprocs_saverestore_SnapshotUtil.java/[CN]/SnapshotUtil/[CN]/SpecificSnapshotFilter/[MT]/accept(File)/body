{
  if (!super.accept(pathname)) {
    return false;
  }
  if (pathname.isDirectory()) {
    return true;
  }
  for (  String snapshotName : snapshotNames) {
    if (pathname.getName().startsWith(snapshotName + "-") || pathname.getName().equals(snapshotName + ".digest") || pathname.getName().equals(snapshotName + ".jar")) {
      return true;
    }
  }
  return false;
}
