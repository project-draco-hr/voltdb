{
  Thread.currentThread().setName("Iv2ExecutionSite: " + String.valueOf(m_siteId));
  try {
    while (m_shouldContinue) {
      runLoop();
    }
  }
 catch (  final RuntimeException e) {
    hostLog.l7dlog(Level.ERROR,LogKeys.host_ExecutionSite_RuntimeException.name(),e);
    throw e;
  }
  shutdown();
}
