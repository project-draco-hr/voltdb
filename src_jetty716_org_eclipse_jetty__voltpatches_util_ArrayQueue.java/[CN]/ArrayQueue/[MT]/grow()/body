{
  if (_growCapacity <= 0)   return false;
  Object[] elements=new Object[_elements.length + _growCapacity];
  int split=_elements.length - _nextE;
  if (split > 0)   System.arraycopy(_elements,_nextE,elements,0,split);
  if (_nextE != 0)   System.arraycopy(_elements,0,elements,split,_nextSlot);
  _elements=elements;
  _nextE=0;
  _nextSlot=_size;
  return true;
}
