{
synchronized (_lock) {
    if (index < 0 || index >= _size)     throw new IndexOutOfBoundsException("!(" + 0 + "<"+ index+ "<="+ _size+ ")");
    int i=_nextE + index;
    if (i >= _elements.length)     i-=_elements.length;
    E old=(E)_elements[i];
    _elements[i]=element;
    return old;
  }
}
