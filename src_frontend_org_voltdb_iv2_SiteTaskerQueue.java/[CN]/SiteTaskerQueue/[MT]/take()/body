{
  SiteTasker task=m_tasks.poll();
  if (task == null) {
    m_starvationTracker.beginStarvation();
  }
 else {
    return task;
  }
  try {
    task=m_tasks.poll(SITE_SPIN_MICROS,TimeUnit.MICROSECONDS);
    if (task == null) {
      task=m_tasks.take();
    }
    return task;
  }
  finally {
    m_starvationTracker.endStarvation();
  }
}
