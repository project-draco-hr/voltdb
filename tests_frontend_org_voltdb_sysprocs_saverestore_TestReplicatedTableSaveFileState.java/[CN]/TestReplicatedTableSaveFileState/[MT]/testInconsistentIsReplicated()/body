{
  addHostToTestData(0);
  addBadHostToTestData(1);
  m_siteInput.resetRowPosition();
  while (m_siteInput.advanceRow()) {
    try {
      m_state.addHostData(m_siteInput);
    }
 catch (    IOException e) {
      assertTrue(m_state.getConsistencyResult().contains("but has a savefile which indicates partitioning at site"));
      return;
    }
  }
  assertTrue(false);
}
