{
  super.dump(b,indent);
  Handler[] handlers=getHandlers();
  if (handlers != null) {
    int last=handlers.length - 1;
    for (int h=0; h <= last; h++) {
      if (handlers[h] == null)       continue;
      b.append(indent);
      b.append(" +-");
      if (handlers[h] instanceof AbstractHandler)       ((AbstractHandler)handlers[h]).dump(b,indent + ((h == last) ? "   " : " | "));
 else {
        b.append(handlers[h]);
        b.append("\n");
      }
    }
  }
}
