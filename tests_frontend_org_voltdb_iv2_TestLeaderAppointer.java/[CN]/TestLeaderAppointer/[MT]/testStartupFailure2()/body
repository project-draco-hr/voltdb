{
  configure(2,2,1,false);
  m_cache.put(0,0L);
  m_cache.put(1,1L);
  waitForAppointee(1);
  registerLeader(0,0L);
  VoltDB.ignoreCrash=true;
  boolean threw=false;
  try {
    m_dut.acceptPromotion();
  }
 catch (  AssertionError ae) {
    threw=true;
  }
  assertTrue(threw);
  assertTrue(VoltDB.wasCrashCalled);
}
