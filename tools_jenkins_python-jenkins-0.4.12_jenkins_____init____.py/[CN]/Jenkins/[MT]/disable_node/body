def disable_node(self, name, msg=''):
    'Disable a node\n\n        :param name: Jenkins node name, ``str``\n        :param msg: Offline message, ``str``\n        '
    info = self.get_node_info(name)
    if info['offline']:
        return
    self.jenkins_open(Request(self._build_url(TOGGLE_OFFLINE, locals()), ''))
