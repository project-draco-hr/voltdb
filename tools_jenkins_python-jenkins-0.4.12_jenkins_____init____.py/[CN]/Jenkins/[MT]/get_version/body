def get_version(self):
    "Get the version of this Master.\n\n        :returns: This master's version number ``str``\n\n        Example::\n\n            >>> info = server.get_version()\n            >>> print info\n            >>> 1.541\n\n        "
    try:
        request = Request(self._build_url(''))
        request.add_header('X-Jenkins', '0.0')
        response = urlopen(request, timeout=self.timeout)
        if (response is None):
            raise EmptyResponseException(('Error communicating with server[%s]: empty response' % self.server))
        if six.PY2:
            return response.info().getheader('X-Jenkins')
        if six.PY3:
            return response.getheader('X-Jenkins')
    except (HTTPError, BadStatusLine):
        raise BadHTTPException(('Error communicating with server[%s]' % self.server))
