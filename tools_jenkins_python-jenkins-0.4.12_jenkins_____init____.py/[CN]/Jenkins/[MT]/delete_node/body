def delete_node(self, name):
    'Delete Jenkins node permanently.\n\n        :param name: Name of Jenkins node, ``str``\n        '
    self.get_node_info(name)
    self.jenkins_open(Request(self._build_url(DELETE_NODE, locals()), ''))
    if self.node_exists(name):
        raise JenkinsException(('delete[%s] failed' % name))
