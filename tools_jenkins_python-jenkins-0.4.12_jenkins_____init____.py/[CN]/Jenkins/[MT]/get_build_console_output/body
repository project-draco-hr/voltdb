def get_build_console_output(self, name, number):
    'Get build console text.\n\n        :param name: Job name, ``str``\n        :param name: Build number, ``int``\n        :returns: Build console output,  ``str``\n        '
    (folder_url, short_name) = self._get_job_folder(name)
    try:
        response = self.jenkins_open(Request(self._build_url(BUILD_CONSOLE_OUTPUT, locals())))
        if response:
            return response
        else:
            raise JenkinsException(('job[%s] number[%d] does not exist' % (name, number)))
    except HTTPError:
        raise JenkinsException(('job[%s] number[%d] does not exist' % (name, number)))
