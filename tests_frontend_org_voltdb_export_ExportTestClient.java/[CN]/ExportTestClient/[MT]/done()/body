{
  boolean retval=true;
  for (  ExportTestVerifier verifier : m_verifiers.values()) {
    if (!verifier.done()) {
      retval=false;
    }
  }
  for (  ExportConnection connection : m_exportConnections.values()) {
    if (!connection.isConnected()) {
      retval=true;
    }
  }
  return retval;
}
