{
  m_clientPSM.killAll();
  Client client=ClientFactory.createClient();
  try {
    client.createConnection(m_config.hosts[0].getHostName(),m_config.hosts[0].getPort(),"","");
    NullCallback cb=new NullCallback();
    client.callProcedure(cb,"@Shutdown");
  }
 catch (  NoConnectionsException e) {
    e.printStackTrace();
  }
catch (  UnknownHostException e) {
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
  m_serverPSM.killAll();
}
