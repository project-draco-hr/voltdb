{
  String string=sequence.toString();
  int pos=indexIn(string);
  if (pos == -1) {
    return string;
  }
  char[] chars=string.toCharArray();
  int spread=1;
  OUT:   while (true) {
    pos++;
    while (true) {
      if (pos == chars.length) {
        break OUT;
      }
      if (matches(chars[pos])) {
        break;
      }
      chars[pos - spread]=chars[pos];
      pos++;
    }
    spread++;
  }
  return new String(chars,0,pos - spread);
}
