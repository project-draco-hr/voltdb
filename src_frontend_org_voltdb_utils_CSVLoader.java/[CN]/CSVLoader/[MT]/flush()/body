{
  inCount.set(0);
  outCount.set(0);
  callBackCount.set(0);
  callProcCount.set(0);
  errorInfo.clear();
  csvReader.close();
  csvClient.drain();
  csvClient.close();
  out_invaliderowfile.flush();
  out_logfile.flush();
  out_reportfile.flush();
  out_invaliderowfile.close();
  out_logfile.close();
  out_reportfile.close();
}
