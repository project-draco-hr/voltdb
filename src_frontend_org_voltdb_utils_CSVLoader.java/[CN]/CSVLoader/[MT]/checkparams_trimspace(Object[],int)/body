{
  if (slot.length != columnCnt) {
    return "Error: Incorrect number of columns. " + slot.length + " found, "+ columnCnt+ " expected.";
  }
  for (int i=0; i < slot.length; i++) {
    Object thisSlot=slot[i];
    if (thisSlot == null) {
      if (config.blank.equalsIgnoreCase("error"))       return "Error: blank item";
 else       if (config.blank.equalsIgnoreCase("empty"))       thisSlot=blankValues.get(typeList.get(i));
    }
 else {
      String str=thisSlot.toString();
      if (config.nowhitespace && (str.charAt(0) == ' ' || str.charAt(str.length() - 1) == ' ')) {
        return "Error: White Space Detected in nowhitespace mode.";
      }
 else       thisSlot=((String)thisSlot).trim();
      if (thisSlot.equals("NULL") || thisSlot.equals(VoltTable.CSV_NULL) || !config.strictquotes && thisSlot.equals(VoltTable.QUOTED_CSV_NULL))       thisSlot=null;
    }
  }
  return null;
}
