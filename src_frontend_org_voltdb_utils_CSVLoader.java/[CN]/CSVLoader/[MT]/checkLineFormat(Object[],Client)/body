{
  VoltTable[] results=null;
  int columnCnt=-1;
  try {
    results=client.callProcedure("@SystemCatalog","COLUMNS").getResults();
    columnCnt=results[0].getRowCount();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  if (linefragement.length != columnCnt)   return false;
 else   return true;
}
