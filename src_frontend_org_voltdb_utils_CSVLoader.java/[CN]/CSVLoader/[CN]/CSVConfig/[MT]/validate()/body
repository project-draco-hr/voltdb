{
  if (abortfailurecount <= 0)   exitWithMessageAndUsage("");
  if (procedurename.equals("") && tablename.equals(""))   exitWithMessageAndUsage("procedure name or a table name required");
  if (!procedurename.equals("") && !tablename.equals(""))   exitWithMessageAndUsage("Only a procedure name or a table name required, pass only one please");
  if (inputfile.equals(""))   standin=true;
}
