{
  int numInserts=100000;
  try {
    String randString;
    for (int i=0; i < numInserts; i++) {
      randString=RandomValues.getString(10);
      client.callProcedure(new GenericCallback(),"InsertAccount",i,randString,1000000L);
    }
  }
 catch (  UnknownHostException e) {
    e.printStackTrace();
    System.exit(-1);
  }
catch (  ConnectException e) {
    e.printStackTrace();
    System.exit(-1);
  }
catch (  IOException e) {
    e.printStackTrace();
    System.exit(-1);
  }
}
