{
  Mailbox mailbox=m_hostMessenger.getMailbox(destinationHSId);
  if (mailbox == null) {
    System.err.printf("Message (%s) sent to unknown site id: %s @ (%s) at " + m_hostMessenger.getHostId() + " from "+ CoreUtils.hsIdToString(message.m_sourceHSId)+ "\n",message.getClass().getSimpleName(),CoreUtils.hsIdToString(destinationHSId),m_socket.getRemoteSocketAddress().toString());
    if (m_hostMessenger.getHostId() != (int)destinationHSId) {
      assert(false);
    }
    return;
  }
  mailbox.deliver(message);
}
