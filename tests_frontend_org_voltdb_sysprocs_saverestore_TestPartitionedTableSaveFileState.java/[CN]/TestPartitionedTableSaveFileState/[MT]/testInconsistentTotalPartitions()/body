{
  addSiteToTestData(0,0,0,4);
  addSiteToTestData(1,0,1,5);
  m_siteInput.resetRowPosition();
  while (m_siteInput.advanceRow()) {
    try {
      m_state.addHostData(m_siteInput);
    }
 catch (    IOException e) {
      assertTrue(m_state.getConsistencyResult().contains("with an inconsistent number of total partitions"));
      return;
    }
  }
  assertTrue(false);
}
