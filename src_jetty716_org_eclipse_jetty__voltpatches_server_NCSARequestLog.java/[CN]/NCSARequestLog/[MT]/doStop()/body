{
  super.doStop();
  try {
    if (_writer != null)     _writer.flush();
  }
 catch (  IOException e) {
    Log.ignore(e);
  }
  if (_out != null && _closeOut)   try {
    _out.close();
  }
 catch (  IOException e) {
    Log.ignore(e);
  }
  _out=null;
  _fileOut=null;
  _closeOut=false;
  _logDateCache=null;
  _writer=null;
  _buffers=null;
  _copy=null;
}
