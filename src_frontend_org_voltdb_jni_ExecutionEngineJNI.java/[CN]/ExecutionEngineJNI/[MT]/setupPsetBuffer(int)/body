{
  if (psetBuffer != null) {
    psetBuffer.discard();
  }
  psetBuffer=DBBPool.allocateDirect(size);
  int errorCode=nativeSetBuffers(pointer,psetBuffer.b(),psetBuffer.b().capacity(),deserializer.buffer(),deserializer.buffer().capacity(),exceptionBuffer,exceptionBuffer.capacity());
  checkErrorCode(errorCode);
}
