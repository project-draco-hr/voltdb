{
  clearPsetAndEnsureCapacity(8 + task.length);
  byte retval[]=null;
  try {
    psetBuffer.putLong(taskType.taskId);
    psetBuffer.put(task);
    deserializer.clear();
    nativeExecuteTask(pointer);
    return (byte[])deserializer.readArray(byte.class);
  }
 catch (  IOException e) {
    Throwables.propagate(e);
  }
  return retval;
}
