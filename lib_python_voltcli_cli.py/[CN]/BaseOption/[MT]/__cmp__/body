def __cmp__(self, other):
    if self.args[0]:
        if other.args[0]:
            return cmp(self.args[0].lower(), other.args[0].lower())
        return 1
    if other.args[0]:
        return (-1)
    if self.args[1]:
        if other.args[1]:
            return cmp(self.args[1].lower(), other.args[1].lower())
        return 1
    if other.args[1]:
        return (-1)
    return 0
