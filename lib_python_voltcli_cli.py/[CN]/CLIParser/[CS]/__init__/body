def __init__(self, verbs, base_options, usage, description, version):
    '\n        Command line processor constructor.\n        '
    self.verb = None
    self.verbs = verbs
    self.verb_names = verbs.keys()
    self.base_options = base_options
    self.verb_names.sort()
    self.base_options.sort()
    optparse.OptionParser.__init__(self, description=description, usage=usage, version=version)
    self.add_base_options()
