{
  super(args);
  for (  String arg : args) {
    String[] parts=arg.split("=",2);
    if (parts.length == 1)     continue;
    if (parts[1].startsWith("${"))     continue;
    if (parts[0].equals("sf"))     m_scalefactor=Integer.parseInt(parts[1]);
 else     if (parts[0].equals("multipartition"))     m_multipartition=Integer.parseInt(parts[1]);
  }
}
