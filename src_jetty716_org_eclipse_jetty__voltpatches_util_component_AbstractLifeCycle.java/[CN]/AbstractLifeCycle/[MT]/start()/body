{
synchronized (_lock) {
    try {
      if (_state == __STARTED || _state == __STARTING)       return;
      setStarting();
      doStart();
      setStarted();
    }
 catch (    Exception e) {
      setFailed(e);
      throw e;
    }
catch (    Error e) {
      setFailed(e);
      throw e;
    }
  }
}
