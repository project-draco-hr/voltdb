{
  ParsedColInfo col=new ParsedColInfo();
  col.expression=(AbstractExpression)expression.clone();
  col.tableName=tableName;
  col.columnName=columnName;
  col.alias=alias;
  col.index=index;
  col.size=size;
  col.orderBy=orderBy;
  col.ascending=ascending;
  col.groupBy=groupBy;
  col.finalOutput=finalOutput;
  return col;
}
