{
  for (  ParsedColInfo groupByCol : colList) {
    AbstractExpression groupByExpr=groupByCol.expression;
    if (groupByExpr == null) {
      continue;
    }
    Collection<AbstractExpression> found=groupByExpr.findAllSubexpressionsOfClass(aeClass);
    if (found.isEmpty()) {
      continue;
    }
    exprs.addAll(found);
  }
}
