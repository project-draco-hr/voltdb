{
  for (  ParsedColInfo groupedCol : groupByColumns) {
    boolean missing=true;
    for (    ParsedColInfo displayCol : displayColumns) {
      if (displayCol.groupBy) {
        if (groupedCol.equals(displayCol)) {
          missing=false;
          break;
        }
      }
    }
    if (missing) {
      return false;
    }
  }
  return true;
}
