{
  if (guaranteesUniqueRow()) {
    return true;
  }
  ArrayList<AbstractExpression> nonOrdered=new ArrayList<AbstractExpression>();
  if (orderByColumnsDetermineUniqueColumns(nonOrdered)) {
    return true;
  }
  if (orderByColumnsDetermineAllDisplayColumns(nonOrdered)) {
    return true;
  }
  return false;
}
