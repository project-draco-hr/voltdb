{
  boolean caught=false;
  try {
    VoltTypeUtil.determineImplicitCasting(VoltType.DECIMAL,VoltType.FLOAT);
  }
 catch (  VoltTypeException e) {
    caught=true;
  }
  assertTrue("VoltType.DECIMAL and VoltType.FLOAT threw" + "exception",caught);
  assertEquals(VoltTypeUtil.determineImplicitCasting(VoltType.DECIMAL,VoltType.DECIMAL),VoltType.DECIMAL);
  assertEquals(VoltTypeUtil.determineImplicitCasting(VoltType.DECIMAL,VoltType.SMALLINT),VoltType.DECIMAL);
}
