{
  VoltTable vt=new VoltTable(new ColumnInfo(VoltSystemProcedure.CNAME_HOST_ID,VoltSystemProcedure.CTYPE_ID),new ColumnInfo("KEY",VoltType.STRING),new ColumnInfo("VALUE",VoltType.STRING));
  InetAddress addr=org.voltdb.client.ConnectionUtil.getLocalAddress();
  vt.addRow(VoltDB.instance().getHostMessenger().getHostId(),"IPADDRESS",addr.getHostAddress());
  vt.addRow(VoltDB.instance().getHostMessenger().getHostId(),"HOSTNAME",addr.getHostName());
  vt.addRow(VoltDB.instance().getHostMessenger().getHostId(),"BUILDSTRING",VoltDB.instance().getBuildString());
  vt.addRow(VoltDB.instance().getHostMessenger().getHostId(),"VERSION",VoltDB.instance().getVersionString());
  return vt;
}
