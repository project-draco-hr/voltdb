def toXML(self):
    'Outputs the XML representation of the Python object.\n\n        Return: The XML data.\n        '
    if (type(self.obj) is not dict):
        raise InvalidObject()
    if (not self.tree):
        self.tree = Document()
        self.__buildtree__()
    return self.tree.toprettyxml('  ', encoding='utf-8')
