{
  assertTrue(hsqlEmptyRS.absolute(-1) == voltEmptyRS.absolute(-1));
  assertTrue(hsqlEmptyRS.getRow() == voltEmptyRS.getRow());
  assertTrue(hsqlEmptyRS.absolute(0) == voltEmptyRS.absolute(0));
  assertTrue(hsqlEmptyRS.getRow() == voltEmptyRS.getRow());
  assertTrue(hsqlEmptyRS.absolute(1) == voltEmptyRS.absolute(1));
  assertTrue(hsqlEmptyRS.getRow() == voltEmptyRS.getRow());
}
