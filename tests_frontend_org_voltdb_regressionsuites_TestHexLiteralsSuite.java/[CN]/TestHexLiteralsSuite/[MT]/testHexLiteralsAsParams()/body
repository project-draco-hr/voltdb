{
  Client client=getClient();
  for (int i=0; i < interestingValues.length; ++i) {
    long val=interestingValues[i];
    client.callProcedure("T.Insert",i,longToHexLiteral(val));
    VoltTable vt=client.callProcedure("@AdHoc","select bi from t where pk = ?",i).getResults()[0];
    validateRowOfLongs(vt,new long[]{val});
  }
  verifyProcFails(client,"Unable to convert string x'' to long value for target parameter","T.Insert",21,"x''");
  verifyProcFails(client,"Unable to convert string x'FFFFffffFFFFffffF' to long value for target parameter","T.Insert",21,"x'FFFFffffFFFFffffF'");
}
