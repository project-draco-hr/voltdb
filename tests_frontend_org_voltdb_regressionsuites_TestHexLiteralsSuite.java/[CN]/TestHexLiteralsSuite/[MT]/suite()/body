{
  VoltServerConfig config=null;
  MultiConfigSuiteBuilder builder=new MultiConfigSuiteBuilder(TestHexLiteralsSuite.class);
  boolean success;
  VoltProjectBuilder project=new VoltProjectBuilder();
  String literalSchema="CREATE TABLE T (\n" + "  PK INTEGER NOT NULL PRIMARY KEY,\n" + "  BI BIGINT\n"+ ");\n";
  for (  long val : interestingValues) {
    literalSchema+="CREATE PROCEDURE HEX_LITERAL_PROC_" + makeEvenDigits(val) + " AS\n"+ "  SELECT HEX(BITXOR(X'"+ makeEvenDigits(val)+ "', ?)) FROM T;\n";
  }
  for (  long val : interestingValues) {
    literalSchema+="CREATE PROCEDURE HEX_LITERAL_PROC_WHERE_" + makeEvenDigits(val) + " AS\n"+ "  SELECT PK FROM T WHERE BI = X'"+ makeEvenDigits(val)+ "';\n";
  }
  try {
    project.addLiteralSchema(literalSchema);
  }
 catch (  Exception e) {
    fail();
  }
  config=new LocalCluster("fixedsql-threesite.jar",3,1,0,BackendTarget.NATIVE_EE_JNI);
  success=config.compile(project);
  assertTrue(success);
  builder.addServerConfig(config);
  return builder;
}
