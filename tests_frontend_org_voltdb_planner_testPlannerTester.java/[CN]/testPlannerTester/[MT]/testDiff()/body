{
  AbstractPlanNode pn1=null;
  AbstractPlanNode pn2=null;
  assertTrue(compileDiffDigPattern("select * from l order by b limit ?;",3,true,"select * from l order by a limit ?;",3,true,"same leaf size"));
  assertTrue(compileDiffDigPattern("select * from l, t where t.a=l.b order by b limit ?;",3,true,"select * from l, t where t.a=l.b order by a limit ?;",3,true,"Join query"));
  assertTrue(compileDiffDigPattern("select * from l, t where t.a=l.b order by b;",3,true,"select * from l, t where t.a=l.b order by a limit ?;",3,true,"Plan tree size diff: (5 => 6)","Inline Projection Nodes diff:","([5-SEQSCAN] => [6-SEQSCAN])","Inline IndexScan Nodes diff:","([4-NESTLOOPINDEX] => [5-NESTLOOPINDEX])","Limit Nodes diff:","([] => [2])"));
  assertTrue(compileDiffDigPattern("select * from l, t where t.a=l.b order by b limit ?;",3,true,"select * from l, t where t.a=l.b order by a limit ?;",3,true,"Join query"));
}
