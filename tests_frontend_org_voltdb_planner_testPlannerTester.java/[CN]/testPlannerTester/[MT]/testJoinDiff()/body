{
  assertTrue(compileDiffMatchPattern("select count(*) from t;",3,true,"select * from l,t where l.a = t.a order by b limit ?;",3,true,"Join Node List diff:","([] => NESTLOOPINDEX[5])"));
  assertTrue(compileDiffMatchPattern("select * from l, t where t.a=l.a;",3,true,"select * from l, t where t.b=l.b order by a limit ?;",3,true,"Join Node Type diff:","(NESTLOOPINDEX[3] => NESTLOOP[5])"));
}
