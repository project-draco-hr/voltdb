{
  try {
    m_procNameBytes=m_procName.getBytes("UTF-8");
  }
 catch (  Exception e) {
  }
  int timeoutSize=0;
  if (m_type.getValue() >= BatchTimeoutOverrideType.BATCH_TIMEOUT_VERSION) {
    timeoutSize=1 + (m_batchTimeout == BatchTimeoutOverrideType.NO_TIMEOUT ? 0 : 4);
  }
  int size=1 + (ProcedureInvocationType.isDeprecatedInternalDRType(m_type) ? 16 : 0) + timeoutSize+ m_procNameBytes.length+ 4+ 8+ m_parameters.getSerializedSize();
  return size;
}
