{
  if (loadNativeLib) {
    foundNativeSupport=org.voltdb.EELibraryLoader.loadExecutionEngineLibrary(false);
  }
 else {
    foundNativeSupport=false;
  }
  m_allocateOnHeap=allocateOnHeap;
  if (maxArenaSizes == null) {
    m_maxArenaSizes=new int[]{m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize,m_defaultMaxArenaSize};
  }
 else {
    m_maxArenaSizes=maxArenaSizes;
  }
  m_arenas=initDBBPool();
synchronized (m_pools) {
    m_pools.add(this);
  }
}
