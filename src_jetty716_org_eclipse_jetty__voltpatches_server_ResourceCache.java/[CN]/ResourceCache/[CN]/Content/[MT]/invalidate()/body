{
synchronized (this) {
    _cache.remove(_key);
    _key=null;
    if (_buffer != null)     _cachedSize=_cachedSize - _buffer.length();
    _cachedFiles--;
    if (_mostRecentlyUsed == this)     _mostRecentlyUsed=_next;
 else     _prev._next=_next;
    if (_leastRecentlyUsed == this)     _leastRecentlyUsed=_prev;
 else     _next._prev=_prev;
    _prev=null;
    _next=null;
    _resource.release();
  }
}
