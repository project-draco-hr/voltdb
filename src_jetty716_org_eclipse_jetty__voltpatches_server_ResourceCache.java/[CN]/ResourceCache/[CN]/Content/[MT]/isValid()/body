{
  if (_lastModified == _resource.lastModified()) {
    if (!_locked && _mostRecentlyUsed != this) {
      Content tp=_prev;
      Content tn=_next;
      _next=_mostRecentlyUsed;
      _mostRecentlyUsed=this;
      if (_next != null)       _next._prev=this;
      _prev=null;
      if (tp != null)       tp._next=tn;
      if (tn != null)       tn._prev=tp;
      if (_leastRecentlyUsed == this && tp != null)       _leastRecentlyUsed=tp;
    }
    return true;
  }
  invalidate();
  return false;
}
