{
  if (_cache != null) {
synchronized (this) {
      ArrayList<Content> values=new ArrayList<Content>(_cache.values());
      for (      Content content : values)       content.invalidate();
      _cache.clear();
      _cachedSize=0;
      _cachedFiles=0;
      _mostRecentlyUsed=null;
      _leastRecentlyUsed=null;
    }
  }
}
