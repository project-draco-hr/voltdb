{
synchronized (_cache) {
    while (_maxCachedFiles > 0 && _cachedFiles >= _maxCachedFiles && _leastRecentlyUsed != null)     _leastRecentlyUsed.invalidate();
    if (_maxCachedFiles > 0 && _cachedFiles >= _maxCachedFiles)     return;
    Miss miss=new Miss(resource);
    Content content2=(Content)_cache.get(pathInContext);
    if (content2 != null) {
      miss.release();
      return;
    }
    miss.cache(pathInContext);
  }
}
