{
  if (DEBUG) {
    System.out.println("In PostgreSQLBackend.isIntegerColumn, with columnName: '" + columnName + "'");
  }
  if (columnName == null) {
    return false;
  }
  String columnNameUpper=columnName.trim().toUpperCase();
  return columnNameUpper.equals("ID") || columnNameUpper.equals("NUM") || columnNameUpper.equals("TINY")|| columnNameUpper.equals("SMALL")|| columnNameUpper.equals("BIG")|| columnNameUpper.equals("POINTS")|| columnNameUpper.equals("WAGE")|| columnNameUpper.equals("DEPT")|| columnNameUpper.equals("AGE")|| columnNameUpper.equals("RENT")|| columnNameUpper.equals("A1")|| columnNameUpper.equals("A2")|| columnNameUpper.equals("A3")|| columnNameUpper.equals("A4")|| columnNameUpper.equals("V_G1")|| columnNameUpper.equals("V_CNT")|| columnNameUpper.equals("V_G2")|| columnNameUpper.equals("V_SUM_AGE")|| columnNameUpper.equals("V_SUM_RENT");
}
