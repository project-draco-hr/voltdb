{
  if (DEBUG) {
    System.out.println("In PostgreSQLBackend.isGeoColumn, with columnName: '" + columnName + "'");
  }
  if (columnName == null) {
    return false;
  }
  String columnNameUpper=columnName.trim().toUpperCase();
  return columnNameUpper.startsWith("PT") || columnNameUpper.startsWith("POLY");
}
