{
  if (m_eeProcess != null) {
    boolean done=false;
    while (!done) {
      try {
        m_eeProcess.waitFor();
        done=true;
      }
 catch (      InterruptedException e) {
        System.out.println("Interrupted waiting for EE IPC process to die. Wait again.");
      }
    }
  }
  if (m_stdoutParser != null) {
    m_stdoutParser.join();
  }
  if (m_stderrParser != null) {
    m_stderrParser.join();
  }
}
