{
  try {
    InputStream is=SQLCommand.class.getResourceAsStream(readme);
    while (is.available() > 0) {
      byte[] bytes=new byte[is.available()];
      is.read(bytes,0,bytes.length);
      prtStr.write(bytes);
    }
  }
 catch (  Exception x) {
    System.err.println(x.getMessage());
    System.exit(-1);
  }
}
