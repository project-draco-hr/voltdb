{
  if (RecallToken.matcher(line).matches() || ExitToken.matcher(line).matches() || GoToken.matcher(line).matches()|| ListProceduresToken.matcher(line).matches()|| ListTablesToken.matcher(line).matches()) {
    return;
  }
  Matcher m=FileToken.matcher(line);
  if (m.find()) {
    line=readScriptFileRecursive(m.group(1));
  }
  query.append(line);
  query.append("\n");
}
