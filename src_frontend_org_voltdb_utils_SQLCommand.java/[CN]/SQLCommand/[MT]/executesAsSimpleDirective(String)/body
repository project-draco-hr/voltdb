{
  if (ListProceduresToken.matcher(line).matches()) {
    execListProcedures();
    return true;
  }
  if (ListTablesToken.matcher(line).matches()) {
    execListTables();
    return true;
  }
  if (ListClassesToken.matcher(line).matches()) {
    execListClasses();
    return true;
  }
  if (HelpToken.matcher(line).matches()) {
    printHelp(System.out);
    return true;
  }
  return false;
}
