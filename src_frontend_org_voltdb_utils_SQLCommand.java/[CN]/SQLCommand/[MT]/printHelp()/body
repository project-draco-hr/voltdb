{
  try {
    byte[] bytes=new byte[1024 * 4];
    InputStream is=SQLCommand.class.getResourceAsStream("SQLCommandReadme.txt");
    while (is.available() > 0) {
      is.read(bytes,0,bytes.length);
      System.out.write(bytes);
    }
  }
 catch (  Exception x) {
    System.err.println(x.getMessage());
    System.exit(-1);
  }
}
