{
  VoltTable partition_table=new VoltTable(new ColumnInfo("PT_ID",VoltType.INTEGER),new ColumnInfo("PT_NAME",VoltType.STRING),new ColumnInfo("PT_INTVAL",VoltType.INTEGER),new ColumnInfo("PT_FLOATVAL",VoltType.FLOAT));
  for (int i=indexBase; i < numberOfItems + indexBase; i++) {
    Object[] row=new Object[]{i,"name_" + i,i,new Double(i)};
    partition_table.addRow(row);
  }
  return partition_table;
}
