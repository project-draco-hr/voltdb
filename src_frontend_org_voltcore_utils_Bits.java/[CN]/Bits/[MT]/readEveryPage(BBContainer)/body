{
  long address=cont.address();
  final int offset=(int)(address % Bits.pageSize());
  address-=offset;
  final int numPages=Bits.numPages(cont.b().remaining() + offset);
  byte checksum=0;
  for (int ii=0; ii < numPages; ii++) {
    checksum^=Bits.unsafe.getByte(address);
    address|=checksum;
  }
  if (unused != 0) {
    unused=checksum;
  }
}
