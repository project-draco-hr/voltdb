def make_delete_branches_script(branch_infos, dry_run):
    other_args = ''
    if dry_run:
        other_args = ' --dry-run'
    for bi in branch_infos:
        b = bi['name']
        cmd = ('git push origin --delete %s%s' % (b, other_args))
        comment = make_comment(bi)
        print 
        print comment
        print cmd
