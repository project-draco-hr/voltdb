{
  StringBuilder sb=new StringBuilder();
  sb.append("UPDATE " + table.getTypeName() + " SET ");
  generateCrudExpressionColumns(table,sb);
  generateCrudPKeyWhereClause(partitioncolumn,pkey,sb);
  String partitioninfo=table.getTypeName() + "." + partitioncolumn.getName()+ ":"+ partitioncolumn.getIndex();
  ProcedureDescriptor pd=new ProcedureDescriptor(new ArrayList<String>(),table.getTypeName() + ".update",sb.toString(),null,partitioninfo,true,null,null,null);
  return pd;
}
