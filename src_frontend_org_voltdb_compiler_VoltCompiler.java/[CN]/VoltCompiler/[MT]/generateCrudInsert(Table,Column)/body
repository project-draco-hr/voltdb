{
  StringBuilder sb=new StringBuilder();
  sb.append("INSERT INTO " + table.getTypeName() + " VALUES ");
  generateCrudColumnList(table,sb);
  sb.append(";");
  String partitioninfo=table.getTypeName() + "." + partitioncolumn.getName()+ ":"+ partitioncolumn.getIndex();
  ProcedureDescriptor pd=new ProcedureDescriptor(new ArrayList<String>(),table.getTypeName() + ".insert",sb.toString(),null,partitioninfo,true);
  compilerLog.info("Synthesized built-in INSERT procedure: " + sb.toString() + " for "+ table.getTypeName()+ " with partitioning: "+ partitioninfo);
  return pd;
}
