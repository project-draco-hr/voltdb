{
  StringBuilder sb=new StringBuilder();
  sb.append("UPSERT INTO " + table.getTypeName() + " VALUES ");
  generateCrudColumnList(table,sb);
  sb.append(";");
  String partitioninfo=table.getTypeName() + "." + partitioncolumn.getName()+ ":"+ partitioncolumn.getIndex();
  ProcedureDescriptor pd=new ProcedureDescriptor(new ArrayList<String>(),table.getTypeName() + ".upsert",sb.toString(),null,partitioninfo,true,null,null,null);
  return pd;
}
