{
  StringBuilder sb=new StringBuilder();
  sb.append("DELETE FROM " + table.getTypeName());
  int partitionoffset=generateCrudPKeyWhereClause(partitioncolumn,pkey,-1,sb);
  String partitioninfo=table.getTypeName() + "." + partitioncolumn.getName()+ ":"+ partitionoffset;
  ProcedureDescriptor pd=new ProcedureDescriptor(new ArrayList<String>(),table.getTypeName() + ".delete",sb.toString(),null,partitioninfo);
  compilerLog.debug("Synthesized built-in DELETE procedure: " + sb.toString() + " for "+ table.getTypeName()+ " with partitioning: "+ partitioninfo);
  return pd;
}
