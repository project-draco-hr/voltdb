{
  StringBuilder sb=new StringBuilder();
  sb.append("SELECT * FROM " + table.getTypeName());
  int partitionOffset=generateCrudPKeyWhereClause(partitioncolumn,pkey,sb);
  String partitioninfo=table.getTypeName() + "." + partitioncolumn.getName()+ ":"+ partitionOffset;
  ProcedureDescriptor pd=new ProcedureDescriptor(new ArrayList<String>(),table.getTypeName() + ".select",sb.toString(),null,partitioninfo,true,null,null);
  return pd;
}
