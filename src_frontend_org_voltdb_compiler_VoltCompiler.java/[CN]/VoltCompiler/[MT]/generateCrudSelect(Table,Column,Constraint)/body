{
  StringBuilder sb=new StringBuilder();
  sb.append("SELECT * FROM " + table.getTypeName());
  int partitionoffset=generateCrudPKeyWhereClause(partitioncolumn,pkey,-1,sb);
  String partitioninfo=table.getTypeName() + "." + partitioncolumn.getName()+ ":"+ partitionoffset;
  ProcedureDescriptor pd=new ProcedureDescriptor(new ArrayList<String>(),table.getTypeName() + ".select",sb.toString(),null,partitioninfo,true);
  compilerLog.info("Synthesized built-in SELECT procedure: " + sb.toString() + " for "+ table.getTypeName()+ " with partitioning: "+ partitioninfo);
  return pd;
}
