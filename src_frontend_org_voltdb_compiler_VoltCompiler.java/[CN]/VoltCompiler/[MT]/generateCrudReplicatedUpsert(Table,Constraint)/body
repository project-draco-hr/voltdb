{
  StringBuilder sb=new StringBuilder();
  sb.append("UPSERT INTO " + table.getTypeName() + " VALUES ");
  generateCrudColumnList(table,sb);
  sb.append(";");
  ProcedureDescriptor pd=new ProcedureDescriptor(new ArrayList<String>(),table.getTypeName() + ".upsert",sb.toString(),null,null,true,null,null,null);
  return pd;
}
