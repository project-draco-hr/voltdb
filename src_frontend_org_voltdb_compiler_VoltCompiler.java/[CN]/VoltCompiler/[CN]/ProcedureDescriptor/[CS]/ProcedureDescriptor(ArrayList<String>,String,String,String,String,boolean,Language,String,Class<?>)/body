{
  assert(className != null);
  assert(singleStmt != null);
  m_authGroups=authGroups;
  m_className=className;
  m_singleStmt=singleStmt;
  m_joinOrder=joinOrder;
  m_partitionString=partitionString;
  m_builtInStmt=builtInStmt;
