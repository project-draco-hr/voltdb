{
  String retval="";
  if (severityLevel == Severity.INFORMATIONAL)   retval="INFO";
  if (severityLevel == Severity.WARNING)   retval="WARNING";
  if (severityLevel == Severity.ERROR)   retval="ERROR";
  if (severityLevel == Severity.UNEXPECTED)   retval="UNEXPECTED ERROR";
  if (fileName != null) {
    retval+=" [" + fileName;
    if (lineNo != NO_LINE_NUMBER)     retval+=":" + lineNo;
    retval+="]";
  }
  retval+=": " + message;
  return retval;
}
