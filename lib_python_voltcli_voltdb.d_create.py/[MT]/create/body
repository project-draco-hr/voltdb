@VOLT.Command(bundles=VOLT.ServerBundle('create', needs_catalog=True, supports_live=False, default_host=True, safemode_available=False, supports_daemon=True, supports_multiple_daemons=True, check_environment_config=True, new_voltdb_instance=True), options=(VOLT.BooleanOption('-r', '--replica', 'replica', 'start replica cluster', default=False),), description='Start a new, empty database.')
def create(runner):
    runner.go()
