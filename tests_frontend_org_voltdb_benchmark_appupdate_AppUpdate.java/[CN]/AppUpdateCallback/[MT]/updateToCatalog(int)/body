{
  Transaction nextCatalog;
switch (catalog) {
case 0:
    nextCatalog=Transaction.CatalogAB;
  break;
case 1:
nextCatalog=Transaction.CatalogA;
break;
case 2:
nextCatalog=Transaction.CatalogB;
break;
default :
throw new UnsupportedOperationException("Invalid catalog update requested");
}
System.err.println("Updating to catalog name: " + nextCatalog.name + " using deployment file: "+ m_deploymentFilePath);
return m_voltClient.callProcedure(new AppUpdateCallback(nextCatalog),"@UpdateApplicationCatalog",nextCatalog.name,m_deploymentFilePath);
}
