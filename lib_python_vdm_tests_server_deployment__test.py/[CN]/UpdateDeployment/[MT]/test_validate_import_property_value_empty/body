def test_validate_import_property_value_empty(self):
    'ensure property value is not empty'
    response = requests.put(__url__, json={'import': {'configuration': [{'enabled': False, 'type': 'KAFKA', 'format': 'test', 'property': [{'name': 'test', }], }], }, })
    value = response.json()
    self.assertEqual(value['success'], False)
    self.assertEqual(response.status_code, 200)
