def test_validate_import_invalid_export_type(self):
    'ensure invalid type is not saved in export'
    response = requests.get(__db_url__)
    value = response.json()
    if value:
        db_length = len(value['databases'])
        last_db_id = value['databases'][(db_length - 1)]['id']
        dep_url = ((__db_url__ + str(last_db_id)) + '/deployment/')
        headers = {'Content-Type': 'application/json; charset=UTF-8', }
        response = requests.put(dep_url, json={'import': {'configuration': [{'format': '', 'enabled': False, 'module': 'test', 'type': 'test', 'property': [{'name': 'test', 'value': 'test', }], }], }, }, headers=headers)
        value = response.json()
        self.assertEqual(value['success'], False)
        self.assertEqual(response.status_code, 200)
