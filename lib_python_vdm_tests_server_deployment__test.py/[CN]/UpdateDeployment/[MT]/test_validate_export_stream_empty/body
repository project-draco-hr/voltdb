def test_validate_export_stream_empty(self):
    'ensure export stream is not empty'
    response = requests.put(__url__, json={'export': {'configuration': [{'exportconnectorclass': '', 'enabled': False, 'type': 'KAFKA', 'property': [{'name': 'test', 'value': 'test', }], }], }, })
    value = response.json()
    self.assertEqual(value['success'], False)
    self.assertEqual(response.status_code, 200)
