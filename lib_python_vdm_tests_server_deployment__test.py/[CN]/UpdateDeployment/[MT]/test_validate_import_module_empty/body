def test_validate_import_module_empty(self):
    'ensure export stream is not empty'
    response = requests.put(__url__, json={'import': {'configuration': [{'format': '', 'enabled': False, 'type': 'KAFKA', 'property': [{'name': 'test', 'value': 'test', }], }], }, })
    value = response.json()
    self.assertEqual(value['success'], False)
    self.assertEqual(response.status_code, 200)
