def test_validate_export_invalid_export_type(self):
    'ensure invalid type is not saved in export'
    response = requests.put(__url__, json={'export': {'configuration': [{'exportconnectorclass': '', 'enabled': False, 'stream': 'test', 'type': 'test', 'property': [{'name': 'test', 'value': 'test', }], }], }, })
    value = response.json()
    self.assertEqual(value['success'], False)
    self.assertEqual(response.status_code, 200)
