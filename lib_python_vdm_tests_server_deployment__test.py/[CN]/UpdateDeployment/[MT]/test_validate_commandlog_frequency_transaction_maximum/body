def test_validate_commandlog_frequency_transaction_maximum(self):
    'ensure commanlog frequency transaction is not greater than 2147483647'
    maximum_value = 2147483648
    response = requests.put(__url__, json={'cluster': {'kfactor': 2, 'sitesperhost': 1, }, 'commandlog': {'frequency': {'transactions': maximum_value, }, }, })
    value = response.json()
    self.assertEqual(value['errors'][0], (str(maximum_value) + ' is greater than the maximum of 2147483647'))
    self.assertEqual(response.status_code, 200)
