{
  super.populateColumnSchema(columns);
  columns.add(new ColumnInfo(Columns.CLUSTER_ID,VoltType.INTEGER));
  columns.add(new ColumnInfo(VoltSystemProcedure.CNAME_PARTITION_ID,VoltType.INTEGER));
