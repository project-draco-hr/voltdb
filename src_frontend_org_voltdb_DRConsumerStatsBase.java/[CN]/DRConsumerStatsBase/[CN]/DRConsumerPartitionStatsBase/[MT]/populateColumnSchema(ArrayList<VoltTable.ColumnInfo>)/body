  columns.add(new ColumnInfo(Columns.LAST_RECEIVED_TIMESTAMP,VoltType.TIMESTAMP));
  columns.add(new ColumnInfo(Columns.LAST_APPLIED_TIMESTAMP,VoltType.TIMESTAMP));
}
