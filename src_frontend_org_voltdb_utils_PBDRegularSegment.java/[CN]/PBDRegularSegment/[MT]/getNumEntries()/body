{
  if (m_closed) {
    open(false);
  }
  if (m_fc.size() > 0) {
    m_tmpHeaderBuf.b().clear();
    PBDUtils.readBufferFully(m_fc,m_tmpHeaderBuf.b(),COUNT_OFFSET);
    m_numOfEntries=m_tmpHeaderBuf.b().getInt();
    m_size=m_tmpHeaderBuf.b().getInt();
    return m_numOfEntries;
  }
 else {
    m_numOfEntries=0;
    m_size=0;
    return 0;
  }
}
