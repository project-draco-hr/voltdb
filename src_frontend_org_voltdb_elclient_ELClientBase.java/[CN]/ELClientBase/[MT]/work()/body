{
  for (  ELConnection el_connection : m_elConnections.values()) {
    el_connection.work();
  }
  for (  HashMap<Integer,ELDataSink> part_map : m_sinks.values()) {
    for (    ELDataSink work_sink : part_map.values()) {
      work_sink.work();
    }
  }
  for (  ELConnection el_connection : m_elConnections.values()) {
    el_connection.work();
  }
}
