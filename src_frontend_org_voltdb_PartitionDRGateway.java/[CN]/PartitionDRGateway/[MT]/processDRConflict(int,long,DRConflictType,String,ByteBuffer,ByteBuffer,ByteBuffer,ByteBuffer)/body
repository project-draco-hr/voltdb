{
  BBContainer cont=DBBPool.wrapBB(existingTable);
  DBBPool.registerUnsafeMemory(cont.address());
  cont.discard();
  cont=DBBPool.wrapBB(expectedTable);
  DBBPool.registerUnsafeMemory(cont.address());
  cont.discard();
  cont=DBBPool.wrapBB(newTable);
  DBBPool.registerUnsafeMemory(cont.address());
  cont.discard();
  return 0;
}
