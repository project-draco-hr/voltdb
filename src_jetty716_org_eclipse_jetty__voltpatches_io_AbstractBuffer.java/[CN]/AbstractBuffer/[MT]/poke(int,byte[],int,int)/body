{
  _hash=0;
  if (index + length > capacity()) {
    length=capacity() - index;
  }
  byte[] dst_array=array();
  if (dst_array != null)   System.arraycopy(b,offset,dst_array,index,length);
 else {
    int s=offset;
    for (int i=0; i < length; i++)     poke(index++,b[s++]);
  }
  return length;
}
