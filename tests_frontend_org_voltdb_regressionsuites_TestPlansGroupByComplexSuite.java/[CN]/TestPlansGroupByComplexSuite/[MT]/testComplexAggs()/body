    cr=client.callProcedure(tb,2,20,1,1.1);
    cr=client.callProcedure(tb,3,30,1,1.1);
    cr=client.callProcedure(tb,4,40,2,1.1);
    cr=client.callProcedure(tb,5,50,2,1.2);
  }
  String query="";
  String[] tbs={"R1","P1"};
  for (  String tb : tbs) {
  }
  cr=client.callProcedure("@AdHoc","SELECT dept, SUM(wage) as tag, count(wage)+1 from P1 GROUP BY dept");
  assertEquals(ClientResponse.SUCCESS,cr.getStatus());
  vt=cr.getResults()[0];
  expected=new long[][]{{2,90,3},{1,60,4}};
