{
  VoltServerConfig config=null;
  MultiConfigSuiteBuilder builder=new MultiConfigSuiteBuilder(TestPlansGroupByComplexSuite.class);
  VoltProjectBuilder project=new VoltProjectBuilder();
  final String literalSchema="CREATE TABLE R1 ( " + "ID INTEGER DEFAULT '0' NOT NULL, " + "WAGE INTEGER, "+ "DEPT INTEGER, "+ "TM TIMESTAMP DEFAULT NULL, "+ "PRIMARY KEY (ID) );"+ "CREATE TABLE P1 ( "+ "ID INTEGER DEFAULT '0' NOT NULL, "+ "WAGE INTEGER NOT NULL, "+ "DEPT INTEGER NOT NULL, "+ "TM TIMESTAMP DEFAULT NULL, "+ "PRIMARY KEY (ID) );"+ "CREATE TABLE P2 ( "+ "ID INTEGER DEFAULT '0' NOT NULL, "+ "WAGE INTEGER NOT NULL, "+ "DEPT INTEGER NOT NULL, "+ "TM TIMESTAMP DEFAULT NULL, "+ "PRIMARY KEY (ID) );"+ "CREATE TABLE P3 ( "+ "ID INTEGER DEFAULT '0' NOT NULL, "+ "WAGE INTEGER NOT NULL, "+ "DEPT INTEGER NOT NULL, "+ "TM TIMESTAMP DEFAULT NULL, "+ "PRIMARY KEY (ID) );";
  try {
    project.addLiteralSchema(literalSchema);
  }
 catch (  IOException e) {
    assertFalse(true);
  }
  project.addPartitionInfo("P1","ID");
  project.addPartitionInfo("P2","DEPT");
  project.addPartitionInfo("P3","WAGE");
  boolean success;
  config=new LocalCluster("plansgroupby-cluster.jar",2,3,1,BackendTarget.NATIVE_EE_JNI);
  success=config.compile(project);
  assertTrue(success);
  builder.addServerConfig(config);
  return builder;
}
