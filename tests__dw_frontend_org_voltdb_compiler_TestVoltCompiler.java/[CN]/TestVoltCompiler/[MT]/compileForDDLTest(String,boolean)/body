{
  final String simpleProject="<?xml version=\"1.0\"?>\n" + "<project>" + "<database name='database'>"+ "<schemas><schema path='" + schemaPath + "' /></schemas>"+ "<procedures><procedure class='sample'><sql>select * from t</sql></procedure></procedures>"+ "</database>"+ "</project>";
  final File projectFile=VoltProjectBuilder.writeStringToTempFile(simpleProject);
  projectFile.deleteOnExit();
  final String projectPath=projectFile.getPath();
  final VoltCompiler compiler=new VoltCompiler();
  final boolean success=compiler.compileWithProjectXML(projectPath,testout_jar);
  assertEquals(expectSuccess,success);
  return compiler;
}
