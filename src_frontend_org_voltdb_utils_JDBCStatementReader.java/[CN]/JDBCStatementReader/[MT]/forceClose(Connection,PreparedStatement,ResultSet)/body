{
  if (rslt != null)   try {
    rslt.close();
  }
 catch (  Exception ignoreIt) {
  }
  if (stmt != null)   try {
    stmt.close();
  }
 catch (  Exception ignoreIt) {
  }
  if (conn != null)   try {
    conn.close();
  }
 catch (  Exception ignoreIt) {
  }
  try {
    m_loader.close();
  }
 catch (  Exception ignoreIt) {
  }
}
