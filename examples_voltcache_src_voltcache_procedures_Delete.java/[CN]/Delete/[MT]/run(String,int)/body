{
  final int now=baseInit(key);
  if (expires <= 0) {
    voltQueueSQL(delete,key);
    if (voltExecuteSQL()[1].fetchRow(0).getLong(0) == 0)     return Result.NOT_FOUND;
    return Result.DELETED;
  }
 else {
    voltQueueSQL(check,key,now);
    if (voltExecuteSQL()[1].getRowCount() == 0)     return Result.NOT_FOUND;
    voltQueueSQL(update,expirationTimestamp(expires),key);
    voltExecuteSQL(true);
    return Result.DELETED;
  }
}
