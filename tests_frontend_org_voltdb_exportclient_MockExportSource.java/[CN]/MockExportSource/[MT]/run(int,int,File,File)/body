{
  try {
    ServerSocket listener=new ServerSocket(VoltDB.DEFAULT_PORT);
    while (true) {
      Socket sock=listener.accept();
      DataGenerator generator;
      if (csv != null)       generator=new DataGenerator(delay,blockSize,csv,schema);
 else       generator=new DataGenerator(delay,blockSize);
      acceptThread(sock,generator);
    }
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
