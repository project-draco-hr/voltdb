@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_in_folder(self, jenkins_mock):
    jenkins_mock.side_effect = [jenkins.NotFoundException(), None, json.dumps({'name': 'Test Job', })]
    self.j.create_job(u'a Folder/Test Job', self.config_xml)
    self.assertEqual(jenkins_mock.call_args_list[1][0][0].get_full_url(), self.make_url('job/a%20Folder/createItem?name=Test%20Job'))
    self._check_requests(jenkins_mock.call_args_list)
