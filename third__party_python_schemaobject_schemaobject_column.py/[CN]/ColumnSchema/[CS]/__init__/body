def __init__(self, name, parent):
    self.parent = parent
    self.name = name
    self.field = name
    self.ordinal_position = 0
    self.type = None
    self.charset = None
    self.collation = None
    self.null = None
    self.key = None
    self.default = None
    self.extra = None
    self.comment = None
