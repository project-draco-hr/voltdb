{
  try {
    client.callProcedure("NonDeterministicSPProc",0,0,NonDeterministicSPProc.MISMATCH_LENGTH);
    fail("R/W length mismatch didn't fail?!");
  }
 catch (  ProcCallException e) {
    assertTrue(e.getMessage().contains("Connection to database") || e.getMessage().contains("Transaction dropped"));
  }
}
