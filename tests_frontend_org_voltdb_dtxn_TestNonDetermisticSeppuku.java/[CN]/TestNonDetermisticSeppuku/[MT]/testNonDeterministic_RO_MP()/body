{
  try {
    client.callProcedure("NonDeterministicSPProc",0,0,NonDeterministicSPProc.MISMATCH_INSERTION);
    fail("Mismatch inserstion failed");
  }
 catch (  ProcCallException e) {
    assertTrue(e.getMessage().contains("Connection to database") || e.getMessage().contains("Transaction dropped"));
  }
}
