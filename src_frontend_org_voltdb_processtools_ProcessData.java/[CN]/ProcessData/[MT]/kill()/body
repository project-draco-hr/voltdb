{
  m_out.m_expectDeath.set(true);
  m_err.m_expectDeath.set(true);
  int retval=-255;
synchronized (m_process) {
    m_process.destroy();
    try {
      m_process.waitFor();
      retval=m_process.exitValue();
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
  return retval;
}
