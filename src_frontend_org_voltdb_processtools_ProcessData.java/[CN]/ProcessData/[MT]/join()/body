{
  m_out.m_expectDeath.set(true);
  m_err.m_expectDeath.set(true);
  try {
synchronized (m_process) {
      int waitFor=m_process.waitFor();
      System.err.println("Joined pd.process with exit status: " + waitFor);
    }
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
  return kill();
}
