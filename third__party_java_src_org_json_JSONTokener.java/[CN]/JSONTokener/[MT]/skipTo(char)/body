{
  char c;
  try {
    int startIndex=this.index;
    reader.mark(Integer.MAX_VALUE);
    do {
      c=next();
      if (c == 0) {
        reader.reset();
        this.index=startIndex;
        return c;
      }
    }
 while (c != to);
  }
 catch (  IOException exc) {
    throw new JSONException(exc);
  }
  back();
  return c;
}
