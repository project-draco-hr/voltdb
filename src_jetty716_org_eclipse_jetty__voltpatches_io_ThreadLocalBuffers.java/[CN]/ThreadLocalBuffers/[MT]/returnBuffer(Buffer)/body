{
  buffer.clear();
  if (buffer.isVolatile() || buffer.isImmutable())   return;
  int size=buffer.capacity();
  ThreadBuffers buffers=_buffers.get();
  if (buffers._header == null && size == _headerSize && isHeader(buffer))   buffers._header=buffer;
 else   if (size == _bufferSize && buffers._buffer == null)   buffers._buffer=buffer;
 else   buffers._other=buffer;
}
