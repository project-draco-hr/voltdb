{
  ByteBuffer buf=ByteBuffer.allocate(12);
  buf.putLong(m_timestamp);
  buf.putInt(m_coord);
  buf.flip();
  MessageDigest md;
  try {
    md=MessageDigest.getInstance("MD5");
  }
 catch (  NoSuchAlgorithmException e) {
    e.printStackTrace();
    return 0;
  }
  md.update(buf);
  byte[] digest=md.digest();
  return ByteBuffer.wrap(digest).getLong();
}
