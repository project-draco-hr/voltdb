def initialize_mysql_imports():
    '\n    Perform the delayed MySQL module imports.\n    '
    try:
        import schemaobject as schemaobject_
        import MySQLdb as MySQLdb_
        global schemaobject, MySQLdb
        schemaobject = schemaobject_
        MySQLdb = MySQLdb_
    except ImportError as e:
        msgs = ['Failed to import MySQL-related module.', 'Is MySQL properly installed?', ('http://dev.mysql.com/downloads/',)]
        if (sys.platform == 'darwin'):
            msgs.extend(['On OS X you may need to create a symlink, e.g.', (Global.osx_fix,)])
        msgs.append(e)
        utility.abort(*msgs)
