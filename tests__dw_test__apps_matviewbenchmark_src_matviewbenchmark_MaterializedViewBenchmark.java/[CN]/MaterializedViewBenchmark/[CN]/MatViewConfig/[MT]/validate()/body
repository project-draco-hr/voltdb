{
  if (displayinterval <= 0)   exitWithMessageAndUsage("displayinterval must be > 0");
  if (txn < 0)   exitWithMessageAndUsage("txn must be 0 or a positive integer");
  if (warmup < 0)   exitWithMessageAndUsage("warmup must be 0 or a positive integer");
  if (group < 0)   exitWithMessageAndUsage("group must be 0 or a positive integer");
}
