{
  try {
    m_sbq.close();
  }
  finally {
    try {
      File testDir=new File(TEST_DIR);
      if (testDir.exists()) {
        for (        File f : testDir.listFiles()) {
          f.delete();
        }
        testDir.delete();
      }
    }
  finally {
      m_sbq=null;
    }
  }
  System.gc();
  System.gc();
  System.gc();
  System.runFinalization();
}
