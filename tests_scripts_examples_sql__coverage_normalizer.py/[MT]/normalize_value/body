def normalize_value(v, type):
    global __NULL
    if ((type in __NULL) and (v == __NULL[type])):
        return None
    elif (type == VOLTTYPE_FLOAT):
        return round(v, 12)
    elif (type == VOLTTYPE_DECIMAL):
        return decimal.Decimal(v)._rescale((-12), 'ROUND_HALF_EVEN')
    else:
        return v
