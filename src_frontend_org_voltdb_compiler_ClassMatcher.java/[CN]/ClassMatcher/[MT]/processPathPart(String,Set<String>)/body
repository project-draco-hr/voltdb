{
  File rootFile=new File(path);
  if (rootFile.isDirectory() == false) {
    return;
  }
  File[] files=rootFile.listFiles();
  for (  File f : files) {
    if (f.getName().endsWith(".class")) {
      classes.add(f.getName());
    }
    if (f.isDirectory()) {
      Package p=new Package(null,f);
      p.process(classes);
    }
  }
}
