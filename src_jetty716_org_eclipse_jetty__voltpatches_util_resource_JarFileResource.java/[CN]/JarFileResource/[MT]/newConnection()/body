{
  super.newConnection();
  _entry=null;
  _file=null;
  _jarFile=null;
  _list=null;
  int sep=_urlString.indexOf("!/");
  _jarUrl=_urlString.substring(0,sep + 2);
  _path=_urlString.substring(sep + 2);
  if (_path.length() == 0)   _path=null;
  _jarFile=_jarConnection.getJarFile();
  _file=new File(_jarFile.getName());
}
