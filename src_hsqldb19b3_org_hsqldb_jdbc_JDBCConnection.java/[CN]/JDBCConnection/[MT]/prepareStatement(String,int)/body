{
  checkClosed();
  try {
    if (autoGeneratedKeys != ResultConstants.RETURN_GENERATED_KEYS && autoGeneratedKeys != ResultConstants.RETURN_NO_GENERATED_KEYS) {
      throw Util.invalidArgument("autoGeneratedKeys");
    }
    return new JDBCPreparedStatement(this,sql,JDBCResultSet.TYPE_FORWARD_ONLY,JDBCResultSet.CONCUR_READ_ONLY,rsHoldability,autoGeneratedKeys,null,null);
  }
 catch (  HsqlException e) {
    throw Util.sqlException(e);
  }
}
