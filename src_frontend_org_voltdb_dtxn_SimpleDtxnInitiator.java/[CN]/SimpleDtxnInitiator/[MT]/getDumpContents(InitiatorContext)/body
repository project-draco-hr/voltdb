{
  if (m_mailbox instanceof SiteMailbox)   context.mailboxHistory=((SiteMailbox)m_mailbox).getHistory();
  ArrayList<InFlightTxnState> inFlightTxnList=new ArrayList<InFlightTxnState>();
  try {
    for (    InFlightTxnState txn : m_pendingTxns.getInFlightTxns())     inFlightTxnList.add(txn);
  }
 catch (  Exception e) {
  }
  context.inFlightTxns=new InFlightTxnState[inFlightTxnList.size()];
  for (int i=0; i < inFlightTxnList.size(); i++)   context.inFlightTxns[i]=inFlightTxnList.get(i);
}
