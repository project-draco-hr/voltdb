{
  long newestSafeTxnId=m_safetyState.getNewestSafeTxnIdForExecutorBySiteId(coordinatorId);
  InitiateTaskMessage workRequest=new InitiateTaskMessage(m_siteId,coordinatorId,txn.txnId,txn.isReadOnly,txn.isSinglePartition,txn.invocation,newestSafeTxnId);
  try {
    m_mailbox.send(coordinatorId,0,workRequest);
  }
 catch (  MessagingException e) {
    throw new RuntimeException(e);
  }
}
