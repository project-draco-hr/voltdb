{
  long newestSafeTxnId=m_safetyState.getNewestSafeTxnIdForExecutorBySiteId(txn.coordinatorId);
  InitiateTaskMessage workRequest=new InitiateTaskMessage(m_siteId,txn.coordinatorId,txn.txnId,txn.isReadOnly,txn.isSinglePartition,txn.invocation,newestSafeTxnId);
  try {
    m_mailbox.send(txn.coordinatorId,0,workRequest);
  }
 catch (  MessagingException e) {
    throw new RuntimeException(e);
  }
}
