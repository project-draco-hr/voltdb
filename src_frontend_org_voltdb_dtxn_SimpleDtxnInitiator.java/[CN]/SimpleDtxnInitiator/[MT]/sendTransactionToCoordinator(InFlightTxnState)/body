{
  InitiateTask workRequest=new InitiateTask(m_siteId,txn.coordinatorId,txn.txnId,txn.isReadOnly,txn.isSinglePartition,txn.invocation,Long.MAX_VALUE);
  workRequest.setNonCoordinatorSites(txn.otherSiteIds);
  try {
    m_mailbox.send(txn.coordinatorId,0,workRequest);
  }
 catch (  MessagingException e) {
    throw new RuntimeException(e);
  }
}
