{
  long txnId;
  txnId=m_idManager.getNextUniqueTransactionId();
  boolean retval=createTransaction(connectionId,connectionHostname,adminConnection,txnId,invocation,isReadOnly,isSinglePartition,isEveryPartition,isNonDeterministic,partitions,numPartitions,clientData,messageSize,now);
  return retval;
}
