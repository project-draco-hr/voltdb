{
  assert(!((params == null) && (unserializedParams == null)));
  assert((params != null) || (unserializedParams != null));
  out.write(0);
  out.writeString(procName);
  out.writeLong(clientHandle);
  if (ProcedureCallback.measureLatency) {
    out.writeLong(clientQueueTime);
    out.writeLong(CIAcceptTime);
  }
  if (params != null)   out.writeObject(params);
 else   if (unserializedParams != null)   out.write(unserializedParams.array(),unserializedParams.position() + unserializedParams.arrayOffset(),unserializedParams.remaining());
}
