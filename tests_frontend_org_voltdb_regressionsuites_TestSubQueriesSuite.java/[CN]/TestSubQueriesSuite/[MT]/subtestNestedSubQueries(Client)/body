{
  client.callProcedure("InsertR1",-1,-1,-1);
  client.callProcedure("InsertR1",1,1,1);
  client.callProcedure("InsertR1",2,2,2);
  client.callProcedure("InsertR1",3,3,3);
  VoltTable result=client.callProcedure("@AdHoc","select A2 FROM (SELECT A1 AS A2 FROM (SELECT A AS A1 FROM R1) TEMP1 WHERE TEMP1.A1 > 0) TEMP2 WHERE TEMP2.A2 = 3").getResults()[0];
  System.out.println(result.toString());
  assertEquals(1,result.getRowCount());
}
