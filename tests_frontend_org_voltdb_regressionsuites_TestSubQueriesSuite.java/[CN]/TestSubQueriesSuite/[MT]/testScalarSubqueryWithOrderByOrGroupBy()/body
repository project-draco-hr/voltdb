{
  Client client=getClient();
  long[][] expected=new long[100][1];
  for (int i=0; i < 100; ++i) {
    client.callProcedure("@AdHoc","insert into R_ENG8145_1 values (?, ?);",i,i * 2);
    client.callProcedure("@AdHoc","insert into R_ENG8145_2 values (?, ?);",i,i * 2);
    long val=100 - ((i * 2) + 1);
    if (val < 0)     val=0;
    expected[i][0]=val;
  }
  validateTableOfLongs(client,"select (select count(*) from R_ENG8145_1 where ID > parent.num) from R_ENG8145_2 parent order by id;",expected);
  validateTableOfLongs(client,"select (select count(*) from R_ENG8145_1 where ID > parent.num) from R_ENG8145_2 parent group by id;",expected);
}
