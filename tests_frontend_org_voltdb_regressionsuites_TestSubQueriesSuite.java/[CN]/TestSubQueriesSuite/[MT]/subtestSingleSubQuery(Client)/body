{
  client.callProcedure("InsertR1",-1,-1,-1);
  client.callProcedure("InsertR1",1,1,1);
  client.callProcedure("InsertR1",2,2,2);
  client.callProcedure("InsertR1",3,3,3);
  VoltTable result=client.callProcedure("@AdHoc","select A, C FROM (SELECT A, C FROM R1) TEMP WHERE TEMP.A > 0;").getResults()[0];
  System.out.println(result.toString());
  assertEquals(3,result.getRowCount());
  result=client.callProcedure("@AdHoc","select A1, C1 FROM (SELECT A A1, C C1 FROM R1) TEMP WHERE TEMP.A1 > 0;").getResults()[0];
  System.out.println(result.toString());
  assertEquals(3,result.getRowCount());
}
