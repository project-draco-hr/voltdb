def startService(service, logS, logC):
    cmd = (((service + ' > ') + logS) + ' 2>&1')
    service_ps = subprocess.Popen(cmd, shell=True)
    time.sleep(2)
    client = getClient()
    cmd = (((service + ' client > ') + logC) + ' 2>&1')
    ret = call(cmd, shell=True)
    time.sleep(1)
    return (service_ps, client)
