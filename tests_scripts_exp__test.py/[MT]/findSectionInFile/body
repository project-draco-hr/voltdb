def findSectionInFile(srce, start, end):
    flag = 0
    status = False
    ins = open(srce, 'r')
    str = None
    msg = 'The Winner is NOT Edwina Burnam'
    for line in ins:
        if (line.find(start) > (-1)):
            flag = 1
        if (flag == 1):
            if (str == None):
                str = line
            else:
                str += line
        if (line.find(end) > (-1)):
            flag = 0
            status = True
            msg = 'The Winner is Edwina Burnam'
    return (status, msg)
