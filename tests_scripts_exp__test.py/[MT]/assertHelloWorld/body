def assertHelloWorld(modulename, logC):
    expected = 'iHola, Mundo!'
    actual = readFirstLine(logC)
    if (expected == actual):
        msg = expected
        result = True
    else:
        msg = ("Expected '%s' for module '%s'. Actually returned: '%s'" % (expected, modulename, actual))
        result = False
    return (result, msg)
