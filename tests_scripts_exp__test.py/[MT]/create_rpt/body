def create_rpt(info, status, msg, keys, elapsed):
    testtime = ('%.2f' % elapsed)
    testsuites = Element('testsuites', {'time': testtime, })
    for mod in status:
        testsuite = SubElement(testsuites, 'testsuite', {'package': info['pkgname'], 'URL': info['srce'], 'hostname': hostname, 'name': pkgDict[mod], })
        for i in status[mod]:
            failureCnt = '0'
            errCnt = '0'
            if (status[mod][i] == False):
                failureCnt = '1'
            else:
                failureCnt = '0'
            print ("==-->>suite name: '%s', failureCnt: '%s', status = '%s'" % (i, failureCnt, status[mod][i]))
            if (info['ok'] == False):
                errCnt = '1'
            else:
                errCnt = '0'
            testcase = SubElement(testsuite, 'testcase', {'errors': errCnt, 'failures': failureCnt, 'name': i, })
            if (failureCnt == '1'):
                failure = SubElement(testcase, 'failure', {'Message': msg[mod][i], })
                misStr = None
                if (keys[mod][i] != None):
                    for j in keys[mod][i]:
                        if (misStr == None):
                            misStr = j
                        else:
                            misStr += (', ' + j)
                    missing = SubElement(failure, 'Missing', {'MissingString': misStr, })
            else:
                failure = SubElement(testcase, 'info', {'Message': msg[mod][i], })
            if (errCnt == '1'):
                error = SubElement(testcase, 'error', {'Error': info['err'], })
    rptf = '/tmp/exp_rpt.xml'
    fo = open(rptf, 'wb')
    fo.write(prettify(testsuites))
    fo.close()
    if (not os.path.exists(rptf)):
        rptf = None
    return rptf
