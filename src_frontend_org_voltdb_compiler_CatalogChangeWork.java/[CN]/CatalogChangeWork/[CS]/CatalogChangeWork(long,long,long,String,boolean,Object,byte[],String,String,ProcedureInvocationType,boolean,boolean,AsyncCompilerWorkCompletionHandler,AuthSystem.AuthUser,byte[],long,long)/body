{
  super(replySiteId,false,clientHandle,connectionId,hostname,adminConnection,clientData,invocationName,type,onReplica,useAdhocDDL,completionHandler,user);
  if (operationBytes != null) {
    this.operationBytes=operationBytes.clone();
  }
 else {
    this.operationBytes=null;
  }
  this.operationString=operationString;
  adhocDDLStmts=null;
  this.replayHashOverride=replayHashOverride;
