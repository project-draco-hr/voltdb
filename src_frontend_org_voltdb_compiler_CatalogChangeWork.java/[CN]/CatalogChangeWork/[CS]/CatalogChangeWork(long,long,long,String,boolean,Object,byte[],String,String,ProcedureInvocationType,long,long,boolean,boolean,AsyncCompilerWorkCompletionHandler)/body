{
  super(replySiteId,false,clientHandle,connectionId,hostname,adminConnection,clientData,invocationName,type,originalTxnId,originalUniqueId,onReplica,useAdhocDDL,completionHandler);
  if (operationBytes != null) {
    this.operationBytes=operationBytes.clone();
  }
 else {
    this.operationBytes=null;
  }
  this.operationString=operationString;
  adhocDDLStmts=null;
}
