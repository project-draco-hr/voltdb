{
  if (getPlanNodeType() == type)   return true;
  for (  AbstractPlanNode n : m_children) {
    if (n.hasAnyNodeOfType(type)) {
      return true;
    }
  }
  for (  AbstractPlanNode inlined : m_inlineNodes.values()) {
    if (inlined.hasAnyNodeOfType(type)) {
      return true;
    }
  }
  return false;
}
