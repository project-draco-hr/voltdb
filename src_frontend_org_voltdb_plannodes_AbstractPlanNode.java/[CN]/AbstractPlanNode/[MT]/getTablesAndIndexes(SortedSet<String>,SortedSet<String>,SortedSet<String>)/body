{
  getThisNodesTablesAndIndexes(tablesRead,tableUpdated,indexes);
  for (  AbstractPlanNode node : m_inlineNodes.values()) {
    node.getTablesAndIndexes(tablesRead,tableUpdated,indexes);
  }
  for (  AbstractPlanNode node : m_children) {
    node.getTablesAndIndexes(tablesRead,tableUpdated,indexes);
  }
}
