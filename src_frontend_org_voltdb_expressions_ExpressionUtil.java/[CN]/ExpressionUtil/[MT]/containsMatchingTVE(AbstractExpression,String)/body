{
  assert(expr != null);
  List<TupleValueExpression> tves=getTupleValueExpressions(expr);
  for (  TupleValueExpression tve : tves) {
    if (tve.m_tableAlias != null) {
      if (tve.m_tableAlias.equals(tableAlias)) {
        return true;
      }
    }
 else     if (tve.m_tableName.equals(tableAlias)) {
      return true;
    }
  }
  return false;
}
