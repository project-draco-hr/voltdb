{
  if (exp == null)   return;
  if (exp.m_left != null)   assignOutputValueTypesRecursively(exp.m_left);
  if (exp.m_right != null)   assignOutputValueTypesRecursively(exp.m_right);
  Pair<VoltType,Integer> exprData=calculateOutputValueTypes(exp);
  exp.m_valueType=exprData.getFirst();
  exp.m_valueSize=exprData.getSecond();
}
