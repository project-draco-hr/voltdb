{
  assert(m_initialized);
  assert(message != null);
  ForeignHost host=presend(siteId,mailboxId,message);
  if (host != null) {
    int dests[]={siteId};
    host.send(mailboxId,dests,1,new DeferredSerialization(){
      @Override public final BBContainer serialize(      final DBBPool pool) throws IOException {
        return message.getBufferForMessaging(pool);
      }
      @Override public final void cancel(){
        message.discard();
      }
    }
);
  }
}
