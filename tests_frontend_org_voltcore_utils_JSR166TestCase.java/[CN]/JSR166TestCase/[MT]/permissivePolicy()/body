{
  return new AdjustablePolicy(new RuntimePermission("modifyThread"),new RuntimePermission("getClassLoader"),new RuntimePermission("setContextClassLoader"),new SecurityPermission("getPolicy"),new SecurityPermission("setPolicy"),new RuntimePermission("setSecurityManager"),new RuntimePermission("accessDeclaredMembers"),new PropertyPermission("*","read"),new java.io.FilePermission("<<ALL FILES>>","read"));
