{
  try {
    m_handler.stop();
    if (m_bundle != null) {
      m_bundle.stop();
    }
    if (m_channelDistributer != null) {
      m_channelDistributer.registerChannels(m_handlerProxy.getName(),new HashSet<URI>());
    }
  }
 catch (  Exception ex) {
    m_logger.error("Failed to stop the import bundles.",ex);
  }
}
