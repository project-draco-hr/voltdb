{
  if (m_inputDepIds == null)   m_inputDepIds=new ArrayList<?>[m_fragmentIds.length];
  assert(index < m_fragmentIds.length);
  if (m_inputDepIds[index] == null)   m_inputDepIds[index]=new ArrayList<Integer>();
  @SuppressWarnings("unchecked") ArrayList<Integer> l=(ArrayList<Integer>)m_inputDepIds[index];
  l.add(depId);
  m_inputDepCount++;
}
