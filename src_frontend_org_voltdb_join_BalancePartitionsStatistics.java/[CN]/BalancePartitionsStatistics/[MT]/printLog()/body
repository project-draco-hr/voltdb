{
  if (bytesTransferredInLastSec.isEmpty()) {
    log.info("No range is migrated so far.");
  }
 else {
    double rangesPerSecond=(rangeSizeMoved / (double)(bytesTransferredInLastSec.lastKey() - start)) * 1000.0;
    double avgBalanceTime=totalBalanceTime / (double)count;
    avgBalanceTime/=1000000.0;
    log.info(String.format("Migrated %.2f%% of the range at %.2f ranges/sec or %.2f MB/sec with average " + "@BalancePartition round-trip latency of %.2f milliseconds.",(rangeSizeMoved * 100.0) / totalRangeSize,rangesPerSecond,throughput / (1024.0 * 1024.0),avgBalanceTime));
  }
}
