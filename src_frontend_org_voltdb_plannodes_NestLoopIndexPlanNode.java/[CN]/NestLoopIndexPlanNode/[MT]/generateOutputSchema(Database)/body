{
  IndexScanPlanNode inlineScan=(IndexScanPlanNode)m_inlineNodes.get(PlanNodeType.INDEXSCAN);
  assert(inlineScan != null);
  inlineScan.generateOutputSchema(db);
  assert(m_children.size() == 1);
  m_children.get(0).generateOutputSchema(db);
  m_outputSchema=m_children.get(0).getOutputSchema().join(inlineScan.getOutputSchema()).copyAndReplaceWithTVE();
  m_hasSignificantOutputSchema=true;
  if (m_children.get(0).getPlanNodeType() == PlanNodeType.MATERIALIZEDSCAN) {
    assert(((MaterializedScanPlanNode)m_children.get(0)).getSortDirection() == inlineScan.getSortDirection());
    m_sortDirection=inlineScan.getSortDirection();
  }
}
