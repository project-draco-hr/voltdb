{
  m_config=config;
  m_config.m_pathToLicense=getTestLicensePath();
  if (m_config.m_leader == null) {
    m_config.m_leader="";
  }
  if (!m_config.validate()) {
    System.exit(-1);
  }
  m_config.m_noLoadLibVOLTDB=m_config.m_backend == BackendTarget.HSQLDB_BACKEND;
  setName("ServerThread");
}
