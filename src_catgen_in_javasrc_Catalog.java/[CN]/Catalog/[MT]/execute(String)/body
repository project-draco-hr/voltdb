{
  m_currentCatalogVersion++;
  int ctr=0;
  for (  String line : commands.split("\n")) {
    try {
      if (line.length() > 0)       executeOne(line);
    }
 catch (    Exception ex) {
      String msg="Invalid catalog command on line " + ctr + "\n"+ "Contents: '"+ line+ "'\n";
      ex.printStackTrace();
      throw new RuntimeException(msg,ex);
    }
    ctr++;
  }
}
