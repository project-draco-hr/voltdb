{
  try {
    Socket sock=new Socket(hostname,DUMPMANAGER_PORT);
    DataOutputStream dos=new DataOutputStream(sock.getOutputStream());
    dos.writeLong(timestamp);
    dos.flush();
    int data=sock.getInputStream().read();
    sock.close();
    return data == 'a';
  }
 catch (  UnknownHostException e) {
    e.printStackTrace();
  }
catch (  IOException e) {
    e.printStackTrace();
  }
  return false;
}
