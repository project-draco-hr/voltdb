{
  if (fragmentId <= 0)   return;
  FragInfo frag=null;
synchronized (FragInfo.class) {
    frag=m_plansById.get(fragmentId);
    assert(frag != null);
    if (--frag.refCount == 0) {
      boolean notInLRUmap=(frag.lastUse == 0);
      frag.lastUse=++m_nextFragUse;
      if (notInLRUmap) {
        m_plansLRU.put(frag.lastUse,frag);
      }
    }
  }
}
