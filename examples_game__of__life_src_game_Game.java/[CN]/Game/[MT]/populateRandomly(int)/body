{
  double probab;
  for (int i=0; i < NUM_CELLS; i++) {
    probab=Math.random();
    if (probab <= (double)numLive / (NUM_CELLS - i)) {
      db.setIDs(i,1);
      ids[i]=true;
      numLive--;
    }
 else {
      db.setIDs(i,0);
      ids[i]=false;
    }
    neighbors[i]=findNeighborIDs(i);
  }
  if (DEBUG)   print();
}
