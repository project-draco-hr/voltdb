{
  boolean live=ids[id];
  int[] neighborIDs=neighbors[id];
  int numLiveNeighbors=0;
  for (int i=0; i < neighborIDs.length; i++)   if (ids[neighborIDs[i]])   numLiveNeighbors++;
  if (live) switch (numLiveNeighbors) {
case 2:
case 3:
    return true;
default :
  return false;
}
 else switch (numLiveNeighbors) {
case 3:
return true;
default :
return false;
}
}
