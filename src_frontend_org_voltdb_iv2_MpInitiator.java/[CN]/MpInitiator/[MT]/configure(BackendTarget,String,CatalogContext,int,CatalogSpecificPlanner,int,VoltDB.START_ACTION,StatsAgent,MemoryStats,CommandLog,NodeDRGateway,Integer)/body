{
  if ((backend == BackendTarget.NATIVE_EE_IPC) || (backend == BackendTarget.NATIVE_EE_VALGRIND_IPC)) {
    backend=BackendTarget.NATIVE_EE_JNI;
  }
  super.configureCommon(backend,serializedCatalog,catalogContext,csp,numberOfPartitions,startAction,null,null,cl,coreBindId);
  LeaderElector.createParticipantNode(m_messenger.getZK(),LeaderElector.electionDirForPartition(m_partitionId),Long.toString(getInitiatorHSId()),null);
}
