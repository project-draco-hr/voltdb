{
  if (m_executionSite != null) {
    m_executionSite.startShutdown();
  }
  if (m_siteThread != null) {
    try {
      m_siteThread.interrupt();
      m_siteThread.join();
    }
 catch (    InterruptedException giveup) {
    }
  }
  if (m_iv2masters != null) {
    try {
      m_iv2masters.shutdown();
    }
 catch (    Exception e) {
    }
  }
}
