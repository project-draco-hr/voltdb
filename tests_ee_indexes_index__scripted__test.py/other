from subprocess import Popen, PIPE
import os
import sys
print 'Hooray!!!!'
print sys.path[0]
script = '# setup commands:\n#   begin indexname\n#   types list-of-types\n# types:\n#   bi = big integer\n#   s = string\n# general commands:\n#   is = insert success\n#   if = insert failure\n#   ls = lookup success\n#   lf = lookup failure\n\nbegin TestName IntsMultiMapIndex/GenericMultiMapIndex bint bint bint\nis 5 6 7\nls 5 6 7\nexec\ndone\n'
p = Popen(os.path.join(sys.path[0], 'index_scripted_test'), shell=False, stdin=PIPE, close_fds=True)
write(script)
retcode = p.wait()
print 'retcode:', retcode
sys.exit(retcode)
