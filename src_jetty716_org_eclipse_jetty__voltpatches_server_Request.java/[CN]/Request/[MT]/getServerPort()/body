{
  if (_port <= 0) {
    if (_serverName == null)     getServerName();
    if (_port <= 0) {
      if (_serverName != null && _uri != null)       _port=_uri.getPort();
 else       _port=_endp == null ? 0 : _endp.getLocalPort();
    }
  }
  if (_port <= 0) {
    if (getScheme().equalsIgnoreCase(URIUtil.HTTPS))     return 443;
    return 80;
  }
  return _port;
}
