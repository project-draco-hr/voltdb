{
  taskLog.logTask(m_txn.getNotice());
  SpTransactionState txn=(SpTransactionState)m_txn;
  final InitiateResponseMessage response=new InitiateResponseMessage(txn.m_task);
  response.m_sourceHSId=m_initiator.getHSId();
  response.setRecovering(true);
  response.setResults(new ClientResponseImpl(ClientResponse.SUCCESS,new VoltTable[0],null));
  m_initiator.deliver(response);
}
