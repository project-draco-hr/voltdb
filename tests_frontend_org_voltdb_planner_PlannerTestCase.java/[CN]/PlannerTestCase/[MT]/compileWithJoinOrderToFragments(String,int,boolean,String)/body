{
  List<AbstractPlanNode> pn=null;
  try {
    pn=m_aide.compile(sql,paramCount,planForSinglePartition,joinOrder);
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    fail();
  }
  assertTrue(pn != null);
  assertFalse(pn.isEmpty());
  if (planForSinglePartition) {
    assertTrue(pn.size() == 1);
  }
  return pn;
}
