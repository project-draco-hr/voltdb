{
  List<AbstractPlanNode> pn=null;
  try {
    pn=compileWithJoinOrderToFragments(sql,paramCount,m_byDefaultPlanForSinglePartition,joinOrder);
  }
 catch (  Exception ex) {
    ex.printStackTrace();
    fail();
  }
  assertTrue(pn != null);
  assertFalse(pn.isEmpty());
  assertTrue(pn.get(0) != null);
  return pn.get(0);
}
