{
  if (!config.m_forceVoltdbCreate) {
    return;
  }
  File vroot=config.m_voltdbRoot;
  Set<String> paths=ImmutableSet.<String>builder().add(depl.getPaths().getCommandlog().getPath()).add(depl.getPaths().getCommandlogsnapshot().getPath()).add(depl.getPaths().getSnapshots().getPath()).add(depl.getPaths().getExportoverflow().getPath()).add(depl.getPaths().getDroverflow().getPath()).build();
  for (  String path : paths) {
    File f=new File(path);
    f=f.isAbsolute() ? f : new File(vroot,path);
    if (f.isDirectory()) {
      for (      File remove : f.listFiles()) {
        MiscUtils.deleteRecursively(remove);
      }
    }
 else {
      f.delete();
    }
  }
}
