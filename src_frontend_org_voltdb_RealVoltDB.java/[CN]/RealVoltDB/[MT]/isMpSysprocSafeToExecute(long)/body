{
synchronized (m_lastSeenMpTxnId) {
    if (txnId >= m_safeMpTxnId) {
      return true;
    }
    if (txnId > m_lastSeenMpTxnId.get()) {
      m_lastSeenMpTxnId.set(txnId);
      if (!rejoinDataPending() && m_safeMpTxnId == Long.MAX_VALUE) {
        m_safeMpTxnId=txnId;
      }
    }
    return txnId >= m_safeMpTxnId;
  }
}
