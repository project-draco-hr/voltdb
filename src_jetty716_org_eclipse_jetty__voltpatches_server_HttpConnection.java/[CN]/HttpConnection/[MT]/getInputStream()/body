{
  if (_expect100Continue) {
    if (((HttpParser)_parser).getHeaderBuffer() == null || ((HttpParser)_parser).getHeaderBuffer().length() < 2) {
      if (_generator.isCommitted())       throw new IllegalStateException("Committed before 100 Continues");
      ((HttpGenerator)_generator).send1xx(HttpStatus.CONTINUE_100);
    }
    _expect100Continue=false;
  }
  if (_in == null)   _in=new HttpInput(((HttpParser)_parser),_connector.getMaxIdleTime());
  return _in;
}
