{
  SynchronizedStatesManager ssm=null;
  try {
    assert(siteId < NUM_AGREEMENT_SITES);
    ssm=new SynchronizedStatesManager(m_messengers.get(siteId).getZK(),communityName,managerId,numberOfStateMachines);
  }
 catch (  KeeperException|InterruptedException e) {
  }
catch (  Exception e) {
  }
  return ssm;
}
