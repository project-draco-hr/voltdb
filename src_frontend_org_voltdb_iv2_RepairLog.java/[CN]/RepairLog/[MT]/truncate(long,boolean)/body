{
  if (handle == Long.MIN_VALUE) {
    return;
  }
  Deque<RepairLog.Item> deq=null;
  if (isSP) {
    deq=m_logSP;
  }
 else {
    deq=m_logMP;
  }
  RepairLog.Item item=null;
  while ((item=deq.peek()) != null) {
    if (item.canTruncate(handle)) {
      deq.poll();
    }
 else {
      break;
    }
  }
}
