{
  if (spHandle == Long.MIN_VALUE && mpHandle == Long.MIN_VALUE) {
    return;
  }
  Iterator<RepairLog.Item> it=m_log.iterator();
  while (it.hasNext()) {
    RepairLog.Item item=it.next();
    if (item.isSP() && item.m_handle <= spHandle) {
      it.remove();
    }
 else     if (item.isMP() && item.m_txnId <= mpHandle) {
      it.remove();
    }
  }
}
