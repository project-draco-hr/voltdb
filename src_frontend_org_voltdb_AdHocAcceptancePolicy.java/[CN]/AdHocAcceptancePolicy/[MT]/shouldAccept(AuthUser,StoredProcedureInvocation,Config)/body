{
  if (!invocation.procName.equals("@AdHoc")) {
    return null;
  }
  ParameterSet params=invocation.getParams();
  if (params.m_params.length > 2) {
    return new ClientResponseImpl(ClientResponseImpl.GRACEFUL_FAILURE,new VoltTable[0],"Adhoc system procedure requires exactly one or two parameters, " + "the SQL statement to execute with an optional partitioning value.",invocation.clientHandle);
  }
  if ((params.m_params[0] instanceof String) == false) {
    return new ClientResponseImpl(ClientResponseImpl.GRACEFUL_FAILURE,new VoltTable[0],"Adhoc system procedure requires sql in the String type only.",invocation.clientHandle);
  }
  return null;
}
