{
  m_buffer.position(HEADER_SIZE + 1);
  m_metadata=new byte[16];
  m_buffer.get(m_metadata);
  final int payloadLength=m_buffer.getInt();
  if (payloadLength > -1) {
    m_payload=new byte[payloadLength];
    m_buffer.get(m_payload);
  }
  m_buffer=null;
  m_container.discard();
  m_container=null;
}
