def get_required(self, key):
    value = self.get(key)
    if (value is None):
        vcli_util.abort(('Configuration parameter "%s.%s" was not found.' % (path, name)), 'Set parameters using the "config" command, for example:', [('%s config %s.%s=VALUE' % (vcli_meta.bin_name, path, name))])
    return value
