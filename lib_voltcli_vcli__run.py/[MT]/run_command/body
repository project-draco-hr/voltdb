def run_command(verbspace, config, *cmdargs):
    '\n    Run a command after parsing the command line arguments provided.\n    '
    aliases = {}
    alias_dict = config.query(filter='alias.')
    for key in alias_dict:
        name = key.split('.', 2)[1]
        aliases[name] = alias_dict[key]
    processor = vcli_cli.VoltCLICommandProcessor(verbspace.verbs, base_cli_spec.options_spec, aliases, base_cli_spec.usage, '\n'.join((verbspace.description, base_cli_spec.description)), ('%%prog version %s' % verbspace.version))
    command = processor.parse(cmdargs)
    vcli_util.set_debug(command.outer_opts.debug)
    vcli_util.set_dryrun(command.outer_opts.dryrun)
    runner = VerbRunner(command, verbspace, config)
    runner.execute()
