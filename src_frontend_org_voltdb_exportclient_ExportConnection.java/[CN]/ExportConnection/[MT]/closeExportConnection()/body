{
  if (m_socket != null) {
    if (m_socket.isConnected()) {
      try {
        m_socket.close();
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
 else {
      try {
        m_socket.close();
      }
 catch (      IOException e) {
      }
 finally {
        m_socket=null;
      }
    }
  }
  m_state=CLOSED;
  dataSources.clear();
  if (m_bandwidthMonitor != null)   m_bandwidthMonitor.removeHost(m_ipString);
}
