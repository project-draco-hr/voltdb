{
  if (isValgrind()) {
    return;
  }
  Client client=getClient();
  client.callProcedure("InsertBatch",20000000,0);
  boolean threw=false;
  try {
    client.callProcedure("Delete");
  }
 catch (  ProcCallException pce) {
    pce.printStackTrace();
    threw=true;
  }
  assertFalse("Expected to be able to delete large batch but failed",threw);
  client.callProcedure("InsertBatch",20000000,0);
  threw=false;
  try {
    client.callProcedure("Delete");
  }
 catch (  ProcCallException pce) {
    pce.printStackTrace();
    threw=true;
  }
  assertFalse("Expected to be able to delete large batch but failed",threw);
}
