def test_plugin_version_comparison(self):
    "Verify that valid versions are ordinally correct.\n\n        That is, for each given scenario, v1.op(v2)==True where 'op' is the\n        equality operator defined for the scenario.\n        "
    plugin_name = 'Jenkins Mailer Plugin'
    j = jenkins.Jenkins(self.base_url, 'test', 'test')
    plugin_info = j.get_plugins()[plugin_name]
    v1 = plugin_info.get('version')
    op = getattr(v1, self.op)
    self.assertTrue(op(self.v2), msg='Unexpectedly found {0} {2} {1} == False when comparing versions!'.format(v1, self.v2, self.op))
