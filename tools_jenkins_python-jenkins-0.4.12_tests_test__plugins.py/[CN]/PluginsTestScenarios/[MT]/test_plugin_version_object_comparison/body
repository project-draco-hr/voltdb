def test_plugin_version_object_comparison(self):
    'Verify use of PluginVersion for comparison\n\n        Verify that converting the version to be compared to the same object\n        type of PluginVersion before comparing provides the same result.\n        '
    plugin_name = 'Jenkins Mailer Plugin'
    j = jenkins.Jenkins(self.base_url, 'test', 'test')
    plugin_info = j.get_plugins()[plugin_name]
    v1 = plugin_info.get('version')
    op = getattr(v1, self.op)
    v2 = plugins.PluginVersion(self.v2)
    self.assertTrue(op(v2), msg='Unexpectedly found {0} {2} {1} == False when comparing versions!'.format(v1, v2, self.op))
