@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_raise_HTTPError(self, jenkins_mock):
    jenkins_mock.side_effect = jenkins.HTTPError(self.make_url('job/pluginManager/api/json?depth=2'), code=401, msg='basic auth failed', hdrs=[], fp=None)
    with self.assertRaises(jenkins.JenkinsException) as context_manager:
        self.j.get_plugin_info(u'TestPlugin', depth=52)
    self.assertEqual(str(context_manager.exception), 'Error communicating with server[{0}/]'.format(self.base_url))
    self._check_requests(jenkins_mock.call_args_list)
