{
  if (condition) {
    System.out.println("IN TruncatePeople throwing: " + label);
    if (expectedRows != null) {
      System.out.println("EXPECTED: " + expectedRows);
    }
    if (actualRows != null) {
      System.out.println("  ACTUAL: " + actualRows);
    }
    throw new VoltAbortException(label);
  }
}
