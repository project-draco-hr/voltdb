{
  if (m_memoryDeque.size() > 1) {
    m_persistentDeque.offer(streamBlock.asBufferChain());
  }
 else {
    if (pollPersistentDeque(false) != null) {
      if (m_memoryDeque.size() > 1) {
        m_persistentDeque.offer(streamBlock.asBufferChain());
      }
 else       if (pollPersistentDeque(false) == null) {
        m_memoryDeque.offer(streamBlock);
      }
 else {
        m_persistentDeque.offer(streamBlock.asBufferChain());
      }
    }
 else {
      m_memoryDeque.offer(streamBlock);
    }
  }
}
