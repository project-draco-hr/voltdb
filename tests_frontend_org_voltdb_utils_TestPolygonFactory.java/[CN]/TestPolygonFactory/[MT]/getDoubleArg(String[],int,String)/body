{
  double ans=-1;
  if (args.length <= arg) {
    System.err.printf("%s\n",msg);
    System.exit(100);
  }
 else {
    try {
      ans=Double.parseDouble(args[arg]);
    }
 catch (    IllegalArgumentException ex) {
      System.err.printf("%s\n",msg);
      System.exit(100);
    }
  }
  return ans;
}
