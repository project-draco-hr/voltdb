def characters(self, content):
    "Marks the inner content of an element.\n\n        This is the callback used by the SAX XML parser to notify us about the\n        inner content of an element.\n\n        'content': The inner content.\n        "
    content = content.strip()
    if content:
        if (len(self.__current_key) == 0):
            raise InvalidXML('No tag opened.')
        if (type(self.__current[(-1)]) is list):
            self.__current[(-1)].append(content)
        else:
            if (self.__current_key[(-1)] in self.__current[(-1)]):
                content = (self.__current[(-1)][self.__current_key[(-1)]] + content)
            else:
                content = content
            self.__current[(-1)][self.__current_key[(-1)]] = content
