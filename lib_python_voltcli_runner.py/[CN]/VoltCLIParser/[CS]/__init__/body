def __init__(self, verbspace):
    '\n        VoltCLIParser constructor.\n        '
    cli.CLIParser.__init__(self, environment.command_name, verbspace.verbs, base_cli_spec.options, base_cli_spec.usage, '\n'.join((verbspace.description, base_cli_spec.description)), ('%%prog version %s\npro=%s' % (verbspace.version, verbspace.pro_version)))
