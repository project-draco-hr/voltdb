def compile(self, outdir, *srcfiles):
    '\n        Compile Java source using javac.\n        '
    self.initialize()
    if (not os.path.exists(outdir)):
        os.makedirs(outdir)
    utility.run_cmd('javac', '-target', '1.7', '-source', '1.7', '-classpath', self.classpath, '-d', outdir, *srcfiles)
