def run_command(verbspace, config, *cmdargs):
    '\n    Run a command after parsing the command line arguments provided.\n    '
    processor = cli.VoltCLICommandProcessor(verbspace.verbs, base_cli_spec.cli_options, base_cli_spec.usage, '\n'.join((verbspace.description, base_cli_spec.description)), ('%%prog version %s' % verbspace.version))
    command = processor.parse(cmdargs)
    utility.set_debug(command.outer_opts.debug)
    utility.set_dryrun(command.outer_opts.dryrun)
    runner = VerbRunner(command, verbspace, config, processor)
    runner.execute()
