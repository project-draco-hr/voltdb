{
  VoltServerConfig config=null;
  MultiConfigSuiteBuilder builder=new MultiConfigSuiteBuilder(TestGeographyValue.class);
  boolean success;
  VoltProjectBuilder project=new VoltProjectBuilder();
  String literalSchema="CREATE TABLE T (\n" + "  PK INTEGER NOT NULL PRIMARY KEY,\n" + "  NAME VARCHAR(32),\n"+ "  POLY GEOGRAPHY\n"+ ");\n"+ "CREATE TABLE T_NOT_NULL (\n"+ "  PK INTEGER NOT NULL PRIMARY KEY,\n"+ "  NAME VARCHAR(32),\n"+ "  POLY GEOGRAPHY NOT NULL\n"+ ");\n"+ "CREATE PROCEDURE select_in AS \n"+ "  SELECT pk FROM t WHERE poly IN ? ORDER BY pk ASC;\n"+ "\n";
  try {
    project.addLiteralSchema(literalSchema);
  }
 catch (  Exception e) {
    fail();
  }
  config=new LocalCluster("geography-value-onesite.jar",1,1,0,BackendTarget.NATIVE_EE_JNI);
  success=config.compile(project);
  assertTrue(success);
  builder.addServerConfig(config);
  return builder;
}
