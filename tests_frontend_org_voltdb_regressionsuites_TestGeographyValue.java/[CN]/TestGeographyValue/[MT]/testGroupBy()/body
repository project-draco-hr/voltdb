{
  Client client=getClient();
  int pk=0;
  pk=fillTable(client,"t",pk);
  pk=fillTable(client,"t",pk);
  pk=fillTable(client,"t",pk);
  VoltTable vt=client.callProcedure("@AdHoc","select poly, count(*) " + "from t " + "group by poly "+ "order by poly asc").getResults()[0];
  assertContentOfTable(new Object[][]{{null,3},{BERMUDA_TRIANGLE_POLY,3},{BILLERICA_TRIANGLE_POLY,3},{LOWELL_SQUARE_POLY,3},{BERMUDA_TRIANGLE_HOLE_POLY,3}},vt);
}
