{
  if (procedure instanceof MockROSPVoltProcedure)   return new MockSPProcedureRunner((MockSPVoltProcedure)procedure,(ExecutionSite)site);
 else   if (procedure instanceof MockSPVoltProcedure)   return new MockSPProcedureRunner((MockSPVoltProcedure)procedure,(ExecutionSite)site);
 else   if (procedure instanceof MockMPVoltProcedure)   return new MockMPProcedureRunner((MockMPVoltProcedure)procedure,(ExecutionSite)site);
 else   if (procedure instanceof VoltSystemProcedure)   return super.create(procedure,site,catProc,hsql);
 else   assert(false);
  return null;
}
