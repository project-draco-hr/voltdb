{
  message.m_sourceHSId=m_siteId;
  m_totalSends++;
  if (message instanceof HeartbeatResponseMessage) {
    m_heartBeatSends++;
  }
  if (shouldFail(false)) {
    killSite();
    return;
  }
  RussianRouletteMailbox dest=postoffice.get(siteId);
  if (dest != null) {
    dest.deliver(message);
  }
}
