{
  m_siteLogger.get(m_siteId).trace("FUZZTEST selfNodeFailure " + MiscUtils.getHostIdFromHSId(m_siteId));
  m_sites.get(m_siteId).shutdown();
  m_voltdb.killSite(m_siteId);
  m_voltdb.getFaultDistributor().reportFault(new SiteFailureFault(Arrays.asList(new Long[]{m_siteId,MiscUtils.getHSIdFromHostAndSite(m_siteId.intValue(),MiscUtils.getSiteIdFromHSId(m_siteId) + 1000)})));
  postoffice.remove(m_siteId);
  throw new Error();
}
