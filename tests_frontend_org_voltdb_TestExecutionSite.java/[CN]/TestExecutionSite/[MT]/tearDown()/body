{
  super.tearDown();
  for (int i=0; i < m_sites.length; ++i) {
    m_sites[i]=null;
    m_mboxes[i]=null;
  }
  m_voltdb.getFaultDistributor().shutDown();
  m_voltdb.m_snapshotCompletionMonitor.shutdown();
  m_voltdb.getZK().close();
  m_voltdb.m_agreementSite.shutdown();
  m_voltdb=null;
  m_checker=null;
}
