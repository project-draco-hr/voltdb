{
  if (!m_pendingSites.isEmpty()) {
    long nextSite=m_pendingSites.poll();
    m_rejoiningSites.add(nextSite);
    rejoinLog.info("Finished streaming snapshot to site: " + CoreUtils.hsIdToString(HSId) + " and initiating snapshot stream to next site: "+ CoreUtils.hsIdToString(nextSite));
    initiateRejoinOnSite(nextSite);
  }
 else {
    rejoinLog.info("Finished streaming snapshot to site: " + CoreUtils.hsIdToString(HSId));
  }
}
