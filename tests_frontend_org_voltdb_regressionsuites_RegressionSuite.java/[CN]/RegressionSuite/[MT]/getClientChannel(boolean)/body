{
  final List<String> listeners=m_config.getListenerAddresses();
  final Random r=new Random();
  final String listener=listeners.get(r.nextInt(listeners.size()));
  final SocketChannel channel=ConnectionUtil.getAuthenticatedConnection(listener,m_username,m_password,Client.VOLTDB_SERVER_PORT);
  channel.configureBlocking(true);
  if (!noTearDown) {
synchronized (m_clientChannels) {
      m_clientChannels.add(channel);
    }
  }
  return channel;
}
