{
  checkClosed();
  closeCurrentResult();
  if (batch == null || batch.size() == 0)   return new int[0];
  int[] updateCounts=new int[batch.size()];
  for (int i=0; i < batch.size(); i++) {
    try {
      setCurrentResult(null,(int)batch.get(i).execute(sourceConnection.NativeConnection,this.m_timeout)[0].fetchRow(0).getLong(0));
      updateCounts[i]=this.lastUpdateCount;
    }
 catch (    SQLException x) {
      updateCounts[i]=EXECUTE_FAILED;
      throw new BatchUpdateException(Arrays.copyOf(updateCounts,i + 1),x);
    }
  }
  return updateCounts;
}
