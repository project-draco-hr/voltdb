{
  if (params.length != this.parameterCount)   throw SQLError.get(SQLError.ILLEGAL_ARGUMENT);
  if (this.type == TYPE_EXEC)   return new VoltSQL(this.sql,this.parameterCount,this.type,params);
 else   if (this.parameterCount == 0)   return new VoltSQL(this.sql,0,this.type);
 else {
    Object[] paramsOut=new Object[params.length + 1];
    paramsOut[0]=this.sql[0];
    for (int i=0; i < params.length; ++i) {
      paramsOut[i + 1]=params[i];
    }
    return new VoltSQL(new String[]{"@AdHoc"},this.parameterCount,this.type,paramsOut);
  }
}
