{
  Matcher m=ExtractParameterizedCall.matcher(jdbcCall);
  if (m.matches())   return new VoltSQL(new String[]{m.group(1)},CleanCallParameters.matcher(m.group(2)).replaceAll("").length(),TYPE_EXEC,timeout);
 else {
    m=ExtractNoParameterCall.matcher(jdbcCall);
    if (m.matches())     return new VoltSQL(new String[]{m.group(1)},0,TYPE_EXEC,timeout);
  }
  throw SQLError.get(SQLError.ILLEGAL_STATEMENT);
}
