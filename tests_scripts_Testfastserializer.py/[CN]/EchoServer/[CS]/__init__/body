def __init__(self):
    threading.Thread.__init__(self)
    self.int32Type = (lambda length: ('%c%di' % ('>', length)))
    self.client = None
    self.socket = None
    self.socket = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    self.socket.bind(('localhost', 21213))
    self.socket.listen(1)
    self.running = 1
    self.start()
