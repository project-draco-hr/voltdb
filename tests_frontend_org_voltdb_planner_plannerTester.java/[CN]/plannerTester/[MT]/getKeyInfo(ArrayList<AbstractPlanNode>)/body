{
  String str="";
  for (  AbstractPlanNode pn : pnList) {
    str+="[";
    str+=pn.getPlanNodeId();
    str+="-";
    str+=pn.getPlanNodeType().toString();
    str+=", ";
  }
  if (str.length() > 1) {
    str=str.substring(0,str.length() - 2);
    str+="]";
  }
 else {
    str+="[]";
  }
  return str;
}
