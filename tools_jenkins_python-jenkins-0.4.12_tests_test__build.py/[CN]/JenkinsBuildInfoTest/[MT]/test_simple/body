@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_simple(self, jenkins_mock):
    build_info_to_return = {u'building': False, u'msg': u'test', u'revision': 66, u'user': u'unknown', }
    jenkins_mock.return_value = json.dumps(build_info_to_return)
    build_info = self.j.get_build_info(u'Test Job', number=52)
    self.assertEqual(build_info, build_info_to_return)
    self.assertEqual(jenkins_mock.call_args[0][0].get_full_url(), self.make_url('job/Test%20Job/52/api/json?depth=0'))
    self._check_requests(jenkins_mock.call_args_list)
