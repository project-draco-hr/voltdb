@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_in_folder_raise_HTTPError(self, jenkins_mock):
    jenkins_mock.side_effect = jenkins.HTTPError(self.make_url('job/a%20Folder/job/TestJob/api/json?depth=0'), code=401, msg='basic auth failed', hdrs=[], fp=None)
    with self.assertRaises(jenkins.JenkinsException) as context_manager:
        self.j.get_build_info(u'a Folder/TestJob', number=52)
    self.assertEqual(str(context_manager.exception), 'job[a Folder/TestJob] number[52] does not exist')
    self._check_requests(jenkins_mock.call_args_list)
