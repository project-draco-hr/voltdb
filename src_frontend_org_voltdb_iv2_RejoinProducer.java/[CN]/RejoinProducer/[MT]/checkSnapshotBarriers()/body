{
  try {
    if (SnapshotSiteProcessor.m_snapshotCreateSetupBarrier != null && SnapshotSiteProcessor.m_snapshotCreateSetupBarrier.getNumberWaiting() > 0) {
      SnapshotSiteProcessor.m_snapshotCreateSetupBarrier.await();
      SnapshotSiteProcessor.m_snapshotCreateFinishBarrier.await();
    }
  }
 catch (  Exception e) {
    Throwables.propagate(e);
  }
}
