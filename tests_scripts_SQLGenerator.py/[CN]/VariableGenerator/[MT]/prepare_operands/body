def prepare_operands(self, statement):
    result = self.EXPR.search(statement)
    if result:
        self.__label = result.group(3)
        self.__type = result.group(4)
        self._fn = ('{%s}' % self.fn_gen.next())
        stmt = self.EXPR.sub(self._fn, statement, 1)
    else:
        return None
    return [stmt, [self]]
