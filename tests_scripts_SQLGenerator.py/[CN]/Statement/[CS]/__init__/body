def __init__(self, text):
    self.__text = unicode(text)
    self.__generator_types = (CmpGenerator, MathGenerator, LogicGenerator, NegationGenerator, DistinctGenerator, SortOrderGenerator, AggregationGenerator, NumericAggregationGenerator, LikeGenerator, SetGenerator, ConstantGenerator, PickGenerator, IdGenerator)
    self.__statement = self.__text
    self.__table_gens = []
    self.__first_table = None
    table_gen = TableGenerator()
    ret = table_gen.prepare_operands(self.__statement)
    while ret:
        self.__table_gens.append(table_gen)
        self.__statement = ret
        table_gen = TableGenerator()
        ret = table_gen.prepare_operands(self.__statement)
