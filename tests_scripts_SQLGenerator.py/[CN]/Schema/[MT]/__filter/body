def __filter(self, x, type, col_per_table):
    if type:
        self.__types = [i[1] for i in self.__schema[x]['columns'] if (i[1] in type)]
    else:
        self.__types = [i[1] for i in self.__schema[x]['columns']]
    self.__types = set((i for i in self.__types if (self.__types.count(i) >= col_per_table)))
    if (len(self.__types) == 0):
        return False
    return True
