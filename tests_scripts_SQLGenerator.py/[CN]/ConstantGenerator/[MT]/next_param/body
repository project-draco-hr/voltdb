def next_param(self):
    for i in self.__value_generator.generate_values(COUNT):
        if (IS_VOLT and (self.__type == 'timestamp')):
            i = (i * 1000)
        elif isinstance(i, basestring):
            i = (u"'%s'" % i)
        elif isinstance(i, float):
            i = (u'%.20e' % i)
        yield unicode(i)
