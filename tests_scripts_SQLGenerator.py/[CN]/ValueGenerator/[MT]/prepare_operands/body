def prepare_operands(self, statement):
    result = self.EXPR.search(statement)
    if result:
        self.__type = result.group(4)
        self.__range = result.group(8, 9)
        self.__fn = ('{%s}' % self.fn_gen.next())
        stmt = self.EXPR.sub(self.__fn, statement, 1)
    else:
        return None
    return [stmt, [self]]
