def map_type(type_str):
    TYPES = {'int': (FastSerializer.VOLTTYPE_TINYINT, FastSerializer.VOLTTYPE_SMALLINT, FastSerializer.VOLTTYPE_INTEGER, FastSerializer.VOLTTYPE_BIGINT), 'byte': (FastSerializer.VOLTTYPE_TINYINT,), 'int16': (FastSerializer.VOLTTYPE_SMALLINT,), 'int32': (FastSerializer.VOLTTYPE_INTEGER,), 'int64': (FastSerializer.VOLTTYPE_BIGINT,), 'float': (FastSerializer.VOLTTYPE_FLOAT,), 'string': (FastSerializer.VOLTTYPE_STRING,), 'decimal': (FastSerializer.VOLTTYPE_DECIMAL,), 'date': (FastSerializer.VOLTTYPE_TIMESTAMP,), }
    if (type_str in TYPES):
        return TYPES[type_str]
    else:
        return None
