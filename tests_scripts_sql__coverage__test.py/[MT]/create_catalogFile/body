def create_catalogFile(voltcompiler, flush4projectFile, catalogFilename):
    catalogFile = (('/tmp/' + catalogFilename) + '.jar')
    cmd = ((((voltcompiler + ' /tmp ') + flush4projectFile) + ' ') + catalogFile)
    call(cmd, shell=True)
    if (not os.path.exists(catalogFile)):
        catalogFile = None
    return catalogFile
