def get_time_html_table_element(seconds, error_below=0, strong_warn_below=0, warn_below=0, error_above=21600, strong_warn_above=3600, warn_above=600, use_gray=False):
    return ('<td align=right%s>%s</td>' % (get_html_table_element_color(seconds, error_below, strong_warn_below, warn_below, error_above, strong_warn_above, warn_above, use_gray), minutes_colon_seconds(seconds)))
