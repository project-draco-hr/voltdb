def get_numerical_html_table_element(value, error_below=(-1), strong_warn_below=0, warn_below=0, error_above=1000000000, strong_warn_above=1000000, warn_above=100000, use_gray=False):
    return ('<td align=right%s>%d</td>' % (get_html_table_element_color(value, error_below, strong_warn_below, warn_below, error_above, strong_warn_above, warn_above, use_gray), value))
