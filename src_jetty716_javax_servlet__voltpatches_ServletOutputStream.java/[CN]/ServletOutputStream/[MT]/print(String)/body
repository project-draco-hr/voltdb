{
  if (s == null)   s="null";
  int len=s.length();
  for (int i=0; i < len; i++) {
    char c=s.charAt(i);
    if ((c & 0xff00) != 0) {
      String errMsg=lStrings.getString("err.not_iso8859_1");
      Object[] errArgs=new Object[1];
      errArgs[0]=new Character(c);
      errMsg=MessageFormat.format(errMsg,errArgs);
      throw new CharConversionException(errMsg);
    }
    write(c);
  }
}
