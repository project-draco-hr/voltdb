{
  m_closed=true;
  m_allowAcceptingMastership.release();
  RunnableWithES runnable=new RunnableWithES(){
    @Override public void run(){
      try {
        m_committedBuffers.close();
      }
 catch (      IOException e) {
        exportLog.error(e);
      }
 finally {
        getLocalExecutorService().shutdown();
      }
    }
  }
;
  return stashOrSubmitTask(runnable,false);
}
