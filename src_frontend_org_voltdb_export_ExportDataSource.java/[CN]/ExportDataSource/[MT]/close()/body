{
  m_closed=true;
  m_allowAcceptingMastership.release();
  return m_es.submit((new Runnable(){
    @Override public void run(){
      try {
        m_committedBuffers.close();
      }
 catch (      IOException e) {
        exportLog.error(e);
      }
 finally {
        m_es.shutdown();
      }
    }
  }
));
}
