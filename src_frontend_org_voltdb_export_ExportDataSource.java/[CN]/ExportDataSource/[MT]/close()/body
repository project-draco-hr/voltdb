{
  if (m_shutdown) {
    return null;
  }
  return m_es.submit(new Runnable(){
    @Override public void run(){
      try {
        if (m_shutdown) {
          return;
        }
        m_committedBuffers.close();
        m_shutdown=true;
      }
 catch (      IOException e) {
        exportLog.error(e);
      }
 finally {
        m_es.shutdown();
      }
    }
  }
);
}
