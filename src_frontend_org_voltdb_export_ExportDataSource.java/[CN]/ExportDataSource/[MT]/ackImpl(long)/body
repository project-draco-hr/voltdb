{
  if (uso == Long.MIN_VALUE && m_onDrain != null) {
    m_onDrain.run();
    return;
  }
  if (uso > 0) {
    try {
      releaseExportBytes(uso);
    }
 catch (    IOException e) {
      VoltDB.crashLocalVoltDB("Error attempting to release export bytes",true,e);
      return;
    }
  }
 else {
    exportLog.info("Ack with 0 for source ");
  }
}
