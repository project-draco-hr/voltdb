{
  final SettableFuture<BBContainer> fut=SettableFuture.create();
  m_es.submit(new Runnable(){
    @Override public void run(){
      if (m_pollFuture != null) {
        fut.setException(new RuntimeException("Should not poll more than once"));
        return;
      }
      pollImpl(fut);
    }
  }
);
  return fut;
}
