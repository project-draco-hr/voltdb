{
  try {
    return m_es.submit(new Callable<Long>(){
      @Override public Long call() throws Exception {
        return m_committedBuffers.sizeInBytes();
      }
    }
).get();
  }
 catch (  RejectedExecutionException e) {
    return 0;
  }
catch (  Throwable t) {
    Throwables.propagate(t);
    return 0;
  }
}
