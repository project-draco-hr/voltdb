{
  Preconditions.checkNotNull(m_onMastership,"mastership runnable is not yet set");
  m_es.execute(new Runnable(){
    @Override public void run(){
      try {
        if (!m_es.isShutdown()) {
          m_onMastership.run();
        }
      }
 catch (      Exception e) {
        exportLog.error("Error in accepting mastership",e);
      }
    }
  }
);
}
