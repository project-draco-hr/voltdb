{
  if (!expr.name.equals("operation")) {
    return false;
  }
  if (!expr.attributes.containsKey("optype") || !expr.attributes.get("optype").equals("equal")) {
    return false;
  }
  int rowCount=0;
  int tableCount=0;
  for (  VoltXMLElement child : expr.children) {
    if (child.name.equals("row")) {
      rowCount++;
    }
    if (child.name.equals("table")) {
      tableCount++;
    }
  }
  if ((tableCount + rowCount) > 0) {
    assert((rowCount + tableCount) == 2);
    return true;
  }
  return false;
}
