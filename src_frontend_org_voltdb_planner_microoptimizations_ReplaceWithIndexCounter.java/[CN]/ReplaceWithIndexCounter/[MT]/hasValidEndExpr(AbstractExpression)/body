{
  if (endExpr == null)   return true;
  ArrayList<AbstractExpression> subEndExpr=endExpr.findAllSubexpressionsOfClass(ComparisonExpression.class);
  int ctOther=0;
  for (  AbstractExpression ae : subEndExpr) {
    ExpressionType et=ae.getExpressionType();
    if (et == ExpressionType.COMPARE_LESSTHAN || et == ExpressionType.COMPARE_LESSTHANOREQUALTO) {
      ctOther++;
    }
 else     if (et == ExpressionType.COMPARE_EQUAL) {
    }
 else {
      return false;
    }
    if ((ae.getLeft() instanceof TupleValueExpression) == false)     return false;
  }
  if (ctOther != 1)   return false;
  return true;
}
