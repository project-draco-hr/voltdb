{
  String schemaPath="";
  try {
    URL url=TPCCProjectBuilder.class.getResource("tpcc-ddl.sql");
    schemaPath=URLDecoder.decode(url.getPath(),"UTF-8");
  }
 catch (  UnsupportedEncodingException e) {
    e.printStackTrace();
    System.exit(-1);
  }
  String simpleProjectTmpl="<?xml version=\"1.0\"?>\n" + "<project>" + "<database name='database'>"+ "<%ss>"+ "<%s adhoc='" + Boolean.toString(adhoc) + "' name='default' sysproc='true'/>"+ "</%ss>"+ "<schemas><schema path='"+ schemaPath+ "' /></schemas>"+ "<procedures><procedure class='org.voltdb.compiler.procedures.TPCCTestProc' /></procedures>"+ "<partitions><partition table='WAREHOUSE' column='W_ID' /></partitions>"+ "</database>"+ "</project>";
  String simpleProject=String.format(simpleProjectTmpl,elemPfx,elemPfx,elemPfx);
  System.out.println(simpleProject);
  File projectFile=VoltProjectBuilder.writeStringToTempFile(simpleProject);
  String projectPath=projectFile.getPath();
  VoltCompiler compiler=new VoltCompiler();
  assertTrue(compiler.compileWithProjectXML(projectPath,jarFileName));
  return compiler.getCatalog();
}
