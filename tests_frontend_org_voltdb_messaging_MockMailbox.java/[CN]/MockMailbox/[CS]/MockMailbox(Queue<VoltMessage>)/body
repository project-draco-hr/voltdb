{
  for (  Subject s : Subject.values()) {
    if (s.equals(Subject.DEFAULT)) {
      if (queue == null) {
        m_messages.add(s.getId(),new ArrayDeque<VoltMessage>());
      }
 else {
        m_messages.add(s.getId(),queue);
      }
    }
 else {
      m_messages.add(s.getId(),new ArrayDeque<VoltMessage>());
    }
  }
}
