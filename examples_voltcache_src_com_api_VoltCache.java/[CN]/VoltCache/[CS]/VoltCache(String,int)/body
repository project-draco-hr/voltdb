{
  this.Servers=servers;
  this.Port=port;
  this.Connection=ClientConnectionPool.get(servers,port);
synchronized (NullCallback) {
    String key=this.Servers + ":" + this.Port;
    if (!CleanupTaskPool.containsKey(key))     CleanupTaskPool.put(key,new CleanupTask(this.Servers,this.Port));
 else     CleanupTaskPool.get(key).use();
  }
}
