{
  Expression prev=null;
  for (  Expression exp : exps) {
    if (prev != null) {
      if (type == ExpressionType.CONJUNCTION_AND)       prev=new ConjunctionAnd(prev,exp);
 else       if (type == ExpressionType.CONJUNCTION_OR)       prev=new ConjunctionOr(prev,exp);
 else       throw new IllegalArgumentException("Type must be a conjunction type");
    }
 else {
      prev=exp;
    }
  }
  return prev;
}
