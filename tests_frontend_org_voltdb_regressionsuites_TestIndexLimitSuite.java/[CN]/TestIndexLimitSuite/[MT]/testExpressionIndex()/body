  client.callProcedure("TU2.insert",3,3,"jim");
  client.callProcedure("TU2.insert",4,-1,"thea");
  client.callProcedure("TU2.insert",5,2,"thea");
  client.callProcedure("TU2.insert",6,3,"thea");
