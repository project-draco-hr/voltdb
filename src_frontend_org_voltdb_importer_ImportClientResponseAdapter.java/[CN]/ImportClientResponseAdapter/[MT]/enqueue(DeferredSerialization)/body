{
  try {
    ByteBuffer buf=null;
synchronized (this) {
      int sz=ds.getSerializedSize();
      buf=ByteBuffer.allocate(sz);
      ds.serialize(buf);
    }
    enqueue(buf);
  }
 catch (  IOException e) {
    VoltDB.crashLocalVoltDB("enqueue() in ImportClientResponseAdapter throw an exception",true,e);
  }
}
