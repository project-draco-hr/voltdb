{
  ImportCallback cb=new ImportCallback(tcont,proccb);
  long cbhandle=registerCallback(cb);
  cb.setId(cbhandle);
  task.setClientHandle(cbhandle);
  m_pendingCallbacks.put(cbhandle,cb);
  return VoltDB.instance().getClientInterface().createTransaction(connectionId(),task,catProc.getReadonly(),catProc.getSinglepartition(),catProc.getEverysite(),partition,task.getSerializedSize(),nowNanos);
}
