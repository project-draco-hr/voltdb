{
  try {
    myServerSocket.close();
    for (int i=0; i < threadPool.length; i++) {
      Work w=new Work();
      w.shouldDie=true;
      workQueue.put(w);
    }
    if (blocking) {
      serverThread.join();
      for (      HTTPWorkerThread worker : threadPool) {
        worker.join();
      }
    }
  }
 catch (  IOException ioe) {
  }
catch (  InterruptedException e) {
  }
}
