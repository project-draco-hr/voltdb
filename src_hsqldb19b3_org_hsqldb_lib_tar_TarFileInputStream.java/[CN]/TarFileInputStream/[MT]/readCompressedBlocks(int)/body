{
  int bytesSoFar=0;
  int requiredBytes=512 * blocks;
  int i;
  while (bytesSoFar < requiredBytes) {
    i=readStream.read(readBuffer,bytesSoFar,requiredBytes - bytesSoFar);
    if (i < 0) {
      throw new EOFException(RB.singleton.getString(RB.DECOMPRESS_RANOUT,bytesSoFar,requiredBytes));
    }
    bytesRead+=i;
    bytesSoFar+=i;
  }
}
