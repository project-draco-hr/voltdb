{
  if (versionString == null) {
    return null;
  }
  String[] split=versionString.split("\\.");
  int[] v=new int[split.length];
  int i=0;
  for (  String s : split) {
    try {
      v[i++]=Integer.parseInt(s.trim());
    }
 catch (    NumberFormatException e) {
      return null;
    }
  }
  return v;
}
