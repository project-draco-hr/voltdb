{
  String build="";
  String[] splitted=fullBuildString.split("=",2);
  if (splitted.length == 2) {
    build=splitted[1].trim();
    if (build.length() == 0) {
      return null;
    }
    return build;
  }
  Pattern p=Pattern.compile("-(\\d*-\\w{8}(?:-.*)?)");
  Matcher m=p.matcher(fullBuildString);
  if (!m.find())   return null;
  build=m.group(1).trim();
  if (build.length() == 0) {
    return null;
  }
  return build;
}
