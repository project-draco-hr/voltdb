{
  if (cache.isClosing()) {
    return;
  }
  try {
    m_closeAllExecutor.submit(new Runnable(){
      @Override public void run(){
        cache.closeAll();
      }
    }
);
  }
 catch (  RejectedExecutionException e) {
    m_rate_limited_log.log(System.currentTimeMillis(),Level.WARN,e,"Task to close connections in old connection cache was rejected. Old connections will not be closed");
  }
}
