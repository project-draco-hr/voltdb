def killProcess(p):
    "Kill all processes for this user named 'IntrinsicLockPatternTest'"
    javaprocs = cmd_readlines('jps')
    javaprocs = [line.split() for line in javaprocs]
    javaprocs = [t for t in javaprocs if (len(t) > 1)]
    javaprocs = [int(t[0]) for t in javaprocs if t[1].startswith('IntrinsicLockPatternTest')]
    for pid in javaprocs:
        killcmd = ('kill -9 ' + str(pid))
        os.system(killcmd)
    p.wait()
