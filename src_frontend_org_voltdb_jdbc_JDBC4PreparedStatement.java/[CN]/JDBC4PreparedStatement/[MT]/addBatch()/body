{
  checkClosed();
  if (!this.Query.isOfType(VoltSQL.TYPE_EXEC,VoltSQL.TYPE_INSERT,VoltSQL.TYPE_UPDATE,VoltSQL.TYPE_DELETE))   throw SQLError.get(SQLError.ILLEGAL_STATEMENT,this.Query.toSqlString());
  this.addBatch(this.Query.getExecutableQuery(this.parameters));
  this.parameters=this.Query.getParameterArray();
}
