{
  ProcessBuilder pb=new ProcessBuilder(command);
  if (dir != null) {
    File wd=new File(dir);
    pb.directory(wd);
  }
  pb.redirectErrorStream(true);
  if (passwordScript != null) {
    pb.environment().put("SSH_ASKPASS",passwordScript);
  }
  Process p=null;
  try {
    p=pb.start();
  }
 catch (  IOException e) {
    return null;
  }
  return p;
}
