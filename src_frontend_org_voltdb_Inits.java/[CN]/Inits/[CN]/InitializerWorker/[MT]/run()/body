{
  while (true) {
    InitWork iw=null;
    try {
      iw=m_readyJobs.take();
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
      VoltDB.crashVoltDB();
    }
    if (iw instanceof COMPLETION_WORK)     return;
    hostLog.info("Running InitWorker: " + iw.getClass().getName());
    iw.run();
    completeInitWork(iw);
  }
}
