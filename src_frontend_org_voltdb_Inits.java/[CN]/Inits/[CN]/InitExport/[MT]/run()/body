{
  try {
    ExportManager.initialize(m_rvdb.m_myHostId,m_rvdb.m_catalogContext,m_isRejoin,m_rvdb.m_messenger,m_rvdb.m_partitionsToSitesAtStartupForExportInit);
  }
 catch (  ExportManager.SetupException e) {
    hostLog.l7dlog(Level.FATAL,LogKeys.host_VoltDB_ExportInitFailure.name(),e);
    System.exit(-1);
  }
}
