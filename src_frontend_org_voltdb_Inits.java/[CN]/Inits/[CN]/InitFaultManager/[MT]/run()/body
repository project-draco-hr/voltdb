{
  m_rvdb.m_faultManager=new FaultDistributor(m_rvdb);
  m_rvdb.m_faultManager.registerFaultHandler(NodeFailureFault.NODE_FAILURE_CATALOG,m_rvdb.m_faultHandler,FaultType.NODE_FAILURE);
  if (!m_rvdb.m_faultManager.testPartitionDetectionDirectory(m_rvdb.m_catalogContext.cluster.getFaultsnapshots().get("CLUSTER_PARTITION"))) {
    VoltDB.crashVoltDB();
  }
}
