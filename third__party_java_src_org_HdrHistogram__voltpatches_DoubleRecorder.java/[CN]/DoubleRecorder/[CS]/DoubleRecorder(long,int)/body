{
  activeHistogram=new InternalConcurrentDoubleHistogram(instanceId,highestToLowestValueRatio,numberOfSignificantValueDigits);
  inactiveHistogram=new InternalConcurrentDoubleHistogram(instanceId,highestToLowestValueRatio,numberOfSignificantValueDigits);
  activeHistogram.setStartTimeStamp(System.currentTimeMillis());
}
