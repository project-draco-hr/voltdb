{
  SiteTaskerScheduler sts=new SiteTaskerScheduler();
  Task t1=new Task(0);
  Task t2=new Task(0);
  Task t3=new Task(0);
  sts.offer(t1);
  sts.offer(t2);
  sts.offer(t3);
  assertTrue("Insertion order maintained",sts.poll() == t1);
  assertTrue("Insertion order maintained",sts.poll() == t2);
  assertTrue("Insertion order maintained",sts.poll() == t3);
}
