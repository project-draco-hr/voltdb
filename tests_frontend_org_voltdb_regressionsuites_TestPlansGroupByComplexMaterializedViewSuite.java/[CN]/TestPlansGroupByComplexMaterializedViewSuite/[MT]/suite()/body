{
  VoltServerConfig config=null;
  MultiConfigSuiteBuilder builder=new MultiConfigSuiteBuilder(TestPlansGroupByComplexMaterializedViewSuite.class);
  String literalSchema=null;
  boolean success=true;
  VoltProjectBuilder project=new VoltProjectBuilder();
  literalSchema="CREATE TABLE R1 ( " + "ID INTEGER DEFAULT '0' NOT NULL, " + "WAGE INTEGER, "+ "DEPT INTEGER, "+ "TM TIMESTAMP DEFAULT NULL, "+ "PRIMARY KEY (ID) );"+ "CREATE VIEW V_R1 (V_R1_dept,  V_R1_CNT, V_R1_sum_wage) "+ "AS SELECT dept, count(*), SUM(wage) FROM R1 GROUP BY dept;"+ "";
  try {
    project.addLiteralSchema(literalSchema);
  }
 catch (  IOException e) {
    assertFalse(true);
  }
  config=new LocalCluster("plansgroupby-onesite.jar",1,1,0,BackendTarget.NATIVE_EE_JNI);
  success=config.compile(project);
  assertTrue(success);
  builder.addServerConfig(config);
  return builder;
}
