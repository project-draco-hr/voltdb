{
  Client client=null;
  try {
    client=this.getClient();
  }
 catch (  Exception e) {
    fail();
  }
  String orderbyCol=mvTable + "_G1";
  if (mvTable.endsWith("2")) {
    orderbyCol+="," + mvTable + "_G2";
  }
  VoltTable vt=null;
  try {
    vt=client.callProcedure("@AdHoc","select * from " + mvTable + " ORDER BY "+ orderbyCol).getResults()[0];
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  if (vt.getRowCount() != 0) {
    assertEquals(expected.length,vt.getRowCount());
    compareTable(vt,expected);
  }
 else {
    try {
      vt=client.callProcedure("@AdHoc","select count(*) from " + mvTable).getResults()[0];
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
    assertEquals(1,vt.getRowCount());
    compareTable(vt,new long[][]{{0}});
  }
}
