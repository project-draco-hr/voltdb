def _relaunch():
    log.warn('Relaunching...')
    _cmd1 = ['-c', 'install', '--single-version-externally-managed']
    _cmd2 = ['-c', 'install', '--record']
    if ((sys.argv[:3] == _cmd1) or (sys.argv[:3] == _cmd2)):
        sys.argv[0] = 'setup.py'
    args = ([sys.executable] + sys.argv)
    sys.exit(subprocess.call(args))
