@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_simple(self, jenkins_mock):
    jobs = {u'url': u'http://your_url_here/job/my_job/', u'color': u'blue', u'name': u'my_job', }
    job_info_to_return = {u'jobs': jobs, }
    jenkins_mock.return_value = json.dumps(job_info_to_return)
    job_info = self.j.get_jobs()
    jobs[u'fullname'] = jobs[u'name']
    self.assertEqual(job_info, [jobs])
    self.assertEqual(jenkins_mock.call_args[0][0].get_full_url(), self.make_url('api/json?tree=jobs[url,color,name,jobs]'))
    self._check_requests(jenkins_mock.call_args_list)
