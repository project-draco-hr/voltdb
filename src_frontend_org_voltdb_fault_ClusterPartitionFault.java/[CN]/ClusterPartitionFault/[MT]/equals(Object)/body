{
  if (other instanceof ClusterPartitionFault) {
    ClusterPartitionFault fault=(ClusterPartitionFault)other;
    return getCause().getHostId() == fault.getCause().getHostId();
  }
 else   if (other instanceof NodeFailureFault) {
    NodeFailureFault fault=(NodeFailureFault)other;
    return getCause().getHostId() == fault.getHostId();
  }
  return false;
}
