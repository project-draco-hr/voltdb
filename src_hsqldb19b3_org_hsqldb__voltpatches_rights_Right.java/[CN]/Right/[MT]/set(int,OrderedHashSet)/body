{
switch (type) {
case GrantConstants.SELECT:
    if (set == null) {
      isFullSelect=true;
    }
  selectColumnSet=set;
break;
case GrantConstants.DELETE:
if (set == null) {
isFullDelete=true;
}
break;
case GrantConstants.INSERT:
if (set == null) {
isFullInsert=true;
}
insertColumnSet=set;
break;
case GrantConstants.UPDATE:
if (set == null) {
isFullUpdate=true;
}
updateColumnSet=set;
break;
case GrantConstants.REFERENCES:
if (set == null) {
isFullReferences=true;
}
referencesColumnSet=set;
break;
case GrantConstants.TRIGGER:
if (set == null) {
isFullTrigger=true;
}
triggerColumnSet=set;
break;
}
}
