{
  try {
    className=className.replace('.','/') + ".class";
    URL url=Loader.getResource(null,className,false);
    String s=url.toString();
    if (s.startsWith("jar:file:"))     return new URL(s.substring(4,s.indexOf("!/")));
  }
 catch (  Exception e) {
    Log.ignore(e);
  }
  return null;
}
