{
  if (!user.hasPermission(proc)) {
    authLog.l7dlog(Level.INFO,LogKeys.auth_ClientInterface_LackingPermissionForProcedure.name(),new String[]{user.m_name,invocation.procName},null);
    final ClientResponseImpl errorResponse=new ClientResponseImpl(ClientResponseImpl.UNEXPECTED_FAILURE,new VoltTable[0],"User does not have permission to invoke " + invocation.procName,invocation.clientHandle);
    s.enqueue(errorResponse);
    return false;
  }
  return true;
}
