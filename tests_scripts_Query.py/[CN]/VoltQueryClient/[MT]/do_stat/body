def do_stat(self, command):
    if (self.fs == None):
        return
    if (not command):
        return self.help_stat()
    args = command.split()
    if (len(args) != 2):
        return self.help_stat()
    self.safe_print('Getting statistics')
    self.response = self.stat.call([args[0], int(args[1])], timeout=self.__timeout)
    self.safe_print(self.response)
