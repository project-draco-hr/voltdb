def do_snapshotsave(self, command):
    if (self.fs == None):
        return
    if (not command):
        return self.help_snapshotsave()
    args = command.split()
    if (len(args) != 3):
        return self.help_snapshotsave()
    self.safe_print('Taking snapshot')
    self.response = self.snapshotsave.call([args[0], args[1], int(args[2])], timeout=self.__timeout)
    self.safe_print(self.response)
