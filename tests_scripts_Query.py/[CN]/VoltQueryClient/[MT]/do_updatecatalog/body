def do_updatecatalog(self, command):
    if (self.fs == None):
        return
    if (not command):
        return self.help_updatecatalog()
    args = command.split()
    if (len(args) != 2):
        return self.help_updatecatalog()
    self.safe_print('Updating the application catalog')
    self.response = self.__safe_call(self.updatecatalog, [args[0], args[1]], timeout=self.__timeout)
    self.safe_print(self.response)
