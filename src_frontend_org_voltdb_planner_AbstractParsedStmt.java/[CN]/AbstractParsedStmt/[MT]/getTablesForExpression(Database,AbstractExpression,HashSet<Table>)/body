{
  if (expr.getLeft() != null)   getTablesForExpression(db,expr.getLeft(),tables);
  if (expr.getRight() != null)   getTablesForExpression(db,expr.getRight(),tables);
  if (expr.getExpressionType() == ExpressionType.VALUE_TUPLE) {
    TupleValueExpression tupleExpr=(TupleValueExpression)expr;
    String tableName=tupleExpr.getTableName();
    Table table=db.getTables().getIgnoreCase(tableName);
    tables.add(table);
  }
}
