{
  m_diffMessages.clear();
  boolean noDiff1=diffScans(oldpn,newpn);
  boolean noDiff2=diffInlineAndJoin(oldpn,newpn);
  noDiff1=noDiff1 && noDiff2;
  if (noDiff1) {
    return true;
  }
 else {
    if (print) {
      for (      String msg : m_diffMessages) {
        System.out.println(msg);
      }
    }
    return false;
  }
}
