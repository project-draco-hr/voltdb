{
  numCalls+=1;
  calledName=procName;
  calledParameters=parameters;
  m_lastCallback=callback;
synchronized (m_lock) {
    m_callbacks.add(callback);
  }
  if (originalTxnId <= lastOrigTxnId) {
    origTxnIdOrderCorrect=false;
  }
 else {
    lastOrigTxnId=originalTxnId;
  }
  return m_nextReturn;
}
