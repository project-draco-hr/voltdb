{
  numCalls+=1;
  calledName=procName;
  calledParameters=parameters;
  if (abortMessage != null) {
    ProcCallException e=new ProcCallException(abortMessage,null);
    abortMessage=null;
    throw e;
  }
  VoltTable[] result=nextResult;
  if (resetAfterCall)   nextResult=null;
  return result;
}
