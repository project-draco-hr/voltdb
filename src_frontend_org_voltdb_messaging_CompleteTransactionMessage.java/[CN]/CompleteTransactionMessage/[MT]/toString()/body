{
  StringBuilder sb=new StringBuilder();
  sb.append("COMPLETE_TRANSACTION (FROM COORD: ");
  sb.append(CoreUtils.hsIdToString(m_coordinatorHSId));
  sb.append(") FOR TXN ");
  sb.append(m_txnId);
  if (m_isRollback)   sb.append("\n  THIS IS AN ROLLBACK REQUEST");
  if (m_requiresAck)   sb.append("\n  THIS MESSAGE REQUIRES AN ACK");
  if (m_rollbackForFault) {
    sb.append("\n  THIS ROLLBACK IS FOR FAULT REPAIR");
  }
  return sb.toString();
}
