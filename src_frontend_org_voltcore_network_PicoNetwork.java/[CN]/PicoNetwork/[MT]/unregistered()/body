{
  try {
    if (!m_alreadyStopped) {
      m_alreadyStopped=true;
      try {
        safeStopping();
      }
  finally {
        try {
          m_writeStream.shutdown();
        }
  finally {
          m_readStream.shutdown();
        }
      }
    }
  }
  finally {
    networkLog.debug("Closing channel " + m_toString);
    try {
      m_sc.close();
    }
 catch (    IOException e) {
      networkLog.warn(e);
    }
  }
}
