{
  Tester tester=new Tester();
  tester.bad(".*no semicolon found.*",new DDL("REPLICATE TABLE books"));
  tester.bad(".*Bad REPLICATE DDL statement.*",new DDL("REPLICATE TABLE;"));
  tester.bad(".*Bad REPLICATE DDL statement.*",new DDL("REPLICATE TABLE books NOW;"));
  tester.bad(".*Bad REPLICATE DDL statement.*",new DDL("REPLICATE TABLEX books;"));
  tester.bad(".*Bad indentifier in DDL.*",new DDL("REPLICATE TABLE 0books;"));
}
