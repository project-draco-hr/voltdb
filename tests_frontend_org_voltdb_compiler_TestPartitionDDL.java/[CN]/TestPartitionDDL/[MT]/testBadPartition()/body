{
  Tester tester=new Tester();
  tester.bad(".*no semicolon found.*",new DDL("PARTITION TABLE books ON COLUMN cash"));
  tester.bad(".*Bad PARTITION DDL statement.*",new DDL("PARTITION TABLE;"));
  tester.bad(".*Bad PARTITION DDL statement.*",new DDL("PARTITION TABLE books ON COLUMN cash COW;"));
  tester.bad(".*Bad PARTITION DDL statement.*",new DDL("PARTITION TABLE books PROCEDURE bruha ON COLUMN cash;"));
  tester.bad(".*Bad PARTITION DDL statement.*",new DDL("PARTITION TABLEX books ON COLUMN cash;"));
}
