{
  final String responseNode="/user_snapshot_response_" + requestId;
  Stat exists=m_zk.exists(responseNode,new Watcher(){
    @Override public void process(    final WatchedEvent event){
      if (event.getState() == KeeperState.Disconnected)       return;
switch (event.getType()) {
case NodeCreated:
        m_es.submit(new Runnable(){
          @Override public void run(){
            try {
              processUserSnapshotRequestResponse(event,invocation,c);
            }
 catch (            Exception e) {
              VoltDB.crashLocalVoltDB("Error retrieving user snapshot request response from ZK",true,e);
            }
          }
        }
);
      break;
default :
  }
}
}
);
if (exists != null) {
processUserSnapshotRequestResponse(new WatchedEvent(EventType.NodeCreated,KeeperState.SyncConnected,responseNode),invocation,c);
}
}
