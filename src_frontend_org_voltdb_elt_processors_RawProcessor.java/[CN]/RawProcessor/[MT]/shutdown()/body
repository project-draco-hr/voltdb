{
  m_shouldContinue.set(false);
  m_acceptor.interrupt();
  this.interrupt();
  try {
    System.out.println("Joining acceptor.");
    m_acceptor.join();
  }
 catch (  InterruptedException e) {
  }
  try {
    System.out.println("Joining Processor.");
    this.join();
  }
 catch (  InterruptedException e) {
  }
}
