{
  if (!m_connectionAccepted) {
    m_initializationLock.acquire();
  }
  m_connectionAccepted=true;
  if (m_in == null || m_ack == null) {
    return null;
  }
  Pair<Integer,ByteBuffer> result=null;
  while (!m_EOF) {
    BBContainer container=m_in.take();
    result=processMessage(container);
    if (result != null) {
      break;
    }
  }
  return result;
}
