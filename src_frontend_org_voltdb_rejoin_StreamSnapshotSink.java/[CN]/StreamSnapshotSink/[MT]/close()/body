{
  if (m_in != null) {
    m_in.close();
  }
  if (m_ack != null) {
    m_ack.close();
    try {
      m_ackThread.join();
    }
 catch (    InterruptedException e) {
    }
  }
  m_in=null;
  m_ack=null;
  if (m_mb != null) {
    VoltDB.instance().getHostMessenger().removeMailbox(m_mb.getHSId());
  }
}
