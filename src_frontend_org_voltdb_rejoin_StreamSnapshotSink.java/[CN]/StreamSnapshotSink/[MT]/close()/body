{
  m_acceptThread.interrupt();
  try {
    m_acceptThread.join();
  }
 catch (  InterruptedException e1) {
  }
  if (m_in != null) {
    m_in.close();
  }
  if (m_ack != null) {
    m_ack.close();
    try {
      m_ackThread.join();
    }
 catch (    InterruptedException e) {
    }
  }
  m_in=null;
  m_ack=null;
  try {
    m_sock.close();
    m_serverSocket.close();
  }
 catch (  IOException e) {
    rejoinLog.error("Failed to close sockets used for rejoin: " + e.getMessage());
  }
}
