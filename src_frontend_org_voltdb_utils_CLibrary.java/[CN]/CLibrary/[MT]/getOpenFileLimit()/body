{
  try {
    Rlimit rlimit=new Rlimit();
    int retval=getrlimit(RLIMIT_NOFILE,rlimit);
    if (retval != 0) {
      return null;
    }
 else     if (rlimit.rlim_cur >= 1024) {
      return (int)rlimit.rlim_cur;
    }
 else {
      return null;
    }
  }
 catch (  Exception e) {
    hostLog.warn("Failed to retrieve open file limit via JNA",e);
  }
  return null;
}
