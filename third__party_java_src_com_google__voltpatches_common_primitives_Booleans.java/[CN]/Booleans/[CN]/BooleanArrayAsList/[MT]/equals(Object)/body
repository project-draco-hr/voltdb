{
  if (object == this) {
    return true;
  }
  if (object instanceof BooleanArrayAsList) {
    BooleanArrayAsList that=(BooleanArrayAsList)object;
    int size=size();
    if (that.size() != size) {
      return false;
    }
    for (int i=0; i < size; i++) {
      if (array[start + i] != that.array[that.start + i]) {
        return false;
      }
    }
    return true;
  }
  return super.equals(object);
}
