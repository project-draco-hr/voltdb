{
  Client client=getClient();
  try {
    client.callProcedure("SinglePartitionJavaError",2);
    fail();
  }
 catch (  ProcCallException e) {
  }
catch (  IOException e) {
    e.printStackTrace();
    fail();
  }
  try {
    VoltTable[] results=client.callProcedure("SelectAll");
    assertEquals(results.length,9);
    VoltTable table=results[7];
    assertTrue(table.getRowCount() == 0);
    results=client.callProcedure("ReadMatView",2);
    assertEquals(results.length,1);
    assertTrue(results[0].getRowCount() == 0);
  }
 catch (  ProcCallException e) {
    e.printStackTrace();
    fail();
  }
catch (  IOException e) {
    e.printStackTrace();
    fail();
  }
}
