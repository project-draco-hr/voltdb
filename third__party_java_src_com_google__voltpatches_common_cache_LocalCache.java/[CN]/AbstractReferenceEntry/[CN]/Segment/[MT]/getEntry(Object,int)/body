{
  for (ReferenceEntry<K,V> e=getFirst(hash); e != null; e=e.getNext()) {
    if (e.getHash() != hash) {
      continue;
    }
    K entryKey=e.getKey();
    if (entryKey == null) {
      tryDrainReferenceQueues();
      continue;
    }
    if (map.keyEquivalence.equivalent(key,entryKey)) {
      return e;
    }
  }
  return null;
}
