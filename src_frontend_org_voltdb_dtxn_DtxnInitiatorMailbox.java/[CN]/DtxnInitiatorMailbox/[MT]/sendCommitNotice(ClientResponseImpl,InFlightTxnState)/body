{
  VoltDB.instance().scheduleWork(new Runnable(){
    @Override public void run(){
      CompleteTransactionMessage ft=new CompleteTransactionMessage(getHSId(),state.firstCoordinatorId,state.txnId,state.isReadOnly,toSend.getStatus() != ClientResponse.SUCCESS,false);
      send(state.otherSiteIds,ft);
    }
  }
,0,-1,TimeUnit.SECONDS);
}
