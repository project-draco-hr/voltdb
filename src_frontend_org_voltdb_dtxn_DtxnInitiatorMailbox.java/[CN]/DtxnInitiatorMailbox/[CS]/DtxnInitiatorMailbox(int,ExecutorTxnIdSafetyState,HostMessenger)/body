{
  assert(safetyState != null);
  assert(hostMessenger != null);
  m_hostMessenger=hostMessenger;
  m_siteId=siteId;
  m_safetyState=safetyState;
  m_stats=new InitiatorStats("Initiator " + siteId + " stats",siteId);
  VoltDB.instance().getFaultDistributor().registerFaultHandler(NodeFailureFault.NODE_FAILURE_INITIATOR,new InitiatorNodeFailureFaultHandler(),FaultType.NODE_FAILURE);
}
