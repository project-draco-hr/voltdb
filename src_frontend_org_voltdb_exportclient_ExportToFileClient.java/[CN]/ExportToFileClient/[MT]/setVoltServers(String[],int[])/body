{
  if (ports == null) {
    ports=new int[voltServers.length];
    for (int i=0; i < voltServers.length; i++)     ports[i]=VoltDB.DEFAULT_PORT;
  }
  ArrayList<InetSocketAddress> servers=new ArrayList<InetSocketAddress>();
  for (int i=0; i < voltServers.length; i++) {
    InetSocketAddress server=new InetSocketAddress(voltServers[i],ports[i]);
    servers.add(server);
  }
  setServerInfo(servers);
}
