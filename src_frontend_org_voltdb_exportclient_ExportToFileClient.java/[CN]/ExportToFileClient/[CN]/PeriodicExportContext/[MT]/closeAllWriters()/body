{
  if (m_hasClosed)   return;
  for (  Entry<FileHandle,CSVWriter> entry : m_writers.entrySet()) {
    CSVWriter writer=entry.getValue();
    if (writer == null)     continue;
    try {
      writer.flush();
      writer.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
  if (m_batched)   closeBatch();
 else   closeFiles();
  m_writers.clear();
  m_hasClosed=true;
}
