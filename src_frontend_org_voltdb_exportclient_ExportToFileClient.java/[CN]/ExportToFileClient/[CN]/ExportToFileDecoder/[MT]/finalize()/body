{
  try {
    if (m_writer != null) {
      m_writer.flush();
      m_writer.close();
      m_writer=null;
    }
  }
 catch (  Exception e) {
    m_logger.error(e.getMessage());
    throw new RuntimeException();
  }
 finally {
    if (m_context != null)     m_context.decref(this);
  }
  super.finalize();
}
