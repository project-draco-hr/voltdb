{
  m_batchLock.writeLock().lock();
  try {
    HashMap<String,ExportToFileDecoder> decoders=m_tableDecoders.get(m_generation);
    if (decoders != null) {
      decoders.remove(m_tableName);
      if (decoders.isEmpty()) {
        m_tableDecoders.remove(m_generation);
      }
    }
  }
  finally {
    m_batchLock.writeLock().unlock();
  }
}
