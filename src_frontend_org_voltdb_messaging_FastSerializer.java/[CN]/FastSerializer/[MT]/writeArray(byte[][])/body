{
  if (values.length > VoltType.MAX_VALUE_LENGTH) {
    throw new IOException("Array exceeds maximum length of " + VoltType.MAX_VALUE_LENGTH + " bytes");
  }
  writeShort(values.length);
  for (int i=0; i < values.length; ++i) {
    if (values[i] == null) {
      writeInt(-1);
    }
 else {
      writeArray(values[i]);
    }
  }
}
