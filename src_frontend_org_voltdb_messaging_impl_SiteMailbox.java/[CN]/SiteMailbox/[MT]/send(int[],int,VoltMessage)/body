{
  assert(message != null);
  assert(siteIds != null);
  m_hostMessenger.send(siteIds,mailboxId,message);
  if (message instanceof Heartbeat)   return;
synchronized (m_lastTenSentMessages) {
    m_lastTenSentMessages.addLast(message);
    if (m_lastTenSentMessages.size() > MESSAGE_HISTORY_SIZE)     m_lastTenSentMessages.pollFirst();
  }
}
