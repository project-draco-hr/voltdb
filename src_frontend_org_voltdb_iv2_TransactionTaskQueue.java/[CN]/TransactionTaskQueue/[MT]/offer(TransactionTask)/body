{
  Iv2Trace.logTransactionTaskQueueOffer(task);
  boolean retval=false;
  if (!m_backlog.isEmpty()) {
    if (task.getTxnId() != m_backlog.getFirst().getTxnId()) {
      m_backlog.addLast(task);
      retval=true;
    }
 else {
      taskQueueOffer(task);
    }
  }
 else {
    if (!task.getTransactionState().isSinglePartition()) {
      m_backlog.addLast(task);
      retval=true;
    }
    taskQueueOffer(task);
  }
  return retval;
}
