{
  long logLevels=0;
  for (int ii=0; ii < loggers.length; ii++) {
    final int level=loggers[ii].getEffectiveLevel().toInt();
switch (level) {
case Level.TRACE_INT:
      logLevels|=trace << (ii * 3);
    break;
case Level.ALL_INT:
  logLevels|=all << (ii * 3);
break;
case Level.DEBUG_INT:
logLevels|=debug << (ii * 3);
break;
case Level.ERROR_INT:
logLevels|=error << (ii * 3);
break;
case Level.FATAL_INT:
logLevels|=fatal << (ii * 3);
break;
case Level.INFO_INT:
logLevels|=info << (ii * 3);
break;
case Level.OFF_INT:
logLevels|=off << (ii * 3);
break;
case Level.WARN_INT:
logLevels|=warn << (ii * 3);
break;
default :
throw new RuntimeException("Unhandled log level " + level);
}
}
return logLevels;
}
