{
  Pair<Integer,ByteBuffer> dataPair=m_snapshotData.poll();
  if (dataPair != null) {
    JOINLOG.debug("P" + m_partitionId + " restoring table "+ dataPair.getFirst()+ " block of ("+ dataPair.getSecond().position()+ ","+ dataPair.getSecond().limit()+ ")");
    restoreBlock(dataPair,siteConnection);
  }
}
