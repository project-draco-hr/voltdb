{
  Pair<Integer,ByteBuffer> dataPair=m_snapshotData.poll();
  if (dataPair != null) {
    if (JOINLOG.isTraceEnabled()) {
      JOINLOG.trace(m_whoami + "restoring replicated table " + dataPair.getFirst()+ " block of ("+ dataPair.getSecond().position()+ ","+ dataPair.getSecond().limit()+ ")");
    }
    restoreBlock(dataPair,siteConnection);
  }
}
