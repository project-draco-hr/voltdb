{
  int eval=2;
  if (m4_process != null) {
    try {
      m4_process.destroy();
      Thread.sleep(100);
    }
 catch (    InterruptedException ex) {
      Logger.getLogger(NCProcess.class.getName()).log(Level.SEVERE,null,ex);
    }
  }
  if (m6_process != null) {
    try {
      m6_process.destroy();
      Thread.sleep(100);
    }
 catch (    InterruptedException ex) {
      Logger.getLogger(NCProcess.class.getName()).log(Level.SEVERE,null,ex);
    }
  }
  return eval;
}
