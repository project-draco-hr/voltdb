{
  assert(parentNode.getLeftNode() != null);
  JoinNode outerChildNode=parentNode.getLeftNode();
  List<AbstractExpression> joinOuterList=(parentNode.getJoinType() == JoinType.INNER) ? parentNode.m_joinOuterList : null;
  if (outerChildNode.getNodeType() == JoinNode.NodeType.JOIN) {
    generateOuterAccessPaths(outerChildNode);
    generateInnerAccessPaths(outerChildNode);
    outerChildNode.m_accessPaths.add(getRelevantNaivePath(joinOuterList,parentNode.m_whereOuterList));
  }
 else   if (outerChildNode.getNodeType() == JoinNode.NodeType.LEAF || outerChildNode.getNodeType() == JoinNode.NodeType.SUBQUERY) {
    outerChildNode.m_accessPaths.addAll(getRelevantAccessPathsForTable(outerChildNode.getTableAliasIndex(),joinOuterList,parentNode.m_whereOuterList,null));
  }
 else {
    assert(false);
  }
  assert(outerChildNode.m_accessPaths.size() > 0);
}
