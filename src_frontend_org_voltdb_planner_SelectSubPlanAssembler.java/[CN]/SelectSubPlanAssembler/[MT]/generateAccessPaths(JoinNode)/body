{
  assert(joinNode != null);
  if (joinNode.getNodeType() == JoinNode.NodeType.LEAF || joinNode.getNodeType() == JoinNode.NodeType.SUBQUERY) {
    joinNode.m_accessPaths.addAll(getRelevantAccessPathsForTable(joinNode.getTableAliasIndex(),joinNode.m_joinInnerList,joinNode.m_whereInnerList,null));
  }
 else   if (joinNode.getNodeType() == JoinNode.NodeType.JOIN) {
    generateOuterAccessPaths(joinNode);
    generateInnerAccessPaths(joinNode);
    joinNode.m_accessPaths.add(new AccessPath());
  }
 else {
    assert(false);
  }
}
