{
  assert(joinNode != null);
  if (joinNode.m_tableAliasIndex != StmtCatalogCache.NULL_ALIAS_INDEX) {
    joinNode.m_accessPaths.addAll(getRelevantAccessPathsForTable(joinNode.m_tableAliasIndex,joinNode.m_joinInnerList,joinNode.m_whereInnerList,null));
  }
 else {
    assert(joinNode.m_leftNode != null && joinNode.m_rightNode != null);
    generateOuterAccessPaths(joinNode);
    generateInnerAccessPaths(joinNode);
    joinNode.m_accessPaths.add(new AccessPath());
  }
}
