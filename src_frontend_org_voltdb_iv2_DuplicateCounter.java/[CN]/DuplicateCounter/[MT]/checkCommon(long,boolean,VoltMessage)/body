{
  if (!rejoining) {
    if (m_responseHash == null) {
      m_responseHash=Long.valueOf(hash);
    }
 else     if (!m_responseHash.equals(hash)) {
      String msg=String.format("HASH MISMATCH COMPARING: %d to %d\n" + "PREV MESSAGE: %s\n" + "CURR MESSAGE: %s\n",hash,m_responseHash,m_lastResponse.toString(),message.toString());
      tmLog.error(msg);
      return MISMATCH;
    }
    m_lastResponse=message;
  }
  if (m_lastResponse == null) {
    m_lastResponse=message;
  }
  m_expectedHSIds.remove(message.m_sourceHSId);
  if (m_expectedHSIds.size() == 0) {
    return DONE;
  }
 else {
    return WAITING;
  }
}
