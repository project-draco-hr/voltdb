{
  if (m_responseHash == null) {
    m_responseHash=Long.valueOf(hash);
  }
 else   if (!m_responseHash.equals(hash)) {
    System.out.printf("COMPARING: %d to %d\n",hash,m_responseHash);
    return MISMATCH;
  }
  if (m_expectedResponses.decrementAndGet() == 0) {
    return DONE;
  }
 else {
    return WAITING;
  }
}
