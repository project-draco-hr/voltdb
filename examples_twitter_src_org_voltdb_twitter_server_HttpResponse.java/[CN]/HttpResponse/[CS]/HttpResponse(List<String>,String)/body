{
  this.status=new HttpStatus(200);
  this.contentType=new HttpContentType(contentType);
  this.payload=new ByteArrayOutputStream();
  for (  String line : data) {
    try {
      payload.write(line.getBytes());
      payload.write('\n');
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
