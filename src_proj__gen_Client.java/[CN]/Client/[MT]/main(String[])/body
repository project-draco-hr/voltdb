{
  System.out.println("Client started");
  final ClientConfig clientConfig=new ClientConfig("program","none");
  final org.voltdb.client.Client voltclient=org.voltdb.client.ClientFactory.createClient(clientConfig);
  try {
    voltclient.createConnection("localhost");
  }
 catch (  IOException e) {
    e.printStackTrace();
    System.exit(-1);
  }
  final Client client=new Client(voltclient);
  client.doInsert(1,2);
  client.doInsert(3,4);
  client.doInsert(5,6);
  client.doSelect(5);
  client.doSelect(6);
  client.doSelect(3);
  client.doDelete(5);
  client.doSelect(5);
  client.tryInvalidInsert(10);
  System.out.println("Client finished");
}
