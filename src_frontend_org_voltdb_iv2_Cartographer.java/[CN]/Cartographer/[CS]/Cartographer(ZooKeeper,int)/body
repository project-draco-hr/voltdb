{
  super(false);
  m_numberOfPartitions=numberOfPartitions;
  m_zk=zk;
  m_iv2Masters=new MapCache(m_zk,VoltZK.iv2masters);
  m_iv2Mpi=new MapCache(m_zk,VoltZK.iv2mpi);
  try {
    m_iv2Masters.start(true);
    m_iv2Mpi.start(true);
  }
 catch (  Exception e) {
    VoltDB.crashLocalVoltDB("Screwed",true,e);
  }
}
