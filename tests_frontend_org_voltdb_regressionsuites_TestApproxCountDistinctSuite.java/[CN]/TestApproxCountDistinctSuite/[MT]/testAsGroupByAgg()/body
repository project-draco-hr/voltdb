{
  Client client=getClient();
  fillTable(client,"p");
  fillTable(client,"r");
  compareEstimateAndExact(client,TABLE_NAMES,COLUMN_NAMES,"select bitand(cast(pk as bigint), x'03') as pk_lobits, %s %s ) " + "from %s " + "group by pk_lobits "+ "order by pk_lobits");
  compareEstimateAndExact(client,TABLE_NAMES,COLUMN_NAMES,"select " + "  bitand(cast(pk as bigint), x'03') as pk_lobits, " + "  count(distinct bi), "+ "  %s %s ) "+ "from %s "+ "group by pk_lobits "+ "order by pk_lobits");
  compareEstimateAndExact(client,TABLE_NAMES,COLUMN_NAMES,"select " + "  bitand(cast(pk as bigint), x'03') as pk_lobits, " + "  count(dd), "+ "  %s %s ) "+ "from %s "+ "group by pk_lobits "+ "order by pk_lobits");
}
