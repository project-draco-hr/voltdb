{
  JSONStringer js=new JSONStringer();
  try {
    js.array();
    for (    Object o : m_params) {
      js.value(o);
    }
    js.endArray();
  }
 catch (  JSONException e) {
    e.printStackTrace();
    throw new RuntimeException("Failed to serialize a parameter set to JSON.",e);
  }
  return js.toString();
}
