{
  node.overrideId(nextId++);
  for (  AbstractPlanNode inNode : node.getInlinePlanNodes().values()) {
    inNode.overrideId(0);
  }
  for (int i=0; i < node.getChildCount(); i++) {
    AbstractPlanNode child=node.getChild(i);
    assert(child != null);
    nextId=resetPlanNodeIds(child,nextId);
  }
  return nextId;
}
