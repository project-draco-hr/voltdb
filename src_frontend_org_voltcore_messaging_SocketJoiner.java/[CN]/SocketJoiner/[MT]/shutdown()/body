{
  if (m_selector != null) {
    try {
      m_selector.close();
    }
 catch (    Exception e) {
    }
  }
  m_es.shutdownNow();
  m_es.awaitTermination(356,TimeUnit.DAYS);
  for (  ServerSocketChannel ssc : m_listenerSockets) {
    try {
      ssc.close();
    }
 catch (    Exception e) {
    }
  }
  m_listenerSockets.clear();
  if (m_selector != null) {
    try {
      m_selector.close();
    }
 catch (    Exception e) {
    }
    m_selector=null;
  }
}
