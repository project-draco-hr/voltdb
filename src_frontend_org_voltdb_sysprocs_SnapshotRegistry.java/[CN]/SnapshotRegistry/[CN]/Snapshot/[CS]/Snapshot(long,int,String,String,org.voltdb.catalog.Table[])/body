{
  timeStarted=startTime;
  this.path=path;
  this.nonce=nonce;
  timeFinished=0;
synchronized (this.tables) {
    for (    org.voltdb.catalog.Table table : tables) {
      String filename=SnapshotUtil.constructFilenameForTable(table,nonce,Integer.toString(hostId));
      this.tables.put(table.getTypeName(),new Table(table.getTypeName(),filename,startTime));
    }
  }
  result=false;
}
