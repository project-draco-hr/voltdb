{
  this.txnId=txnId;
  this.path=path;
  this.nonce=nonce;
  this.csv=csv;
  timeFinished=0;
synchronized (this.tables) {
    for (    org.voltdb.catalog.Table table : tables) {
      String filename=SnapshotUtil.constructFilenameForTable(table,nonce,csv ? ".csv" : ".vpt",hostId);
      this.tables.put(table.getTypeName(),new Table(table.getTypeName(),filename));
    }
  }
  result=false;
  bytesWritten=0;
}
