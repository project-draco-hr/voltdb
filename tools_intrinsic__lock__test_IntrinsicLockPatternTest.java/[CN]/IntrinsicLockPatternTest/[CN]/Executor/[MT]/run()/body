{
  try {
    while (true) {
      Runnable nextTask=null;
synchronized (m_tasks) {
        nextTask=m_tasks.poll();
        while (nextTask == null) {
          m_tasks.wait();
          nextTask=m_tasks.poll();
        }
      }
      if (nextTask == null) {
        return;
      }
      nextTask.run();
    }
  }
 catch (  InterruptedException e) {
  }
}
