{
  try {
    try {
      m_handler.stopped(this);
    }
  finally {
      try {
        m_writeStream.shutdown();
      }
  finally {
        m_readStream.shutdown();
      }
    }
  }
  finally {
    networkLog.info("Closing channel " + m_toString);
    try {
      m_channel.close();
    }
 catch (    IOException e) {
      networkLog.warn(e);
    }
  }
}
