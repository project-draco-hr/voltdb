{
  Type[] paramTypes=m_def.getParamTypes();
  for (int i=0; i < nodes.length; i++) {
    if (nodes[i] != null) {
      nodes[i].resolveTypes(session,this);
    }
  }
switch (funcType) {
case FunctionId.FUNC_VOLT_FIELD:
    if (nodes[0].dataType == null && nodes[0].isParam) {
      nodes[0].dataType=Type.SQL_VARCHAR;
    }
  if (nodes[1].dataType == null && nodes[1].isParam) {
    nodes[1].dataType=Type.SQL_VARCHAR;
  }
break;
case FunctionId.FUNC_VOLT_DECODE:
Type inferredInputType=null;
Type inferredResultType=null;
boolean needParamType=false;
for (int ii=0; ii < nodes.length; ii++) {
if (ii >= 2 && (ii == (nodes.length - 1) || (ii % 2) == 0)) {
if (nodes[ii].dataType == null) {
if (nodes[ii].isParam) {
needParamType=true;
}
}
 else if (inferredResultType == null) {
inferredResultType=nodes[ii].dataType;
}
 else if (inferredResultType != nodes[ii].dataType) {
inferredResultType=Type.SQL_VARCHAR;
}
}
 else {
if (nodes[ii].dataType == null) {
if (nodes[ii].isParam) {
needParamType=true;
}
}
 else if (inferredInputType == null) {
inferredInputType=nodes[ii].dataType;
}
 else if (inferredInputType != nodes[ii].dataType) {
inferredInputType=Type.SQL_VARCHAR;
}
}
}
if (!needParamType) {
break;
}
for (int ii=0; ii < nodes.length; ii++) {
if (nodes[ii].dataType != null) {
continue;
}
if (!nodes[ii].isParam) {
continue;
}
if (ii >= 2 && (ii == (nodes.length - 1) || (ii % 2) == 0)) {
if (inferredResultType == null) {
nodes[ii].dataType=Type.SQL_VARCHAR;
}
 else {
nodes[ii].dataType=inferredResultType;
}
}
 else {
if (inferredInputType == null) {
nodes[ii].dataType=Type.SQL_VARCHAR;
}
 else {
nodes[ii].dataType=inferredInputType;
}
}
}
break;
default :
break;
}
for (int i=0; i < nodes.length; i++) {
if (nodes[i] != null) {
if (i >= paramTypes.length) {
break;
}
if (paramTypes[i] == null) {
continue;
}
if (paramTypes[i].canConvertFrom(nodes[i].dataType)) {
continue;
}
throw Error.error(ErrorCode.X_42565);
}
}
dataType=m_def.getDataType();
if (dataType == null && nodes.length > 0) {
if (parameterArg < 0 || parameterArg >= nodes.length) {
throw Error.error(ErrorCode.X_42565);
}
Expression like_child=nodes[parameterArg];
if (like_child != null) {
dataType=like_child.dataType;
}
}
}
