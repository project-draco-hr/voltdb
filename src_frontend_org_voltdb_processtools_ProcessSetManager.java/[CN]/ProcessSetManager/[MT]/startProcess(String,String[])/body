{
  ProcessData pd;
  try {
    pd=new ProcessData(processName,cmd,null,m_handler);
synchronized (createdProcesses) {
      createdProcesses.add(pd);
    }
    assert(m_processes.containsKey(processName) == false);
    m_processes.put(processName,pd);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
