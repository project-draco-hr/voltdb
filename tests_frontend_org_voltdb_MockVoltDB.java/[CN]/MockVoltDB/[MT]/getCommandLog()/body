{
  return new CommandLog(){
    @Override public void shutdown() throws InterruptedException {
    }
    @Override public void init(    CatalogContext context){
    }
    @Override public void log(    InitiateTaskMessage message){
    }
    @Override public Semaphore logFault(    Set<Integer> failedSites,    List<Long> faultedTxns){
      return new Semaphore(1);
    }
  }
;
}
