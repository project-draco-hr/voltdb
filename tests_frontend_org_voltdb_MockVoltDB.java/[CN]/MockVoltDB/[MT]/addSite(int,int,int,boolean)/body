{
  m_context.catalog.execute("add " + m_context.cluster.getPath() + " sites "+ siteId);
  m_context.catalog.execute("set " + getSite(siteId).getPath() + " host "+ getHost(hostId).getPath());
  m_context.catalog.execute("set " + getSite(siteId).getPath() + " isexec "+ isExec);
  String partition_path="null";
  if (isExec) {
    partition_path=getPartition(partitionId).getPath();
    m_execSiteCount++;
  }
  m_context.catalog.execute("set " + getSite(siteId).getPath() + " partition "+ partition_path);
  m_context=new CatalogContext(m_context.catalog,CatalogContext.NO_PATH);
}
