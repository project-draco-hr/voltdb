{
  Iterator itor=_listeners.iterator();
  while (itor.hasNext()) {
    Object l=itor.next();
    try {
      if (l instanceof BulkListener)       ((BulkListener)l).filesChanged(filenames);
    }
 catch (    Exception e) {
      warn(l,filenames.toString(),e);
    }
catch (    Error e) {
      warn(l,filenames.toString(),e);
    }
  }
}
