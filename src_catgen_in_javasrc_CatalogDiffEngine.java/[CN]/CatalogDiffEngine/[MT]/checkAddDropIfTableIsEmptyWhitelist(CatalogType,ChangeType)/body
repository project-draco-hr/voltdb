{
  String[] retval=new String[2];
  if (suspect instanceof Index) {
    Index idx=(Index)suspect;
    assert(idx.getUnique());
    retval[0]=suspect.getParent().getTypeName();
    retval[1]=String.format("Unable to add unique index %s because table %s is not empty.",suspect.getTypeName(),retval[0]);
    return retval;
  }
  if ((suspect instanceof ColumnRef) && (suspect.getParent() instanceof Index)) {
    Index idx=(Index)suspect.getParent();
    assert(idx.getUnique());
    assert(changeType == ChangeType.DELETION);
    retval[0]=suspect.getParent().getTypeName();
    retval[1]=String.format("Unable to restrict unique index %s because table %s is not empty.",suspect.getParent().getTypeName(),retval[0]);
    return retval;
  }
  return null;
}
