{
  StringBuilder sb=new StringBuilder();
  if (m_isHeartBeat)   sb.append("HEARTBEAT_NOTICE (FROM ");
 else   sb.append("MEMBERSHIP_NOTICE (FROM ");
  sb.append(m_initiatorSiteId);
  sb.append(" TO ");
  sb.append(receivedFromSiteId);
  sb.append(") FOR TXN ");
  sb.append(m_txnId);
  if (!m_isHeartBeat) {
    sb.append("\n");
    if (m_isReadOnly)     sb.append("  READ, COORD ");
 else     sb.append("  WRITE, COORD ");
    sb.append(m_coordinatorSiteId);
  }
  return sb.toString();
}
