{
  Client client=getClient();
  populateTables(client);
  VoltTable vt=client.callProcedure("@AdHoc","select places.name, LATITUDE(places.loc), LONGITUDE(places.loc) " + "from places " + "order by places.pk").getResults()[0];
  assertContentOfTable(new Object[][]{{"Denver",39.70399856567383,-104.95899963378906},{"Albuquerque",35.112998962402344,-106.5989990234375},{"Cheyenne",41.13399887084961,-104.81300354003906},{"Fort Collins",40.584999084472656,-105.0770034790039},{"Neverwhere",360.0,360.0}},vt);
  vt=client.callProcedure("@AdHoc","select places.name, LATITUDE(places.loc), LONGITUDE(places.loc) " + "from places, borders " + "where contains(borders.region, places.loc) "+ "group by places.name, places.loc "+ "order by places.name").getResults()[0];
  assertContentOfTable(new Object[][]{{"Cheyenne",41.13399887084961,-104.81300354003906},{"Denver",39.70399856567383,-104.95899963378906},{"Fort Collins",40.584999084472656,-105.0770034790039}},vt);
}
