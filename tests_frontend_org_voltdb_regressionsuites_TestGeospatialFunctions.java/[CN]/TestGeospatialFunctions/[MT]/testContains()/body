{
  Client client=getClient();
  populateTables(client);
  VoltTable vt=client.callProcedure("@AdHoc","select places.name || ', ' || borders.name " + "from places, borders " + "where contains(borders.region, places.loc) "+ "order by places.pk, borders.pk").getResults()[0];
  assertContentOfTable(new Object[][]{{"Denver, Colorado"},{"Cheyenne, Wyoming"},{"Fort Collins, Colorado"},{"Fort Collins, Colorado with a hole around Denver"}},vt);
}
