{
  long mirrorRowCount=0;
  try {
    VoltTable[] countQueryResult=client.callProcedure("CountMirror").getResults();
    mirrorRowCount=countQueryResult[0].asScalarLong();
  }
 catch (  Exception e) {
    log.error("Exception from callProcedure",e);
    System.exit(-1);
  }
  return mirrorRowCount;
}
