{
  long importRowCount=0;
  try {
    VoltTable[] countQueryResult=client.callProcedure("CountImport").getResults();
    importRowCount=countQueryResult[0].asScalarLong();
  }
 catch (  Exception e) {
    log.error("Exception from callProcedure",e);
    System.exit(-1);
  }
  return importRowCount;
}
