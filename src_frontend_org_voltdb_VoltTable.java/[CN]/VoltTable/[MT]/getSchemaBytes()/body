{
  if (getRowCount() > 0) {
    throw new RuntimeException("getSchemaBytes() Only works if the table is empty");
  }
  ByteBuffer dup=m_buffer.duplicate();
  dup.limit(dup.limit() - 4);
  dup.position(0);
  byte retvalBytes[]=new byte[dup.remaining()];
  dup.get(retvalBytes);
  return retvalBytes;
}
