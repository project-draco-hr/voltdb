{
  String tableAlias=null;
  if (m_left != null) {
    tableAlias=m_left.baseTableAlias();
    if (tableAlias != null)     return tableAlias;
  }
  if (m_right != null) {
    tableAlias=m_right.baseTableAlias();
    if (tableAlias != null)     return tableAlias;
  }
  if (m_args != null) {
    for (    AbstractExpression expr : m_args) {
      tableAlias=expr.baseTableAlias();
      if (tableAlias != null)       return tableAlias;
    }
  }
  return null;
}
