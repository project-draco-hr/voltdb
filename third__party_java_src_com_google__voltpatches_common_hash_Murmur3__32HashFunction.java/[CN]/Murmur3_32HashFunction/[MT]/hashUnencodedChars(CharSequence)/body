{
  int h1=seed;
  for (int i=1; i < input.length(); i+=2) {
    int k1=input.charAt(i - 1) | (input.charAt(i) << 16);
    k1=mixK1(k1);
    h1=mixH1(h1,k1);
  }
  if ((input.length() & 1) == 1) {
    int k1=input.charAt(input.length() - 1);
    k1=mixK1(k1);
    h1^=k1;
  }
  return fmix(h1,Chars.BYTES * input.length());
}
