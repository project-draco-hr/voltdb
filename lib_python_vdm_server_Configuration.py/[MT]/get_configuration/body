def get_configuration():
    '\n    Gets the voltdeploy json object\n    :return: voltdeploy json object\n    '
    deployment_json = {'voltdeploy': {'databases': HTTPListener.Global.DATABASES, 'members': HTTPListener.Global.SERVERS, 'deployments': HTTPListener.Global.DEPLOYMENT, 'deployment_users': HTTPListener.Global.DEPLOYMENT_USERS, }, }
    return deployment_json
