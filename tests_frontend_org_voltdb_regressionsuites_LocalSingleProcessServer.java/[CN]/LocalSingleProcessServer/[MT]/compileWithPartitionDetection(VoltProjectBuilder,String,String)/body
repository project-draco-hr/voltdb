{
  int hostCount=1;
  int replication=0;
  if (m_compiled) {
    return true;
  }
  m_compiled=builder.compile(m_jarFileName,m_siteCount,hostCount,replication,null,true,snapshotPath,ppdPrefix);
  m_pathToDeployment=builder.getPathToDeployment();
  m_pathToVoltRoot=builder.getPathToVoltRoot();
  return m_compiled;
}
