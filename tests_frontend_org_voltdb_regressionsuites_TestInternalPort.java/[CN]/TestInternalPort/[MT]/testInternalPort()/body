{
  BufferedReader bi=new BufferedReader(new FileReader(new File(pf.m_filename)));
  String line;
  boolean failed=true;
  final CharSequence cs="Connecting to the VoltDB cluster leader";
  while ((line=bi.readLine()) != null) {
    System.out.println(line);
    if (line.contains(cs)) {
      failed=false;
      break;
    }
  }
  assertFalse(failed);
}
