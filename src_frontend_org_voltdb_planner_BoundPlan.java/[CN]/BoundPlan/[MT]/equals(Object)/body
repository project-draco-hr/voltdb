{
  if (!(obj instanceof BoundPlan)) {
    return false;
  }
  BoundPlan other=(BoundPlan)obj;
  if (!core.equals(other.core)) {
    return false;
  }
  if (constants == null) {
    if (other.constants != null) {
      return false;
    }
  }
 else   if (!constants.equals(other.constants)) {
    return false;
  }
  return true;
}
