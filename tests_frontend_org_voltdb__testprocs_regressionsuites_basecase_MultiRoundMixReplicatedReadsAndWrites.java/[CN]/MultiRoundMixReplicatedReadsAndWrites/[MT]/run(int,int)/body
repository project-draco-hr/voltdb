{
  long sum=0;
  for (int i=0; i < reps; i++) {
    voltQueueSQL(insertR1,i,i,i,Integer.toHexString(i));
    long a=voltExecuteSQL()[0].asScalarLong();
    voltQueueSQL(countR1);
    long b=voltExecuteSQL()[0].asScalarLong();
    sum+=(a * b);
    if (i == die) {
      voltQueueSQL(ConstraintViolationUpdate);
      voltExecuteSQL();
    }
  }
  return sum;
}
