def start(self, verb, runner):
    verb.merge_java_options('java_opts_override', '-server', '-XX:+HeapDumpOnOutOfMemoryError', '-XX:HeapDumpPath=/tmp', '-XX:+UseParNewGC', '-XX:+UseConcMarkSweepGC', '-XX:+CMSParallelRemarkEnabled', '-XX:+UseTLAB', '-XX:CMSInitiatingOccupancyFraction=75', '-XX:+UseCMSInitiatingOccupancyOnly', '-XX:+UseCondCardMark')
