{
  if (data == null) {
    return null;
  }
  StringBuilder sb=new StringBuilder();
  for (  byte b : data) {
    char ch=Character.forDigit((b >> 4) & 0xF,16);
    if (Character.isLetter(ch)) {
      ch-=caseDiff;
    }
    sb.append(ch);
    ch=Character.forDigit(b & 0xF,16);
    if (Character.isLetter(ch)) {
      ch-=caseDiff;
    }
    sb.append(ch);
  }
  return sb.toString();
}
