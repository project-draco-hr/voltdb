{
  String type=null;
  Field[] fields=OpTypes.class.getFields();
  for (  Field f : fields) {
    if (f.getType() != int.class)     continue;
    int value=0;
    try {
      value=f.getInt(null);
    }
 catch (    IllegalArgumentException e) {
      e.printStackTrace();
    }
catch (    IllegalAccessException e) {
      e.printStackTrace();
    }
    if (value == opType) {
      type=f.getName();
    }
  }
  assert(type != null);
  return super.toString() + ": " + type;
}
