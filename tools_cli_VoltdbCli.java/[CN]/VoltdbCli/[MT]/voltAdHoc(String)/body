{
  VoltTable[] results;
  if (!checkConnection())   return;
  try {
    results=database.callProcedure("@adhoc",sql);
    if (results.length == 1) {
      parseresults(results[0]);
    }
    ;
  }
 catch (  Exception e) {
    errormessage("SQL statement failed for the following reason:\n   " + e.getMessage());
    return;
  }
}
