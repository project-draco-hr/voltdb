{
  String[] tokens=new String[10];
  String tmpstr=s.trim();
  int i;
  int currenttoken=0;
  for (i=0; i < 10; i++) {
    tokens[i]=null;
  }
  while ((s.length() > 0) && (currenttoken < 10)) {
    i=s.indexOf(" ");
    if (i > 0) {
      tokens[currenttoken]=s.substring(0,i);
      currenttoken++;
      s=s.substring(i + 1).trim();
    }
 else {
      tokens[currenttoken]=s;
      s="";
    }
  }
  return tokens;
}
