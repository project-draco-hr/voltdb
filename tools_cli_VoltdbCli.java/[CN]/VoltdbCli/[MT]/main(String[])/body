{
  initcli();
  if (args.length > 0) {
    String fullstring="";
    for (    String s : args) {
      if (s.indexOf(' ') > -1) {
        fullstring+=" \"" + s + "\"";
      }
 else {
        fullstring+=" " + s;
      }
    }
    fullstring=fullstring.trim();
    interpreter(fullstring);
  }
 else {
    while (interpreter(getcli())) {
    }
    System.out.println("Goodbye!");
  }
}
