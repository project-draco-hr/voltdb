{
  String cmd;
  String fullclasspath;
  if (!checkpaths())   return;
  if ((classpathroot != null) && (classpathroot != "")) {
    fullclasspath=classpathroot + ":";
  }
 else {
    fullclasspath="";
  }
  fullclasspath=fullclasspath + ".:" + voltdbjarpath;
  cmd="java" + " -classpath " + fullclasspath + " org.voltdb.compiler.VoltCompiler ";
  cmd=cmd + projectdef + " "+ numofnodes.toString()+ " "+ numofpartitions.toString()+ " "+ host+ " "+ catalog;
  voltShell(cmd);
}
