{
  m_outputSchema=new NodeSchema();
  m_hasSignificantOutputSchema=true;
  for (int i=0; i < m_columnNames.length; ++i) {
    TupleValueExpression tve=new TupleValueExpression(m_tableName,m_tableName,m_columnNames[i],m_columnNames[i],i);
    m_outputSchema.addColumn(new SchemaColumn(m_tableName,m_tableName,m_columnNames[i],m_columnNames[i],tve));
  }
}
