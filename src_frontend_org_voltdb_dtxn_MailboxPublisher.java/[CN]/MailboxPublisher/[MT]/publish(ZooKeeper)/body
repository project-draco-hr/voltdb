{
  try {
    byte payload[]=m_mailboxes.toString(4).getBytes("UTF-8");
    if (m_publishedPath == null) {
      m_publishedPath=zk.create(m_publishRoot + "/mailbox",payload,Ids.OPEN_ACL_UNSAFE,CreateMode.EPHEMERAL_SEQUENTIAL);
    }
 else {
      zk.setData(m_publishRoot,payload,-1);
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
