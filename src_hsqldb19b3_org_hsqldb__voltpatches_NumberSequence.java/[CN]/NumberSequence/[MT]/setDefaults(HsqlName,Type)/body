{
  this.name=name;
  dataType=type;
  this.name=name;
  dataType=type;
  long min;
  long max;
switch (dataType.typeCode) {
case Types.SQL_SMALLINT:
    max=Short.MAX_VALUE;
  min=Short.MIN_VALUE;
break;
case Types.SQL_INTEGER:
max=Integer.MAX_VALUE;
min=Integer.MIN_VALUE;
break;
case Types.SQL_BIGINT:
max=Long.MAX_VALUE;
min=Long.MIN_VALUE;
break;
case Types.SQL_NUMERIC:
case Types.SQL_DECIMAL:
if (type.scale == 0) {
max=Long.MAX_VALUE;
min=Long.MIN_VALUE;
break;
}
default :
throw Error.error(ErrorCode.X_42565);
}
minValue=min;
maxValue=max;
increment=1;
}
