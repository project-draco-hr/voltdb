@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_in_another_folder_failed(self, jenkins_mock):
    jenkins_mock.side_effect = [jenkins.JenkinsException()]
    with self.assertRaises(jenkins.JenkinsException) as context_manager:
        self.j.copy_job(u'a Folder/TestJob', u'another Folder/TestJob_2')
    self.assertEqual(str(context_manager.exception), 'copy[a Folder/TestJob to another Folder/TestJob_2] failed, source and destination folder must be the same')
    self._check_requests(jenkins_mock.call_args_list)
