@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_simple(self, jenkins_mock):
    jenkins_mock.side_effect = [json.dumps({'name': 'Test Job_2', }), json.dumps({'name': 'Test Job_2', }), json.dumps({'name': 'Test Job_2', })]
    self.j.copy_job(u'Test Job', u'Test Job_2')
    self.assertEqual(jenkins_mock.call_args_list[0][0][0].get_full_url(), self.make_url('createItem?name=Test%20Job_2&mode=copy&from=Test%20Job'))
    self.assertTrue(self.j.job_exists('Test Job_2'))
    self._check_requests(jenkins_mock.call_args_list)
