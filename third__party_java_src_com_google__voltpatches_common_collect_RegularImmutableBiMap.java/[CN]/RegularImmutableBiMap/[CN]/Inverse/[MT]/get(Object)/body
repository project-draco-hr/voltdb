{
  if (value == null) {
    return null;
  }
  int bucket=Hashing.smear(value.hashCode()) & mask;
  for (ImmutableMapEntry<K,V> entry=valueTable[bucket]; entry != null; entry=entry.getNextInValueBucket()) {
    if (value.equals(entry.getValue())) {
      return entry.getKey();
    }
  }
  return null;
}
