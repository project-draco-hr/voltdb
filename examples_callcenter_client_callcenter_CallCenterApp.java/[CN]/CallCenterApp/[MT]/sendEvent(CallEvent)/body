{
  if (call.endTS == null) {
    assert(call.startTS != null);
    client.callProcedure("BeginCall",call.agentId,call.phoneNoStr(),call.callId,call.startTS);
  }
 else {
    assert(call.startTS == null);
    client.callProcedure("EndCall",call.agentId,call.phoneNoStr(),call.callId,call.endTS);
  }
}
