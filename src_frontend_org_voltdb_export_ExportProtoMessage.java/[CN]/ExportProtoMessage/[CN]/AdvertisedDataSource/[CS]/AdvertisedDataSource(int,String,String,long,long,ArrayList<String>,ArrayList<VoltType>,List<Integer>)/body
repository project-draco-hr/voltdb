{
  partitionId=p_id;
  signature=t_signature;
  tableName=t_name;
  m_generation=generation;
  this.systemStartTimestamp=systemStartTimestamp;
  if (names != null)   columnNames.addAll(names);
  if (types != null)   columnTypes.addAll(types);
  if (lengths != null) {
    columnLengths.addAll(lengths);
  }
}
