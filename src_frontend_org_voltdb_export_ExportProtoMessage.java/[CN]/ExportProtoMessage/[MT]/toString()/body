{
  String s="ExportProtoMessage: type(" + m_type + ") offset("+ m_offset+ ") partitionId("+ m_partitionId+ ") signature("+ m_signature+ ")"+ " serializableBytes("+ serializableBytes()+ ")";
  if (m_data != null) {
    s=s + " payloadBytes(" + m_data.remaining()+ ")";
  }
 else {
    s=s + " no payload.";
  }
  return s;
}
