{
  super.validate();
  if (m_value == null) {
    throw new Exception("ERROR: The constant value for '" + this + "' is NULL");
  }
 else   if (m_valueType == VoltType.NULL || m_valueType == VoltType.VOLTTABLE) {
    throw new Exception("ERROR: Invalid constant value type '" + m_valueType + "' for '"+ this+ "'");
  }
}
