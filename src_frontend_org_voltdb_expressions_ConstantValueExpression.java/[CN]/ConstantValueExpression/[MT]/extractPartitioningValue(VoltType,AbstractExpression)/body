{
  if (constExpr instanceof ConstantValueExpression) {
    String stringValue=((ConstantValueExpression)constExpr).getValue();
    if (voltType.isInteger()) {
      try {
        return new Long(stringValue);
      }
 catch (      NumberFormatException nfe) {
      }
    }
 else {
      return stringValue;
    }
  }
  return null;
}
