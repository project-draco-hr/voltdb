{
  try {
    ServerSocket accept=null;
    Socket sock=null;
    try {
      accept=new ServerSocket(9999);
      sock=accept.accept();
    }
  finally {
      if (accept != null) {
        try {
          accept.close();
        }
 catch (        IOException e) {
          e.printStackTrace();
        }
      }
    }
    try {
      assert(sock.isConnected());
      accepted.set(true);
      InputStream in=sock.getInputStream();
      int realLastByteRead=0;
      while ((realLastByteRead=in.read()) != -1) {
        lastByteRead=realLastByteRead;
      }
    }
  finally {
      sock.close();
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
