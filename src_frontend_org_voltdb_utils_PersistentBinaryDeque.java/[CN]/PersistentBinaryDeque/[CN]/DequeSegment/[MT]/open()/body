{
  if (!m_file.exists()) {
    m_syncedSinceLastEdit=false;
  }
  if (m_ras != null) {
    throw new IOException(m_file + " was already opened");
  }
  m_ras=new RandomAccessFile(m_file,"rw");
  m_fc=m_ras.getChannel();
  m_fc.position(4);
  if (m_fc.size() >= 4) {
    m_discardsUntilDeletion=getNumEntries();
  }
}
