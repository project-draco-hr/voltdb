{
  if (m_writeSegment == null) {
    throw new IOException("Closed");
  }
  int needed=0;
  for (  BBContainer b : objects) {
    needed+=b.b.remaining();
  }
  assert(needed < DequeSegment.m_chunkSize);
  if (m_writeSegment.remaining() < needed) {
    openNewWriteSegment();
  }
  m_writeSegment.offer(objects);
}
