{
  assertions();
  if (m_closed) {
    throw new IOException("Closed");
  }
  PBDSegment segment=m_segments.peek();
  if (segment == null) {
    return true;
  }
  if (segment.hasMoreEntries())   return false;
  for (  PBDSegment s : m_segments) {
    if (segment.hasMoreEntries())     return false;
  }
  return true;
}
