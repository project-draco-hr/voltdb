{
  if (!token.startsWith("-") || token.length() == 1) {
    return false;
  }
  int pos=token.indexOf("=");
  String t=pos == -1 ? token : token.substring(0,pos);
  if (!options.getMatchingOptions(t).isEmpty()) {
    return true;
  }
 else   if (getLongPrefix(token) != null && !token.startsWith("--")) {
    return true;
  }
  return false;
}
