{
  m_blockOnBackpressure=true;
  if (Runtime.getRuntime().availableProcessors() > 4) {
    new Thread(){
      @Override public void run(){
        try {
          while (true) {
            m_tpccSim2.doOne();
          }
        }
 catch (        IOException e) {
        }
      }
    }
.start();
  }
  try {
    while (true) {
      m_tpccSim.doOne();
    }
  }
 catch (  IOException e) {
    throw (NoConnectionsException)e;
  }
}
