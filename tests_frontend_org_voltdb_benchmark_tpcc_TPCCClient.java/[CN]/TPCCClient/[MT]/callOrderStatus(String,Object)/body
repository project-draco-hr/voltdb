{
  if (m_blockOnBackpressure) {
    final VerifyBasicCallback cb=new VerifyBasicCallback(TPCCSimulation.Transaction.ORDER_STATUS,proc);
    while (!m_voltClient.callProcedure(cb,proc,paramlist)) {
      try {
        m_voltClient.backpressureBarrier();
      }
 catch (      InterruptedException e) {
        e.printStackTrace();
      }
    }
  }
 else {
    m_queuedMessage=m_voltClient.callProcedure(new VerifyBasicCallback(TPCCSimulation.Transaction.ORDER_STATUS,proc),proc,paramlist);
  }
}
