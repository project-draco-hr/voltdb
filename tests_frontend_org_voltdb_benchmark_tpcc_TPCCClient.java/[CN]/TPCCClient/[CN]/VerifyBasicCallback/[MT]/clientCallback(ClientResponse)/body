{
  boolean errorExpected=false;
  if (m_procedureName != null && (m_procedureName.equals(Constants.ORDER_STATUS_BY_NAME) || m_procedureName.equals(Constants.ORDER_STATUS_BY_ID)))   errorExpected=true;
  boolean status=checkTransaction(m_procedureName,clientResponse,errorExpected);
  assert status;
  if (m_transactionType != null) {
    m_counts[m_transactionType.ordinal()].incrementAndGet();
  }
}
