{
  ClientResponseImpl resp=new ClientResponseImpl();
  try {
    b.position(4);
    resp.initFromBuffer(b);
    if (m_callback != null) {
      m_callback.handleResponse(resp);
    }
  }
 catch (  IOException e) {
    throw new RuntimeException("Unable to deserialize ClientResponse in SimpleClientResponseAdapter",e);
  }
}
