{
  if (m_tableSchema == null) {
    m_tableSchema=new NodeSchema();
    int columnIdx=1;
    for (    AbstractExpression colExpr : m_columnList) {
      assert(colExpr instanceof ParameterValueExpression);
      ParameterValueExpression pve=(ParameterValueExpression)colExpr;
      String columnName="C" + Integer.toString(columnIdx);
      TupleValueExpression tve=new TupleValueExpression(m_targetTableName,m_targetTableAlias,columnName,columnName,columnIdx);
      tve.setTypeSizeBytes(pve.getValueType(),pve.getValueSize(),pve.getInBytes());
      m_tableSchema.addColumn(new SchemaColumn(m_targetTableName,m_targetTableAlias,columnName,columnName,tve));
      ++columnIdx;
    }
    m_outputSchema=m_tableSchema;
    m_hasSignificantOutputSchema=true;
  }
}
