{
  PartitionProcedureCallback callback=buildTable();
  if (table.getRowCount() <= 0) {
    assert(callback.m_batchRowList.size() == 0);
    return;
  }
  try {
    m_clientImpl.callProcedure(callback,m_procName,m_tableName,table);
  }
 catch (  IOException e) {
    final ClientResponse r=new ClientResponseImpl(ClientResponse.CONNECTION_LOST,new VoltTable[0],"Connection to database was lost");
    callback.clientCallback(r);
  }
  table.clearRowData();
}
