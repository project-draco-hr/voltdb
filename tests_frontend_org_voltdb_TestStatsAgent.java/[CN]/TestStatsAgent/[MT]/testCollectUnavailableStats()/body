{
  for (  StatsSelector selector : StatsSelector.values()) {
    m_mvoltdb.getStatsAgent().collectStats(m_mockConnection,32,"STATISTICS",subselect(selector.name(),0));
    ClientResponseImpl response=responses.take();
    assertEquals(ClientResponse.GRACEFUL_FAILURE,response.getStatus());
    VoltTable results[]=response.getResults();
    assertEquals(0,results.length);
    assertEquals("Requested statistic \"" + selector.name() + "\" is not yet available or not "+ "supported in the current configuration.",response.getStatusString());
  }
}
