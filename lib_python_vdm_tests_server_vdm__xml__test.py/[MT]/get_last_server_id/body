def get_last_server_id():
    '\n        Get last Server Id\n    '
    response = requests.get(__server_url__)
    value = response.json()
    last_server_id = 0
    if value:
        server_length = len(value['servers'])
        last_server_id = value['servers'][(server_length - 1)]['id']
    return last_server_id
