{
  for (  Pair<Table,SnapshotPredicates> tableAndPredicate : tablesAndPredicates.values()) {
    Table table=tableAndPredicate.getFirst();
    SnapshotPredicates predicates=tableAndPredicate.getSecond();
    if (!ee.activateTableStream(table.getRelativeIndex(),TableStreamType.SNAPSHOT,predicates)) {
      VoltDB.crashLocalVoltDB("Attempted to activate copy on write mode for table " + table.getTypeName() + " and failed",false,null);
    }
  }
}
