{
  if (m_snapshotTargetTerminators != null && !m_snapshotTargetTerminators.isEmpty()) {
    for (    Thread terminator : m_snapshotTargetTerminators) {
      if (terminator.isAlive()) {
        m_siteTaskerQueue.offer(new SnapshotTask(true));
        return;
      }
    }
    emptyBufferPool();
  }
}
