def __init__(self, testname, timestamp, timeout, task, safety, cores):
    self.name = testname
    self.timestamp = timestamp
    self.timeout = timeout
    self.task = task
    self.safety = safety
    self.cores = cores
    self.failures = 0
    self.testsrun = 0
    self.duration = 0.0
    self.didtimeout = False
