{
  String prefix=String.format("s-size-summary-%s",label);
  sb.append("<tr>").append(String.format("<td>%s: Count</td>",name)).append(String.format("<td id='%s-count'>%d</td>",prefix,rollup.itemCount)).append("</tr>\n").append("<tr>").append(String.format("<td>%s: Size - Minimum</td>",name)).append(String.format("<td id='%s-min'>%d</td>",prefix,rollup.widthMin)).append("</tr>\n").append("<tr>").append(String.format("<td>%s: Size - Maximum</td>",name)).append(String.format("<td id='%s-max'>%d</td>",prefix,rollup.widthMax)).append("</tr>\n");
}
