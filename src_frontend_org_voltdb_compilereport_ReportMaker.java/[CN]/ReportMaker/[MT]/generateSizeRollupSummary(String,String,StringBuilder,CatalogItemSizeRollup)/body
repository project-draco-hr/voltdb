{
  String prefix=String.format("s-size-summary-%s",label);
  sb.append("<tr>").append(String.format("<td>%s (count):</td>",name)).append(String.format("<td id='%s-count' class='right-cell'>%d</td>",prefix,rollup.itemCount)).append("</tr>\n").append("<tr>").append(String.format("<td>%s (minimum size):</td>",name)).append(String.format("<td id='%s-min' class='right-cell calculated-cell'>%d</td>",prefix,rollup.widthMin)).append("</tr>\n").append("<tr>").append(String.format("<td>%s (maximum size):</td>",name)).append(String.format("<td id='%s-max' class='right-cell calculated-cell'>%d</td>",prefix,rollup.widthMax)).append("</tr>\n");
}
