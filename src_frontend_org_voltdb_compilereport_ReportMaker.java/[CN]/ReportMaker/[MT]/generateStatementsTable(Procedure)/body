{
  StringBuilder sb=new StringBuilder();
  sb.append("    <table class='table tableL2 tablesorter table-condensed'>\n    <thead><tr>" + "<th><span style='white-space: nowrap;'>Statement Name</span></th>" + "<th>Statement SQL</th>"+ "<th>Params</th>"+ "<th>R/W</th>"+ "<th>Attributes</th>"+ "</tr></thead>\n    <tbody>\n");
  for (  Statement statement : procedure.getStatements()) {
    sb.append(genrateStatementRow(procedure,statement));
  }
  sb.append("    </tbody>\n    </table>\n");
  return sb.toString();
}
