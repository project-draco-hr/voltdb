{
  StringBuilder sb=new StringBuilder();
  sb.append("    <table class='table tableL2 tablesorter table-condensed'>\n    <thead><tr>" + "<th>Index Name</th>" + "<th>Type</th>"+ "<th>Columns</th>"+ "<th>Uniqueness</th>"+ "</tr></thead>\n    <tbody>\n");
  for (  Index index : table.getIndexes()) {
    sb.append(genrateIndexRow(table,index));
  }
  sb.append("    </tbody>\n    </table>\n");
  return sb.toString();
}
