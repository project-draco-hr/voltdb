{
  if (m_txnRate == -1) {
    if (m_sampler != null) {
      m_sampler.start();
    }
    try {
      runLoop();
    }
 catch (    final IOException e) {
    }
catch (    final InterruptedException e) {
    }
  }
 else {
    System.err.println("Running rate controlled m_txnRate == " + m_txnRate + " m_txnsPerMillisecond == "+ m_txnsPerMillisecond);
    System.err.flush();
    rateControlledRunLoop();
  }
  if (m_exitOnCompletion) {
    System.exit(0);
  }
}
