{
  final byte status=clientResponse.getStatus();
  if (status != ClientResponse.SUCCESS) {
    if (errorExpected)     return true;
    if (abortExpected && status == ClientResponse.USER_ABORT)     return true;
    return false;
  }
  if (m_checkGenerator.nextFloat() >= m_checkTransaction)   return true;
  return checkConstraints(procName,clientResponse);
}
