{
  try {
    Watcher watcher=new Watcher(){
      @Override public void process(      final WatchedEvent event){
        final Watcher thisWatcher=this;
        es.execute(new Runnable(){
          @Override public void run(){
            try {
              List<String> children=zk.getChildren(VoltZK.root,thisWatcher);
              for (              String s : children) {
                if (s.equals("request_truncation_snapshot")) {
                  snapshotted=true;
                  LinkedList<SnapshotCompletionInterest> interests=new LinkedList<SnapshotCompletionInterest>(m_interests);
                  for (                  SnapshotCompletionInterest i : interests) {
                    i.snapshotCompleted(new SnapshotCompletionEvent("","",0,Collections.<Integer,Long>emptyMap(),true,"",null));
                  }
                  break;
                }
              }
            }
 catch (            Exception e) {
              e.printStackTrace();
            }
          }
        }
);
      }
    }
;
    zk.getChildren(VoltZK.root,watcher);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
