{
  m_hostCount=3;
  buildCatalog(m_hostCount,8,0,newVoltRoot(null));
  MockInitiator initiator=new MockInitiator(null);
  List<RestoreAgent> agents=new ArrayList<RestoreAgent>();
  for (int i=0; i < m_hostCount - 1; i++) {
    agents.add(new RestoreAgent(context,initiator,getClient(0),this,i,"all"));
  }
  for (  RestoreAgent agent : agents) {
    agent.restore();
  }
  int count=0;
  while (!m_done && count++ < 10) {
    try {
      Thread.sleep(200);
    }
 catch (    InterruptedException e) {
    }
  }
  if (m_done) {
    fail();
  }
  RestoreAgent agent=new RestoreAgent(context,initiator,getClient(0),this,m_hostCount - 1,"all");
  agent.restore();
  count=0;
  while (!m_done && count++ < 10) {
    try {
      Thread.sleep(500);
    }
 catch (    InterruptedException e) {
    }
  }
  if (!m_done) {
    fail();
  }
}
