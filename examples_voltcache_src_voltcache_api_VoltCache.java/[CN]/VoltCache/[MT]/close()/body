{
  try {
    this.client.drain();
    this.client.close();
  }
 catch (  NoConnectionsException e) {
    e.printStackTrace();
  }
catch (  InterruptedException e) {
    e.printStackTrace();
  }
  lock.lock();
  try {
    String key=this.servers + ":" + this.port;
    CleanupTaskPool.get(key).dispose();
    if (CleanupTaskPool.get(key).Users <= 0)     CleanupTaskPool.remove(key);
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
 finally {
    lock.unlock();
  }
}
