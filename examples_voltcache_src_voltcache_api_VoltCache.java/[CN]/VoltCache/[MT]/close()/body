{
  try {
    this.client.drain();
    this.client.close();
  }
 catch (  NoConnectionsException e) {
    e.printStackTrace();
  }
catch (  InterruptedException e) {
    e.printStackTrace();
  }
  lock.lock();
  try {
    String key=this.servers;
    cleanupTaskPool.get(key).dispose();
    if (cleanupTaskPool.get(key).users <= 0) {
      cleanupTaskPool.remove(key);
    }
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
 finally {
    lock.unlock();
  }
}
