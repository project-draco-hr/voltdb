{
  this.connection=ClientConnectionPool.get(servers,port);
  this.timer=new Timer();
  this.timer.scheduleAtFixedRate(new TimerTask(){
    private final ClientConnection con;
{
      con=connection;
    }
    @Override public void run(){
      try {
        con.execute("Cleanup");
      }
 catch (      Exception x) {
      }
    }
  }
,10000,10000);
}
