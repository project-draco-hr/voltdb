{
  if (prependDelimiter)   writer.append(m_tsvDelimiter);
  if (s.matches(m_tsvEscapeMatch)) {
    for (int ii=0; ii < s.length(); ii++) {
      char c=s.charAt(ii);
switch (c) {
case '\\':
        writer.append("\\\\");
      break;
case '\t':
    writer.append("\\t");
  break;
case '\r':
writer.append("\\r");
break;
case '\n':
writer.append("\\n");
break;
default :
writer.append(c);
break;
}
}
}
 else {
writer.append(s);
}
}
