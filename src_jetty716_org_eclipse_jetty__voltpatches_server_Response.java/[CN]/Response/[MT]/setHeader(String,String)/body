{
  if (_connection.isIncluding()) {
    if (name.startsWith(SET_INCLUDE_HEADER_PREFIX))     name=name.substring(SET_INCLUDE_HEADER_PREFIX.length());
 else     return;
  }
  _connection.getResponseFields().put(name,value);
  if (HttpHeaders.CONTENT_LENGTH.equalsIgnoreCase(name)) {
    if (value == null)     _connection._generator.setContentLength(-1);
 else     _connection._generator.setContentLength(Long.parseLong(value));
  }
}
