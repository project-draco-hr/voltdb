{
  boolean valid=true;
  TransactionRecord model_txn=null;
  for (  SiteLog site : sites) {
    if (model_txn == null) {
      model_txn=site.currentTxn();
    }
 else     if (!model_txn.equals(site.currentTxn())) {
      System.out.println("VALIDATION FAILURE, MISMATCHED TRANSACTIONS");
      valid=false;
    }
  }
  return valid;
}
