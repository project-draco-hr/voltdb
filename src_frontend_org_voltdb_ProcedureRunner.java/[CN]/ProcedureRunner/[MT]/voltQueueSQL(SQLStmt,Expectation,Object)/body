{
  if (m_batch.size() >= MAX_BATCH_SIZE) {
    throw new RuntimeException("Procedure attempted to queue more than " + MAX_BATCH_SIZE + "statements in a single batch.\n  You may use multiple batches of up to 1000 statements,"+ "each,\n  but you may also want to consider dividing this work into multiple procedures.");
  }
  QueuedSQL queuedSQL=new QueuedSQL();
  queuedSQL.expectation=expectation;
  queuedSQL.params=getCleanParams(stmt,args);
  queuedSQL.stmt=stmt;
  m_batch.add(queuedSQL);
}
