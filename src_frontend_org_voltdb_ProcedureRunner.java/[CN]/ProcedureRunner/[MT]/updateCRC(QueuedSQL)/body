{
  if (!queuedSQL.stmt.isReadOnly) {
    m_inputCRC.update(queuedSQL.stmt.sqlCRC);
    try {
      queuedSQL.params.addToCRC(m_inputCRC);
    }
 catch (    IOException e) {
      log.error("Unable to compute CRC of parameters to " + "a SQL statement in procedure: " + m_procedureName,e);
    }
  }
}
