{
  System.out.println("STARTING testKillOPPlanner");
  Client client=getClient();
  try {
    client.callProcedure("@AdHoc","select * from WAREHOUSE, DISTRICT, CUSTOMER, CUSTOMER_NAME, HISTORY, STOCK, ORDERS, NEW_ORDER, ORDER_LINE where " + "WAREHOUSE.W_ID = DISTRICT.D_W_ID and " + "WAREHOUSE.W_ID = CUSTOMER.C_W_ID and "+ "WAREHOUSE.W_ID = CUSTOMER_NAME.C_W_ID and "+ "WAREHOUSE.W_ID = HISTORY.H_W_ID and "+ "WAREHOUSE.W_ID = STOCK.S_W_ID and "+ "WAREHOUSE.W_ID = ORDERS.O_W_ID and "+ "WAREHOUSE.W_ID = NEW_ORDER.NO_W_ID and "+ "WAREHOUSE.W_ID = ORDER_LINE.OL_W_ID and "+ "WAREHOUSE.W_ID = 0");
  }
 catch (  ProcCallException e) {
  }
  try {
    Thread.sleep(200);
  }
 catch (  InterruptedException e) {
    fail();
  }
  client.callProcedure("@AdHoc","select * from warehouse;");
}
