{
  VoltTable vt;
  String qs;
  for (int i=1; i <= 10; i++) {
    for (int j=0; j < i; j++) {
      qs="INSERT INTO T1 VALUES (" + pkey++ + ", "+ i+ ");";
      vt=client.callProcedure("@AdHoc",qs)[0];
      assertTrue(vt.getRowCount() == 1);
    }
  }
  vt=client.callProcedure("@AdHoc","insert into t1 values (" + pkey++ + ",11);")[0];
  assertTrue(vt.getRowCount() == 1);
  return pkey;
}
