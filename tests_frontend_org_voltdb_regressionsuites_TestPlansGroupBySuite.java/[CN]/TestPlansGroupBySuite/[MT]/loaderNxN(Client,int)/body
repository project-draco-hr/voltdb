{
  VoltTable vt;
  for (int i=1; i <= 10; i++) {
    for (int j=0; j < i; j++) {
      vt=client.callProcedure("T1Insert",pkey++,i)[0];
      assertTrue(vt.getRowCount() == 1);
    }
  }
  vt=client.callProcedure("@AdHoc","insert into t1 values (" + pkey++ + ",11);")[0];
  assertTrue(vt.getRowCount() == 1);
  return pkey;
}
