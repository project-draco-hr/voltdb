{
  boolean wasDelimiter=true;
  StringBuffer sb=new StringBuffer();
  for (int i=0; i < statement.length; i++) {
    if (!statement[i].isDelimiter && !wasDelimiter) {
      sb.append(' ');
    }
    sb.append(statement[i].getSQL());
    wasDelimiter=statement[i].isDelimiter;
  }
  return sb.toString();
}
