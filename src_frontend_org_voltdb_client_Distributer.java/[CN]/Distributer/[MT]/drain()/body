{
  boolean more;
  do {
    more=false;
synchronized (this) {
      for (      NodeConnection cxn : m_connections) {
        more=more || cxn.m_callbacksToInvoke.get() > 0;
      }
    }
    if (more) {
      Thread.sleep(5);
    }
  }
 while (more);
synchronized (this) {
    for (    NodeConnection cxn : m_connections) {
      assert(cxn.m_callbacks.size() == 0);
    }
  }
}
