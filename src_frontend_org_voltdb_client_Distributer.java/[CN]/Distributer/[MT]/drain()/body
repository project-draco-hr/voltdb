{
  boolean more;
  do {
    more=false;
synchronized (this) {
      for (      NodeConnection cxn : m_connections) {
synchronized (cxn.m_callbacks) {
          more=more || cxn.m_callbacks.size() > 0;
        }
      }
    }
    Thread.yield();
  }
 while (more);
synchronized (this) {
    for (    NodeConnection cxn : m_connections) {
      assert(cxn.m_callbacks.size() == 0);
    }
  }
}
