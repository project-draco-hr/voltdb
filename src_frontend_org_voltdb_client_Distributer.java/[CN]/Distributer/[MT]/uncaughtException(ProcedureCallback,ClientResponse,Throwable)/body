{
  boolean handledByClient=false;
  for (  ClientStatusListener csl : m_listeners) {
    if (csl instanceof ClientImpl.CSL) {
      continue;
    }
    try {
      csl.uncaughtException(cb,r,t);
      handledByClient=true;
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  if (!handledByClient) {
    t.printStackTrace();
  }
}
