{
synchronized (this) {
    if (!m_isConnected) {
      final ClientResponse r=new ClientResponseImpl(ClientResponse.CONNECTION_LOST,new VoltTable[0],"Connection to the database was lost before a response was received");
      callback.clientCallback(r);
      c.discard();
      return;
    }
    m_callbacks.put(handle,callback);
  }
  m_connection.writeStream().enqueue(c);
}
