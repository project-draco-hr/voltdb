{
synchronized (this) {
    if (!m_isConnected) {
      final ClientResponse r=new ClientResponseImpl(ClientResponse.CONNECTION_LOST,new VoltTable[0],"Connection to database host (" + m_hostname + ") was lost before a response was received");
      try {
        callback.clientCallback(r);
      }
 catch (      Throwable t) {
        if (m_handler != null) {
          try {
            m_handler.uncaughtException(callback,r,t);
          }
 catch (          Throwable t2) {
            t.printStackTrace();
            t2.printStackTrace();
          }
        }
      }
      return;
    }
    m_callbacks.put(handle,new Object[]{System.currentTimeMillis(),callback,name});
  }
  m_connection.writeStream().enqueue(f);
}
