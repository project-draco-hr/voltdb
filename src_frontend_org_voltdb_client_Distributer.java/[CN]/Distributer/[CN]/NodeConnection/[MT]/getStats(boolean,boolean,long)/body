{
  ArrayList<ClientStats> stats=new ArrayList<ClientStats>();
  for (  ProcedureStatsTracker pst : m_stats.values()) {
    ClientStats s=new ClientStats(pst,interval,since);
    stats.add(s);
  }
  if (rollupByProc) {
    ClientStats s=ClientStats.merge(stats);
    stats.clear();
    stats.add(s);
  }
  return stats;
}
