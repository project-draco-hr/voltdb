{
  if (clientResponse.getStatus() == ClientResponse.SUCCESS) {
    try {
synchronized (Distributer.this) {
        VoltTable results[]=clientResponse.getResults();
        if (results != null && results.length > 0) {
          updatePartitioning(results[0]);
        }
      }
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
