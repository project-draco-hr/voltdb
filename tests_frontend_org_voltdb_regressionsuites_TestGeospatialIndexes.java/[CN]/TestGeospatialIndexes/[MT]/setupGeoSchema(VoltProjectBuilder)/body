{
  String geoSchema="CREATE TABLE PLACES (\n" + "  id         INTEGER NOT NULL,\n" + "  loc        GEOGRAPHY_POINT\n"+ ");\n"+ "CREATE TABLE BORDERS(\n"+ "  id         INTEGER NOT NULL,\n"+ "  region     GEOGRAPHY\n"+ ");\n"+ "\n"+ "CREATE TABLE INDEXED_PLACES (\n"+ "  id         INTEGER NOT NULL,\n"+ "  loc        GEOGRAPHY_POINT\n"+ ");\n"+ "CREATE TABLE INDEXED_BORDERS (\n"+ "  id         INTEGER NOT NULL,\n"+ "  region     GEOGRAPHY\n"+ ");\n"+ "\n";
