{
  if (m_mustDrain) {
    return;
  }
  if (!m_replayEntries.isEmpty()) {
    ReplayEntry head=m_replayEntries.firstEntry().getValue();
    if (!head.isReady()) {
      if (head.hasSentinel() && m_mpiEOLReached) {
        m_mustDrain=true;
      }
 else       if (!head.hasSentinel() && m_eolReached) {
      }
    }
  }
}
