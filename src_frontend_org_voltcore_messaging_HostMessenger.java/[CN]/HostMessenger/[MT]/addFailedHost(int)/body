{
  if (!m_knownFailedHosts.contains(hostId)) {
synchronized (m_mapLock) {
      if (!m_knownFailedHosts.contains(hostId)) {
        ImmutableSet.Builder<Integer> b=ImmutableSet.builder();
        b.addAll(m_knownFailedHosts);
        b.add(hostId);
        m_knownFailedHosts=b.build();
      }
    }
  }
}
