{
switch (constType) {
case CHECK:
    return rangeVariable.usedColumns[colIndex];
case PRIMARY_KEY:
case UNIQUE:
case MAIN:
  return ArrayUtil.find(core.mainCols,colIndex) != -1;
case FOREIGN_KEY:
return ArrayUtil.find(core.refCols,colIndex) != -1;
default :
throw Error.runtimeError(ErrorCode.U_S0500,"Constraint");
}
}
