{
  System.out.println("\n\n\nRUNNING testDeleteOrderByPlan\n\n");
  pns=compileToFragments("DELETE FROM R5 ORDER BY A LIMIT ?");
  assertEquals(2,pns.size());
  AbstractPlanNode collectorRoot=pns.get(1);
  verifyPlan(Arrays.asList(SendPlanNode.class,DeletePlanNode.class,OrderByPlanNode.class,SeqScanPlanNode.class),collectorRoot);
  pns=compileToFragments("DELETE FROM R5 WHERE A = 1 ORDER BY A LIMIT ?");
  assertEquals(2,pns.size());
  collectorRoot=pns.get(1);
  verifyPlan(Arrays.asList(SendPlanNode.class,DeletePlanNode.class,OrderByPlanNode.class,IndexScanPlanNode.class),collectorRoot);
}
