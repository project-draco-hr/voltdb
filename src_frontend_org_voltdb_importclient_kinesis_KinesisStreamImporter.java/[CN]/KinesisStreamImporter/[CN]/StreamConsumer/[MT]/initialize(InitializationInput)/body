{
  m_shardId=initInput.getShardId();
  m_largestExtendedSequenceNumber=null;
  m_formatter=((Formatter<String>)m_config.getFormatterFactory().create());
  info(null,"Initializing record processor for shard: %s, starting seqeunce number: %s",m_shardId,initInput.getExtendedSequenceNumber().getSequenceNumber());
}
