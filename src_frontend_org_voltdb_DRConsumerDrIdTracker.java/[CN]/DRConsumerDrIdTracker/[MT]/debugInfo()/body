{
  if (m_map.isEmpty()) {
    return "Empty Map";
  }
  StringBuilder sb=new StringBuilder();
  for (  Map.Entry<Long,Long> entry : m_map.entrySet()) {
    sb.append(" [" + DRLogSegmentId.getSequenceNumberFromDRId(entry.getKey()) + ", "+ DRLogSegmentId.getSequenceNumberFromDRId(entry.getValue())+ "] ");
  }
  sb.append("lastAckPoint " + DRLogSegmentId.getClusterIdFromDRId(m_lastAckedDrId) + ":"+ DRLogSegmentId.getSentinelOrSeqNumFromDRId(m_lastAckedDrId));
  sb.append("lastSpUniqueId " + UniqueIdGenerator.toShortString(m_lastSpUniqueId));
  sb.append("lastMpUniqueId " + UniqueIdGenerator.toShortString(m_lastMpUniqueId));
  return sb.toString();
}
