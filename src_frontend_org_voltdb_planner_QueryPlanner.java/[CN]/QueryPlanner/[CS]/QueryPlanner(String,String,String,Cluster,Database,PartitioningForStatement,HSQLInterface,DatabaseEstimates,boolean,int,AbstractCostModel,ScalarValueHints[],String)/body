{
  assert(sql != null);
  assert(stmtName != null);
  assert(procName != null);
  assert(HSQL != null);
  assert(catalogCluster != null);
  assert(catalogDb != null);
  assert(costModel != null);
  assert(catalogDb.getCatalog() == catalogCluster.getCatalog());
  m_sql=sql;
  m_stmtName=stmtName;
  m_procName=procName;
  m_HSQL=HSQL;
  m_db=catalogDb;
  m_cluster=catalogCluster;
  m_partitioning=partitioning;
  m_estimates=estimates;
  m_partitioning=partitioning;
  m_maxTablesPerJoin=maxTablesPerJoin;
  m_costModel=costModel;
  m_paramHints=paramHints;
  m_joinOrder=joinOrder;
  m_planSelector=new PlanSelector(m_cluster,m_db,m_estimates,m_stmtName,m_procName,m_sql,m_costModel,m_paramHints,suppressDebugOutput,System.getProperties().contains("compilerdebug"));
}
