{
  lock.lock();
  try {
    this.EndTime=System.currentTimeMillis();
    if (this.StartTime == Long.MAX_VALUE)     this.StartTime=this.EndTime - executionDuration;
    this.cnt++;
    this.tot+=executionDuration;
    if (this.min > executionDuration)     this.min=executionDuration;
    if (this.max < executionDuration)     this.max=executionDuration;
    int bucket=(int)executionDuration;
    if (executionDuration > 100)     bucket=Math.min((int)((executionDuration - 100l) / 50l),8) + 100;
    this.lat[bucket]++;
    if (!success)     this.err++;
  }
  finally {
    lock.unlock();
  }
}
