{
  validateInputNotNull(value,context);
  if (!(value instanceof String)) {
    throw new SuperCsvCellProcessorException(String.class,value,context,this);
  }
  try {
    final SimpleDateFormat formatter;
    if (locale == null) {
      formatter=new SimpleDateFormat(dateFormat);
    }
 else {
      formatter=new SimpleDateFormat(dateFormat,locale);
    }
    formatter.setLenient(lenient);
    final Date result=formatter.parse((String)value);
    return next.execute(result,context);
  }
 catch (  final ParseException e) {
    throw new SuperCsvCellProcessorException(String.format("'%s' could not be parsed as a Date",value),context,this,e);
  }
}
