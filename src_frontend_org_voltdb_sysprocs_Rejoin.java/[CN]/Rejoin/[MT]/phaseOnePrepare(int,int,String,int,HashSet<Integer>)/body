{
  VoltTable retval=new VoltTable(new ColumnInfo("HostId",VoltType.INTEGER),new ColumnInfo("Error",VoltType.STRING));
  String error=null;
  VoltDBInterface instance=VoltDB.instance();
  HostMessenger hm=instance.getHostMessenger();
  if (hm.isDownHostId(rejoinHostId)) {
    error=VoltDB.instance().doRejoinPrepare(getTransactionId(),rejoinHostId,rejoiningHostname,portToConnect,liveHosts);
  }
 else {
    error="Unable to find a down node to replace or to agree on which nodes are down. Try again.";
  }
  retval.addRow(hostId,error);
  return retval;
}
