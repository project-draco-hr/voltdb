{
  final ThreadLocalRandom r=ThreadLocalRandom.current();
  if (m_coreBindId != null) {
  }
  try {
    while (m_shouldStop == false) {
      try {
        if (m_selectedKeys == null) {
          if (m_networkId == 0) {
            nonOptimizedSelectTimerRunner(r);
          }
 else {
            nonOptimizedSelectNoTimerRunner(r);
          }
        }
 else {
          if (m_networkId == 0) {
            optimimzedSelectTimerRunner(r);
          }
 else {
            optimizedSelectNoTimerRunner(r);
          }
        }
      }
 catch (      Throwable ex) {
        ex.printStackTrace();
        m_logger.error(null,ex);
      }
    }
  }
 catch (  Throwable t) {
    t.printStackTrace();
  }
 finally {
    try {
      p_shutdown();
    }
 catch (    Throwable t) {
      m_logger.error("Error shutting down Volt Network",t);
      t.printStackTrace();
    }
  }
}
