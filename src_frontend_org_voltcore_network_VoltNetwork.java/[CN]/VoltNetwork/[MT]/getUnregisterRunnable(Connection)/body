{
  return new Runnable(){
    @Override public void run(){
      VoltPort port=(VoltPort)c;
      assert(c != null);
      SelectionKey selectionKey=port.getKey();
      try {
        if (!m_ports.contains(port)) {
          return;
        }
        try {
          port.unregistering();
        }
  finally {
          try {
            selectionKey.attach(null);
            selectionKey.cancel();
          }
  finally {
            m_ports.remove(port);
          }
        }
      }
  finally {
        port.unregistered();
      }
    }
  }
;
}
