{
  if (runFirst) {
    m_tasks.offer(new Runnable(){
      @Override public void run(){
        if (LatencyWatchdog.isEnable())         LatencyWatchdog.pet();
        callPort(port);
      }
    }
);
  }
 else {
    m_tasks.offer(new Runnable(){
      @Override public void run(){
        if (LatencyWatchdog.isEnable())         LatencyWatchdog.pet();
        installInterests(port);
      }
    }
);
  }
  m_selector.wakeup();
}
