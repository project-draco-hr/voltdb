{
  client.callProcedure("InsertR1",1,2,7);
  client.callProcedure("InsertR1",2,2,7);
  client.callProcedure("InsertR1",4,3,2);
  client.callProcedure("InsertR1",5,6,null);
  VoltTable result=client.callProcedure("@AdHoc","SELECT * FROM R1 A JOIN R1 B ON A.A = B.C;").getResults()[0];
  System.out.println(result.toString());
  assertEquals(2,result.getRowCount());
  result=client.callProcedure("@AdHoc","SELECT * FROM R1 A LEFT JOIN R1 B ON A.A = B.D;").getResults()[0];
  System.out.println(result.toString());
  assertEquals(4,result.getRowCount());
}
