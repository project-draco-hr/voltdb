{
  String javahome=System.getenv("JAVA_HOME");
  if (javahome != null) {
    File f=new File(new File(javahome,"bin"),"java");
    if (f.exists() && f.canExecute()) {
      return f.getAbsolutePath();
    }
 else {
      hostLog.warn("JAVA_HOME environment variable (" + javahome + ") was set, but could not find an executable java binary at "+ f.getAbsolutePath());
    }
  }
 else {
    hostLog.warn("JAVA_HOME environment variable was not set, couldn't be used to find a java binary");
  }
  return null;
}
