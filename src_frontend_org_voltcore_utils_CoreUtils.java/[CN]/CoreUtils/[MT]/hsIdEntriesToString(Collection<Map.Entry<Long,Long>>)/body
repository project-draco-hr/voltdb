{
  StringBuilder sb=new StringBuilder();
  sb.append('{');
  boolean first=true;
  for (  Map.Entry<Long,Long> entry : entries) {
    if (!first)     sb.append(", ");
    first=false;
    sb.append(CoreUtils.hsIdToString(entry.getKey())).append(" -> ");
    sb.append(CoreUtils.hsIdToString(entry.getValue()));
  }
  sb.append('}');
  return sb.toString();
}
