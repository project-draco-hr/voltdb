{
  if (data == null || data.getRowCount() == 0)   return;
  Cluster cluster=m_site.catalog.getClusters().get(clusterName);
  if (cluster == null) {
    throw new VoltAbortException("cluster '" + clusterName + "' does not exist");
  }
  Database db=cluster.getDatabases().get(databaseName);
  if (db == null) {
    throw new VoltAbortException("database '" + databaseName + "' does not exist in cluster "+ clusterName);
  }
  Table table=db.getTables().getIgnoreCase(tableName);
  if (table == null) {
    throw new VoltAbortException("table '" + tableName + "' does not exist in database "+ clusterName+ "."+ databaseName);
  }
  try {
    m_site.ee.loadTable(table.getRelativeIndex(),data,m_site.getCurrentTxnId(),m_site.lastCommittedTxnId,m_site.getNextUndoToken());
  }
 catch (  EEException e) {
    throw new VoltAbortException("Failed to load table: " + tableName + " into EE");
  }
}
