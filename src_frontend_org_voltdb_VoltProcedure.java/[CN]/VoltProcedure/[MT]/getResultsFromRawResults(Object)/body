{
  if (result == null)   return new VoltTable[0];
  if (result instanceof VoltTable[])   return (VoltTable[])result;
  if (result instanceof VoltTable)   return new VoltTable[]{(VoltTable)result};
  if (result instanceof Long) {
    VoltTable t=new VoltTable(new VoltTable.ColumnInfo("",VoltType.BIGINT));
    t.addRow(result);
    return new VoltTable[]{t};
  }
  throw new RuntimeException("Procedure didn't return acceptable type.");
}
