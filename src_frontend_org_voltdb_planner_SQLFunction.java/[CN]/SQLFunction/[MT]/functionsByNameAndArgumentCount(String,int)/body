{
  List<SQLFunction> result=new ArrayList<SQLFunction>();
  SQLFunction[] overloads=name_lookup.get(name.intern());
  if (overloads != null) {
    for (    SQLFunction fn : overloads) {
      if (fn.m_nArguments != nArguments) {
        continue;
      }
      result.add(fn);
    }
  }
  return result;
}
