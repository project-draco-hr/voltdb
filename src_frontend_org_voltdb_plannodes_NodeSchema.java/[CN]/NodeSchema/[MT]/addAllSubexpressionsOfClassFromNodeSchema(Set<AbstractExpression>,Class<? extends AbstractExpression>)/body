{
  for (  SchemaColumn col : getColumns()) {
    AbstractExpression colExpr=col.getExpression();
    if (colExpr == null) {
      continue;
    }
    Collection<AbstractExpression> found=colExpr.findAllSubexpressionsOfClass(aeClass);
    if (found.isEmpty()) {
      continue;
    }
    exprs.addAll(found);
  }
}
