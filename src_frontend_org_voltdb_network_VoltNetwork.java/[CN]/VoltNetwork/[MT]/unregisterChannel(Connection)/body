{
  VoltPort port=(VoltPort)c;
  assert(c != null);
  SelectionKey selectionKey=port.getKey();
  acquireRegistrationLock();
  try {
synchronized (m_ports) {
      if (!m_ports.contains(port)) {
        return;
      }
    }
    port.unregistering();
    selectionKey.cancel();
    selectionKey.attach(null);
synchronized (m_ports) {
      m_ports.remove(port);
    }
  }
  finally {
    releaseRegistrationLock();
  }
  port.unregistered();
}
