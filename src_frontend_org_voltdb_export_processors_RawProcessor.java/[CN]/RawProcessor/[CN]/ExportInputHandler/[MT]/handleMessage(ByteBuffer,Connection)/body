{
  try {
    FastDeserializer fds=new FastDeserializer(message);
    final ExportProtoMessage m=ExportProtoMessage.readExternal(fds);
    m_mailbox.add(new Runnable(){
      @Override public void run(){
        m_sb.event(m);
      }
    }
);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
