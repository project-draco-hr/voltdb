{
  final File schemaFile=VoltProjectBuilder.writeStringToTempFile(ddl);
  final String schemaPath=schemaFile.getPath();
  final String simpleProject="<?xml version=\"1.0\"?>\n" + "<project>" + "<database name='database'>"+ "<schemas>"+ "<schema path='" + schemaPath + "' />"+ "</schemas>"+ "<procedures>"+ "<procedure class='"+ procedureClass+ "' />"+ "</procedures>"+ "<partitions>"+ "<partition table='"+ table+ "' column='PKEY' />"+ "</partitions>"+ "</database>"+ "</project>";
  final File projectFile=VoltProjectBuilder.writeStringToTempFile(simpleProject);
  final String projectPath=projectFile.getPath();
  final VoltCompiler compiler=new VoltCompiler();
  final boolean success=compiler.compile(projectPath,testout_jar,null,null);
  assertFalse(success);
  return compiler.m_errors;
}
