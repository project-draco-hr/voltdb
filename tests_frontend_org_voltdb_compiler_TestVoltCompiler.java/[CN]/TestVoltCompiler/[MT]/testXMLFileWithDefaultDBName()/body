{
  final File schemaFile=VoltProjectBuilder.writeStringToTempFile("create table T(ID INTEGER);");
  final String simpleXML="<?xml version=\"1.0\"?>\n" + "<project>" + "<database>"+ "<schemas>"+ "<schema path='" + schemaFile.getAbsolutePath() + "'/>"+ "</schemas>"+ "<procedures>"+ "<procedure class='proc'><sql>select * from T</sql></procedure>"+ "</procedures>"+ "</database>"+ "</project>";
  final File xmlFile=VoltProjectBuilder.writeStringToTempFile(simpleXML);
  final String path=xmlFile.getPath();
  final VoltCompiler compiler=new VoltCompiler();
  final boolean success=compiler.compile(path,tmpfile,System.out,null);
  assertTrue(success);
  assertTrue(compiler.m_catalog.getClusters().get("cluster").getDatabases().get("database") != null);
}
