{
  String schemaPath="";
  try {
    final URL url=TestVoltCompiler.class.getResource("toowidetable-ddl.sql");
    schemaPath=URLDecoder.decode(url.getPath(),"UTF-8");
  }
 catch (  final UnsupportedEncodingException e) {
    e.printStackTrace();
    System.exit(-1);
  }
  final String simpleProject="<?xml version=\"1.0\"?>\n" + "<project>" + "<database name='database'>"+ "<schemas><schema path='" + schemaPath + "' /></schemas>"+ "<procedures><procedure class='org.voltdb.compiler.procedures.TPCCTestProc' /></procedures>"+ "</database>"+ "</project>";
  final File projectFile=VoltProjectBuilder.writeStringToTempFile(simpleProject);
  final String projectPath=projectFile.getPath();
  final VoltCompiler compiler=new VoltCompiler();
  final boolean success=compiler.compileWithProjectXML(projectPath,testout_jar);
  assertFalse(success);
  boolean found=false;
  for (  final VoltCompiler.Feedback fb : compiler.m_errors) {
    if (fb.message.startsWith("Table MANY_COLUMNS has"))     found=true;
  }
  assertTrue(found);
}
