{
  _statsStartedAt.set(System.currentTimeMillis());
  _requestStats.reset();
  _requestTimeStats.reset();
  _dispatchedStats.reset();
  _dispatchedTimeStats.reset();
  _suspendStats.reset();
  _resumes.set(0);
  _expires.set(0);
  _responses1xx.set(0);
  _responses2xx.set(0);
  _responses3xx.set(0);
  _responses4xx.set(0);
  _responses5xx.set(0);
  _responsesTotalBytes.set(0L);
}
