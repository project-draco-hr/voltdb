{
  failToCompile("SELECT RANK() OVER (PARTITION BY A ORDER BY B ) FROM AAA GROUP BY A;","Use of both windowed operations and GROUP BY is not supported.");
  failToCompile("SELECT RANK() OVER (PARTITION BY A ORDER BY B ) AS R1, " + "       RANK() OVER (PARTITION BY B ORDER BY A ) AS R2  " + "FROM AAA;","At most one windowed display column is supported.");
  failToCompile("SELECT RANK() OVER (PARTITION BY A ORDER BY A, B) FROM AAA;","Aggregate windowed expressions with range " + "window frame units can have only one order by expression.");
  failToCompile("SELECT RANK() OVER (PARTITION BY A ORDER BY CAST(A AS FLOAT)) FROM AAA;","Aggregate windowed expressions with RANGE " + "window frame units can have only integer or TIMESTAMP value types.");
  failToCompile("SELECT A, B, C FROM AAA WHERE RANK() OVER (PARTITION BY A ORDER BY B) < 3;","Windowed expressions can only appear in the select list.");
  failToCompile("SELECT RANK() OVER (PARTITION BY A ORDER BY A, B) AS ARANK " + "FROM (select A, B, C from AAA where A < B) ALPHA, BBB " + "WHERE ALPHA.C <> BBB.C;","Column \"A\" is ambiguous.  It\'s in tables: ALPHA, BBB");
}
