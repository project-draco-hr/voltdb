{
  if (hasConflictTableInSchema(m_schema,CatalogUtil.DR_CONFLICTS_PARTITIONED_EXPORT_TABLE)) {
    sb.append("DROP TABLE " + CatalogUtil.DR_CONFLICTS_PARTITIONED_EXPORT_TABLE + ";\n");
