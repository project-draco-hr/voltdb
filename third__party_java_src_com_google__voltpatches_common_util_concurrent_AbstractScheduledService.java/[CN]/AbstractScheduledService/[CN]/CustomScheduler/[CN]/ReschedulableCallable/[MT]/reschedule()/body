{
  lock.lock();
  try {
    if (currentFuture == null || !currentFuture.isCancelled()) {
      final Schedule schedule=CustomScheduler.this.getNextSchedule();
      currentFuture=executor.schedule(this,schedule.delay,schedule.unit);
    }
  }
 catch (  Throwable e) {
    service.notifyFailed(e);
  }
 finally {
    lock.unlock();
  }
}
