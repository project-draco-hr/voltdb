{
  if (rowIndex >= getRowCount())   return null;
  if (columnIndex >= 2)   return null;
  int i=0;
  for (  String fieldName : m_type.getFields()) {
    if (i == rowIndex) {
      if (columnIndex == 0)       return fieldName;
      if (columnIndex == 1)       return m_type.getField(fieldName);
    }
    i++;
  }
  return null;
}
