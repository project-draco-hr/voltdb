{
  int count=m_backPressureCount.get();
  if (count > 0) {
    try {
      if (count > 8) {
        Thread.sleep(256);
      }
 else {
        Thread.sleep(1 << count);
      }
    }
 catch (    InterruptedException e) {
      if (m_logger.isDebugEnabled()) {
        m_logger.debug("Sleep for back pressure interrupted",e);
      }
    }
  }
}
