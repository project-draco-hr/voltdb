{
  if (id % 10 == 0)   return PARTITIONED_MP;
  if (id % 10 == 1)   return PARTITIONED_MP;
  if (id % 10 == 2)   return REPLICATED;
  if (id % 10 == 3)   return REPLICATED;
  if (id % 10 == 4)   return HYBRID;
  if (id % 10 == 5)   return HYBRID;
  if (allowInProcAdhoc && (id % 10 == 6) && (id % 20 != 6))   return ADHOC_MP;
  return PARTITIONED_SP;
}
