{
  if ((skip & ExportProtoMessage.kOpen) == 0)   assertFalse(m.isOpen());
  if ((skip & ExportProtoMessage.kOpenResponse) == 0)   assertFalse(m.isOpenResponse());
  if ((skip & ExportProtoMessage.kPoll) == 0)   assertFalse(m.isPoll());
  if ((skip & ExportProtoMessage.kPollResponse) == 0)   assertFalse(m.isPollResponse());
  if ((skip & ExportProtoMessage.kAck) == 0)   assertFalse(m.isAck());
  if ((skip & ExportProtoMessage.kClose) == 0)   assertFalse(m.isClose());
  if ((skip & ExportProtoMessage.kError) == 0)   assertFalse(m.isError());
}
