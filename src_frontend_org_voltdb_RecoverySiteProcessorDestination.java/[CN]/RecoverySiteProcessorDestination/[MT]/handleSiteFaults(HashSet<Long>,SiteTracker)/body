{
  for (  Map.Entry<Integer,RecoveryTable> entry : m_tables.entrySet()) {
    if (failedSites.contains(entry.getValue().m_sourceHSId)) {
      VoltDB.crashLocalVoltDB("Node fault during recovery of Site " + m_HSId + " resulted in source Site "+ entry.getValue().m_sourceHSId+ " becoming unavailable. Failing recovering node.",false,null);
    }
  }
}
