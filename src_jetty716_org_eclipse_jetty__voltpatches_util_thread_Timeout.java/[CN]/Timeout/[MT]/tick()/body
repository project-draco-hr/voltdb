{
  final long expiry=_now - _duration;
  Task task=null;
  while (true) {
    try {
synchronized (_lock) {
        task=_head._next;
        if (task == _head || task._timestamp > expiry)         break;
        task.unlink();
        task._expired=true;
        task.expire();
      }
      task.expired();
    }
 catch (    Throwable th) {
      Log.warn(Log.EXCEPTION,th);
    }
  }
}
