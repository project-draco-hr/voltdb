{
  m_pendingTxnBytes-=messageSize;
  m_pendingTxnCount--;
  if (m_pendingTxnBytes < (MAX_DESIRED_PENDING_BYTES * .8) && m_pendingTxnCount < (MAX_DESIRED_PENDING_TXNS * .8)) {
    if (m_hadBackPressure) {
      hostLog.info("TXN backpressure ended");
      m_hadBackPressure=false;
      m_ci.offBackPressure();
    }
  }
}
