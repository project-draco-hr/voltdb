{
  if (!(service instanceof NoOpService)) {
    logger.log(Level.FINE,"Service {0} has terminated. Previous state was: {1}",new Object[]{service,from});
  }
  state.monitor.enter();
  try {
    if (from == State.NEW) {
      startTimer();
      finishedStarting(false);
    }
    state.serviceTerminated(service);
  }
  finally {
    state.monitor.leave();
    state.executeListeners();
  }
}
