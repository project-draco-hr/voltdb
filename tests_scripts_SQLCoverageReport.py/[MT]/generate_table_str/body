def generate_table_str(res):
    tablestr = {'jni': '', 'hsqldb': '', }
    for k in tablestr.iterkeys():
        tmp = []
        if (('Result' in res[k]) and (res[k]['Result'] != None)):
            for i in xrange(len(res[k]['Result'])):
                result = []
                result.append(('column count: %d' % len(res[k]['Result'][i].columns)))
                result.append(('row count: %d' % len(res[k]['Result'][i].tuples)))
                result.append(('cols: ' + ', '.join(map((lambda x: str(x)), res[k]['Result'][i].columns))))
                result.append('rows -')
                result.extend(map((lambda x: str(x)), res[k]['Result'][i].tuples))
                for j in xrange(len(result)):
                    if (('highlight' in res) and isinstance(res['highlight'][i], list) and (j in res['highlight'][i])):
                        result[j] = highlight(result[j])
                tmp.append('<br />'.join(result))
            tablestr[k] = '<br />'.join(tmp)
    return tablestr
