{
  assertTrue(hsqlEmptyRS.relative(-1) == voltEmptyRS.relative(-1));
  assertTrue(hsqlEmptyRS.getRow() == voltEmptyRS.getRow());
  assertTrue(hsqlEmptyRS.relative(0) == voltEmptyRS.relative(0));
  assertTrue(hsqlEmptyRS.getRow() == voltEmptyRS.getRow());
  assertTrue(hsqlEmptyRS.relative(1) == voltEmptyRS.relative(1));
  assertTrue(hsqlEmptyRS.getRow() == voltEmptyRS.getRow());
}
