{
  validateInputNotNull(value,context);
  final Long result;
  if (value instanceof Long) {
    result=(Long)value;
  }
 else {
    try {
      result=Long.parseLong(value.toString());
    }
 catch (    final NumberFormatException e) {
      throw new SuperCsvCellProcessorException(String.format("'%s' could not be parsed as a Long",value),context,this,e);
    }
  }
  if (result < min || result > max) {
    throw new SuperCsvConstraintViolationException(String.format("%d does not lie between the min (%d) and max (%d) values (inclusive)",result,min,max),context,this);
  }
  return next.execute(result,context);
}
