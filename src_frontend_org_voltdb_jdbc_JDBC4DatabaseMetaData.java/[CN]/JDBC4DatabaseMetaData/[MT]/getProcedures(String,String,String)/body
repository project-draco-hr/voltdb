{
  if (procedureNamePattern != null && procedureNamePattern != "%")   throw new SQLException(String.format("getProcedures('%s','%s','%s') does not support pattern filtering",catalog,schemaPattern,procedureNamePattern));
  checkClosed();
  this.sysCatalog.setString(1,"PROCEDURES");
  ResultSet res=this.sysCatalog.executeQuery();
  return res;
}
