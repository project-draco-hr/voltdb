{
  ClientStats stats=periodicStatsContext.fetchAndResetBaseline().getStats();
  long thrup;
  thrup=stats.getTxnThroughput();
  log.info(String.format("Throughput %d/s, Aborts/Failures %d/%d, Avg/95%% Latency %.2f/%.2fms",thrup,stats.getInvocationAborts(),stats.getInvocationErrors(),stats.getAverageLatency(),stats.kPercentileLatencyAsDouble(0.95)));
}
