{
  while (_header != null && _header.length() > 0) {
    byte ch=_header.peek();
    if (ch == HttpTokens.CARRIAGE_RETURN || ch == HttpTokens.LINE_FEED) {
      _eol=ch;
      _header.skip(1);
    }
 else     break;
  }
  while (_body != null && _body.length() > 0) {
    byte ch=_body.peek();
    if (ch == HttpTokens.CARRIAGE_RETURN || ch == HttpTokens.LINE_FEED) {
      _eol=ch;
      _body.skip(1);
    }
 else     break;
  }
}
