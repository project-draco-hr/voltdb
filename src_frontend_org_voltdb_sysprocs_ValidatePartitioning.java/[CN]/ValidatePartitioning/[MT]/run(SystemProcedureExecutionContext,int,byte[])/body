{
  final long startTime=System.currentTimeMillis();
  VoltTable retval[];
  if (config != null) {
    retval=performValidatePartitioningWork(type,config);
  }
 else {
    retval=performValidatePartitioningWork(TheHashinator.getCurrentConfig().getFirst().typeId(),TheHashinator.getCurrentConfig().getSecond());
  }
  final long endTime=System.currentTimeMillis();
  final long duration=endTime - startTime;
  HOST_LOG.info("Validating partitioning took " + duration + " milliseconds");
  return retval;
}
