{
  int msgsize=super.getSerializedSize();
  msgsize+=8 + 8 + 8+ 1+ 1+ 1+ 2;
  msgsize+=4 * m_dependencyCount;
  for (  VoltTable dep : m_dependencies) {
    msgsize+=dep.getSerializedSize();
  }
  if (m_exception != null) {
    msgsize+=m_exception.getSerializedSize();
  }
 else {
    msgsize+=4;
  }
  return msgsize;
}
