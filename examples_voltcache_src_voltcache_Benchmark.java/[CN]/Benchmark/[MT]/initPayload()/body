{
  final PayloadProcessor processor=new PayloadProcessor(this.config.keysize,this.config.minvaluesize,this.config.maxvaluesize,this.config.poolsize,this.config.usecompression);
  if (this.config.preload) {
    System.out.print("Initializing data store... ");
    for (int i=0; i < this.config.poolsize; i++) {
      final PayloadProcessor.Pair pair=processor.generateForStore(i);
      cache.set(pair.Key,0,0,pair.getStoreValue(),true);
    }
    System.out.println(" Done.");
  }
  return processor;
}
