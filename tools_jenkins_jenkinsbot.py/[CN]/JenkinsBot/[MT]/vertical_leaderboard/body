def vertical_leaderboard(self, headers, table):
    "\n        Displays each row in the table as one over the other. Similar to mysql's '\\G'\n        "
    table_str = ''
    for (i, row) in enumerate(table):
        table[i] = list(row)
        table_str += ('%d\n' % (i + 1))
        for (j, entry) in enumerate(row):
            table_str += (((headers[j] + ': ') + str(entry)) + '\n')
        table_str += '\n\n'
    return table_str
