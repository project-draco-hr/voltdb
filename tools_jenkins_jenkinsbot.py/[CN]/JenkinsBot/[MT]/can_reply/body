def can_reply(self, incoming):
    '\n        :param incoming: A dictionary describing what data is incoming to the bot\n        :return: true if bot can act on incoming data\n        '
    return ((len(incoming) > 0) and (incoming[0].get('text', None) is not None) and (incoming[0].get('bot_id', None) is None) and (incoming[0].get('file', None) is None) and (incoming[0]['channel'] != GENERAL_CHANNEL) and (incoming[0]['channel'] != RANDOM_CHANNEL))
