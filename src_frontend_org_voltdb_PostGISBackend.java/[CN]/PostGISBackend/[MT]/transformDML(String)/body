{
  return transformCentroidQuery(transformAreaQuery(transformDistanceQuery(transformContainsQuery(transformLongitudeQuery(transformLatitudeQuery(PostgreSQLBackend.transformDML(dml))))))).replace("pointFromText('POINT","ST_GeographyFromText('POINT").replace("polygonFromText('POLYGON","ST_GeographyFromText('POLYGON").replace("asText","ST_AsText");
}
