{
  ClientStats stats=periodicStatsContext.fetchAndResetBaseline().getStats();
  long time=Math.round((stats.getEndTimestamp() - benchmarkStartTS) / 1000.0);
  log.info(_F("%02d:%02d:%02d Throughput %d/s, Aborts/Failures %d/%d, Avg/95%% Latency %.2f/%dms",time / 3600,(time / 60) % 60,time % 60,stats.getTxnThroughput(),stats.getInvocationAborts(),stats.getInvocationErrors(),stats.getAverageLatency(),stats.kPercentileLatency(0.95)));
}
