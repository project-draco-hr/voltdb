{
  Client clientTmp=this.client;
  this.users=0;
  this.client=null;
  if (clientTmp != null) {
    try {
      clientTmp.close();
    }
 catch (    InterruptedException e) {
    }
  }
  clientTmp=ClientFactory.createClient(this.config);
  for (  String server : this.servers) {
    clientTmp.createConnection(server);
  }
  this.client=clientTmp;
  this.users++;
}
