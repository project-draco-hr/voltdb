{
  if (session == null)   return;
synchronized (this) {
    _sessions.put(session.getClusterId(),session);
  }
  try {
    session.willPassivate();
    storeSession(((JDBCSessionManager.Session)session)._data);
    session.didActivate();
  }
 catch (  Exception e) {
    Log.warn("Unable to store new session id=" + session.getId(),e);
  }
}
