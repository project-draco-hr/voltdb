{
  if (buffer.capacity() == 0) {
    return super.getMessage();
  }
 else {
    StringBuilder sb=new StringBuilder(super.getMessage());
    sb.append('\n');
    sb.append("Constraint Type ");
    sb.append(type);
    sb.append(", Table CatalogId ");
    sb.append(tableId);
    sb.append('\n');
    sb.append(getTuples().toString());
    return sb.toString();
  }
}
