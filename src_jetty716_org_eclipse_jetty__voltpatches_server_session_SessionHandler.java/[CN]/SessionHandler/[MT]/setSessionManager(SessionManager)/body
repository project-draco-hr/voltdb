{
  if (isStarted())   throw new IllegalStateException();
  SessionManager old_session_manager=_sessionManager;
  if (getServer() != null)   getServer().getContainer().update(this,old_session_manager,sessionManager,"sessionManager",true);
  if (sessionManager != null)   sessionManager.setSessionHandler(this);
  _sessionManager=sessionManager;
  if (old_session_manager != null)   old_session_manager.setSessionHandler(null);
}
