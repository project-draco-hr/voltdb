  assertEquals(EXPECTED_DOY,result);
  EXPECTED_QUARTER=3;
  result=r.getLong(columnIndex++);
  assertEquals(EXPECTED_QUARTER,result);
  EXPECTED_HOUR=12;
  result=r.getLong(columnIndex++);
  assertEquals(EXPECTED_HOUR,result);
  EXPECTED_MINUTE=53;
