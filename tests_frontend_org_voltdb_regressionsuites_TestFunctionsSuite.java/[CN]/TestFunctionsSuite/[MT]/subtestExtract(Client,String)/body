  assertEquals(extractFailed,EXPECTED_WEEK,result);
  result=r.getLong(columnIndex++);
  assertEquals(extractFailed,EXPECTED_WEEK,result);
  EXPECTED_WEEKDAY=0;
  if (isHSQL()) {
    EXPECTED_WEEKDAY=EXPECTED_DOW;
