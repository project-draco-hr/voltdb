{
  System.out.println("-------------------------------------------------------------------------------------");
  int maxLength=24;
  for (  Argument a : Arguments)   if (maxLength < a.Name.length())   maxLength=a.Name.length();
  for (  Argument a : Arguments) {
    System.out.printf("%1$#" + (maxLength - 1) + "s : ",a.Name);
    System.out.println(a.Value);
  }
  System.out.println("-------------------------------------------------------------------------------------");
  return this;
}
