{
  validateInputNotNull(value,context);
  if (!(value instanceof String)) {
    throw new SuperCsvCellProcessorException(String.class,value,context,this);
  }
  final String stringValue=((String)value).toLowerCase();
  final Boolean result;
  if (trueValues.contains(stringValue)) {
    result=Boolean.TRUE;
  }
 else   if (falseValues.contains(stringValue)) {
    result=Boolean.FALSE;
  }
 else {
    throw new SuperCsvCellProcessorException(String.format("'%s' could not be parsed as a Boolean",value),context,this);
  }
  return next.execute(result,context);
}
