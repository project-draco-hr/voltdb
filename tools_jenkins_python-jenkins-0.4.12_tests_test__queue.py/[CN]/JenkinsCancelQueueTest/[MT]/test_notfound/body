@patch.object(jenkins.Jenkins, 'jenkins_open', side_effect=jenkins.NotFoundException('not found'))
def test_notfound(self, jenkins_mock):
    job_name_to_return = {u'name': 'TestJob', }
    jenkins_mock.return_value = json.dumps(job_name_to_return)
    self.j.cancel_queue(52)
    self.assertEqual(jenkins_mock.call_args[0][0].get_full_url(), self.make_url('queue/cancelItem?id=52'))
    self._check_requests(jenkins_mock.call_args_list)
