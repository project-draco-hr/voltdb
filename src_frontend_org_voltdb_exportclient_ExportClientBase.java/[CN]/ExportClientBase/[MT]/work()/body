{
  int offered_msgs=0;
  for (  HashMap<Integer,ExportDataSink> part_map : m_sinks.values()) {
    for (    ExportDataSink work_sink : part_map.values()) {
      work_sink.work();
    }
  }
  for (  ExportConnection el_connection : m_exportConnections.values()) {
    offered_msgs=el_connection.work();
  }
  return offered_msgs;
}
