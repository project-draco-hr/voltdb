{
  return new TypeSafeMatcher<VoltMessage>(){
    Matcher<BinaryPayloadMessage> payloadMatcher=ackPayloadIs(partitionId,signature,uso);
    @Override public void describeTo(    Description d){
      d.appendDescriptionOf(payloadMatcher);
    }
    @Override protected boolean matchesSafely(    VoltMessage m){
      return instanceOf(BinaryPayloadMessage.class).matches(m) && payloadMatcher.matches(BinaryPayloadMessage.class.cast(m));
    }
  }
;
}
