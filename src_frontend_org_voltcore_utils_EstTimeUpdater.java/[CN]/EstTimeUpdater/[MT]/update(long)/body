{
  final long estNow=EstTime.m_now.get();
  if (estNow == now) {
    return null;
  }
  EstTime.m_now.lazySet(now);
  if (now - estNow > 2000) {
    if (lastErrorReport > now) {
      lastErrorReport=now;
    }
    if (now - lastErrorReport > maxErrorReportInterval) {
      lastErrorReport=now;
      return now - estNow;
    }
  }
  return null;
}
