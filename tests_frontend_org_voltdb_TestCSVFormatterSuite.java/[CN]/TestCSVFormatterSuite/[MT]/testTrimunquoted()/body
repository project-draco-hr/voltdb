{
  System.out.println("testTrimunquoted");
  Client client=getClient();
  while (!((ClientImpl)client).isHashinatorInitialized()) {
    Thread.sleep(1000);
    System.out.println("Waiting for hashinator to be initialized...");
  }
  String[] myData={"12,10.05,  test"};
  CountDownLatch latch=new CountDownLatch(1);
  (new SocketDataPusher("localhost",7002,latch,myData)).start();
  VoltTable ts_table=client.callProcedure("@AdHoc","SELECT * FROM importCSVTable ORDER BY clm_integer;").getResults()[0];
  while (ts_table.advanceRow()) {
    String value=ts_table.getString(3);
    assertEquals("  test",value);
    break;
  }
}
