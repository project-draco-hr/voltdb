{
  String proc=catProc.getTypeName();
  if (proc.charAt(0) < 'a') {
    currentTimingInfo=null;
    return;
  }
  startTime=System.nanoTime();
  if (profileStartTime == 0)   profileStartTime=startTime;
  TimingInfo ti=times.get(proc);
  if (ti == null) {
    ti=new TimingInfo();
    ti.name=proc;
    ti.proc=catProc;
    times.put(proc,ti);
  }
  currentTimingInfo=ti;
}
