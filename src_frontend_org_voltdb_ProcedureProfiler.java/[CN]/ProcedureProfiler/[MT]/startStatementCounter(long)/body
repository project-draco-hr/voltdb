{
  if (profilingLevel == Level.DISABLED)   return;
  if (currentTimingInfo == null)   return;
  stmtStartTime=System.nanoTime();
  TimingInfo ti=currentTimingInfo.stmts.get(fragId);
  if (ti == null) {
    ti=new TimingInfo();
    currentTimingInfo.stmts.put(fragId,ti);
  }
  if ((fragId == -1) && (ti.name == null))   ti.name="Batched Statements";
  currentStmtTimingInfo=ti;
}
