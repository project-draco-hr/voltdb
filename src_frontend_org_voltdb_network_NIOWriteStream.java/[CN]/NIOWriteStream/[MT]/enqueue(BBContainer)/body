{
  assert(c != null);
  assert(c.b != null);
  if (c.b.remaining() == 0) {
    c.discard();
    return false;
  }
synchronized (this) {
    if (m_isShutdown) {
      c.discard();
      return false;
    }
    updateLastPendingWriteTimeAndQueueBackpressure();
    updateQueued(c.b.remaining());
    m_queuedBuffers.offer(c);
    m_port.setInterests(SelectionKey.OP_WRITE,0);
  }
  return true;
}
