{
  if (Log.isDebugEnabled())   Log.debug("Container " + parent + " + "+ child+ " as "+ relationship);
  if (_listeners != null) {
    Relationship event=new Relationship(this,parent,child,relationship);
    for (int i=0; i < LazyList.size(_listeners); i++)     ((Listener)LazyList.get(_listeners,i)).add(event);
  }
}
