{
  VoltProjectBuilder builder=new VoltProjectBuilder();
  String schema="CREATE ROLE admin WITH sysproc, adhoc, defaultproc;" + "CREATE PROCEDURE ALLOW admin FROM CLASS org.voltdb.compiler.procedures.NotAnnotatedEmptyProcedure;";
  builder.addLiteralSchema(schema);
  boolean success=builder.compile(pathToCatalog);
  assertTrue(success);
  MiscUtils.copyFile(builder.getPathToDeployment(),pathToDeployment);
  VoltDB.Configuration config=new VoltDB.Configuration();
  config.m_pathToCatalog=pathToCatalog;
  config.m_pathToDeployment=pathToDeployment;
  startSystem(config);
  ClientResponse resp=m_client.callProcedure("NotAnnotatedEmptyProcedure",1l,"Test","Yuning He");
  VoltTable vt=resp.getResults()[0];
  vt.advanceToRow(0);
  assertEquals(vt.get(0,VoltType.INTEGER),1);
  teardownSystem();
}
