def buildPro():
    with cd((builddir + '/pro')):
        run('pwd')
        run('git status')
        run('git describe --dirty')
        run(('VOLTCORE=../voltdb ant -f mmt.xml -Djmemcheck=NO_MEMCHECK -Dallowreplication=true -Dlicensedays=%d -Dkitbuild=true %s clean dist.pro' % (defaultlicensedays, build_args)))
