def systemCheck():
    detectHostname()
    detect64bitOS()
    detectThreadCount()
    detectOSRelease()
    detectMemory()
    detectNTP()
    detectJavaVersion()
    detectPythonVersion()
    detectTransparentHugePages()
    detectSwapoff()
    detectSwappiness()
    detectMemoryOvercommit()
    detectSegmentationOffload()
    displayResults()
