def displayResults():
    fails = 0
    warns = 0
    for (key, val) in sorted(output.items()):
        if (val[0] == 'FAIL'):
            fails += 1
        elif (val[0] == 'WARN'):
            warns += 1
        print ('Stats: %-25s %-8s %-9s' % (key, val[0], val[1]))
    if (fails > 0):
        print '\nCheck FAILED. Please review.'
    elif (warns > 0):
        print (('\nCheck completed with ' + str(warns)) + ' WARNINGS.')
    else:
        print '\nCheck completed successfully.'
