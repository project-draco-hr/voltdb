{
  MultiException mex=new MultiException();
  try {
    super.doStop();
  }
 catch (  Throwable e) {
    mex.add(e);
  }
  if (_handlers != null) {
    for (int i=_handlers.length; i-- > 0; )     try {
      _handlers[i].stop();
    }
 catch (    Throwable e) {
      mex.add(e);
    }
  }
  mex.ifExceptionThrow();
}
