def compare_results(suite, seed, statements_path, hsql_path, jni_path, output_dir, report_invalid, report_all, extra_stats, comparison_database, modified_sql_path):
    'Just calls SQLCoverageReport.generate_html_reports(...).\n    '
    return generate_html_reports(suite, seed, statements_path, hsql_path, jni_path, output_dir, report_invalid, report_all, extra_stats, comparison_database, modified_sql_path)
