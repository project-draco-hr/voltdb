{
synchronized (this) {
    if (_header != null && !_headerOut) {
      _headerOut=true;
      return _header;
    }
    if (_buffer != null && _header != null && _header.capacity() == _buffer.capacity() && !_bufferOut) {
      _bufferOut=true;
      return _buffer;
    }
    if (_header != null)     return new ByteArrayBuffer(_header.capacity());
    return new ByteArrayBuffer(4096);
  }
}
