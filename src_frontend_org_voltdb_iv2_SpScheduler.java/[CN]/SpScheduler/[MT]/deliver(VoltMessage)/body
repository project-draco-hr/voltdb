{
  if (message instanceof TransactionInfoBaseMessage && (((TransactionInfoBaseMessage)message).isForReplay())) {
    if (!m_replaySequencer.offer(message)) {
      deliver2(message);
    }
 else {
      VoltMessage m=m_replaySequencer.poll();
      while (m != null) {
        deliver2(m);
        m=m_replaySequencer.poll();
      }
    }
  }
 else {
    deliver2(message);
  }
}
