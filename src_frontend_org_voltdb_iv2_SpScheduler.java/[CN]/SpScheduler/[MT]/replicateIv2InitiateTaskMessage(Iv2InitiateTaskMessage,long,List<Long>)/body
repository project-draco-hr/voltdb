{
  if (replicaHSIds.size() > 0) {
    try {
      Iv2InitiateTaskMessage replmsg=new Iv2InitiateTaskMessage(m_mailbox.getHSId(),m_mailbox.getHSId(),msg.getTxnId(),msg.isReadOnly(),msg.isSinglePartition(),msg.getStoredProcedureInvocation(),msg.getClientInterfaceHandle());
      replmsg.setSpHandle(newSpHandle);
      m_mailbox.send(com.google.common.primitives.Longs.toArray(m_replicaHSIds),replmsg);
    }
 catch (    MessagingException e) {
      hostLog.error("Failed to deliver response from execution site.",e);
    }
    List<Long> expectedHSIds=new ArrayList<Long>(replicaHSIds);
    expectedHSIds.add(m_mailbox.getHSId());
    DuplicateCounter counter=new DuplicateCounter(msg.getInitiatorHSId(),msg.getTxnId(),expectedHSIds);
    m_duplicateCounters.put(newSpHandle,counter);
  }
}
