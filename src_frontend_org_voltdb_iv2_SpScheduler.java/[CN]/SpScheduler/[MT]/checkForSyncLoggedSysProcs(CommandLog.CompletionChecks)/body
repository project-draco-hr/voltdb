{
  final SiteTaskerRunnable r=new SiteTasker.SiteTaskerRunnable(){
    @Override void run(){
      assert(currentChecks != null);
synchronized (m_lock) {
        currentChecks.checkForSyncLoggedSysProcs();
      }
    }
  }
;
  if (InitiatorMailbox.SCHEDULE_IN_SITE_THREAD) {
    m_tasks.offer(r);
  }
 else {
    r.run();
  }
}
