{
  super(partitionId,taskQueue);
  m_durabilityListener=new DurabilityListener(){
    @Override public void onDurability(    ArrayDeque<Object> durableThings){
synchronized (m_lock) {
        for (        Object o : durableThings) {
          taskQueue.offer((TransactionTask)o);
        }
      }
    }
  }
;
}
