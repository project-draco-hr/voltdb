{
  ClientStats stats=periodicStatsContext.fetchAndResetBaseline().getStats();
  long time=Math.round((stats.getEndTimestamp() - startTime) / 1000.0);
  m_log.info(String.format("%02d:%02d:%02d Throughput %d/s, Aborts/Failures %d/%d",time / 3600,(time / 60) % 60,time % 60,stats.getTxnThroughput(),stats.getInvocationAborts(),stats.getInvocationErrors()));
}
