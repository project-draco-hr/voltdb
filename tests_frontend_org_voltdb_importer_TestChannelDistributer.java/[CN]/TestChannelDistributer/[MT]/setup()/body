{
  setUpZK(3);
  queue=new LinkedBlockingDeque<>();
  zks=ImmutableMap.<String,ZooKeeper>builder().put(ZERO,getClient(0)).put(UNO,getClient(1)).put(DUE,getClient(2)).build();
  distributers=ImmutableMap.<String,ChannelDistributer>builder().put(ZERO,new ChannelDistributer(zks.get(ZERO),ZERO,queue)).put(UNO,new ChannelDistributer(zks.get(UNO),UNO,queue)).put(DUE,new ChannelDistributer(zks.get(DUE),DUE,queue)).build();
}
