{
  long numTotalFound=0;
  long lastItemId=Long.MAX_VALUE;
  long lastCreated=created;
  for (long loop=0; loop < numLoops; loop++) {
    voltQueueSQL(getItems,lastCreated,lastItemId,itemsPerLoop);
    VoltTable results=voltExecuteSQL()[0];
    numTotalFound+=results.getRowCount();
    for (int i=0; i < results.getRowCount(); i++) {
      VoltTableRow row=results.fetchRow(i);
      lastItemId=row.getLong(0);
      lastCreated=row.getLong(1);
    }
  }
  return numTotalFound;
}
