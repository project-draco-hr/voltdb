{
  dut.write(makeTuples(),tableId);
  if (hasSchema) {
    while (m_mb.noSentMessages()) {
      Thread.yield();
    }
    verifyData((RejoinDataMessage)m_mb.pollMessage(),dut.m_targetId,StreamSnapshotMessageType.SCHEMA,tableId,dut.m_blockIndex - 1);
  }
  while (m_mb.noSentMessages()) {
    Thread.yield();
  }
  verifyData((RejoinDataMessage)m_mb.pollMessage(),dut.m_targetId,StreamSnapshotMessageType.DATA,tableId,dut.m_blockIndex - 1);
}
