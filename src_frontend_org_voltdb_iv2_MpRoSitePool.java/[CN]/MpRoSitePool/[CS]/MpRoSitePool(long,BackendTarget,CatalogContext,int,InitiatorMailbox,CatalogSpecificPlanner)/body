{
  m_siteId=siteId;
  m_backend=backend;
  m_catalogContext=context;
  m_partitionId=partitionId;
  m_initiatorMailbox=initiatorMailbox;
  m_csp=csp;
  for (int i=0; i < INITIAL_POOL_SIZE; i++) {
    m_idleSites.push(new MpRoSiteContext(m_siteId,m_backend,m_catalogContext,m_partitionId,m_initiatorMailbox,m_csp));
  }
}
