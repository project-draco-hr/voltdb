{
  boolean retval=canAcceptWork();
  if (!retval) {
    return false;
  }
  if (m_idleSites.isEmpty()) {
    m_idleSites.push(new MpRoSiteContext(m_siteId,m_backend,m_catalogContext,m_partitionId,m_initiatorMailbox,m_csp));
  }
  MpRoSiteContext site=m_idleSites.pop();
  m_busySites.put(txnId,site);
  site.offer(task);
  return true;
}
