{
  MpRoSiteContext site=m_busySites.remove(txnId);
  if (site == null) {
    throw new RuntimeException("No busy site for txnID: " + txnId + " found, shouldn't happen.");
  }
  if (site.getCatalogCRC() == m_catalogContext.getCatalogCRC()) {
    m_idleSites.push(site);
  }
 else {
    site.shutdown();
  }
}
