{
  String sql, sql_rewrote;
  sql="select distinct A1, B1 from R1";
  sql_rewrote="select A1, B1 from R1 group by A1, B1";
  checkQueriesPlansAreTheSame(sql,sql_rewrote);
  sql="select distinct A1+B1 from R1";
  sql_rewrote="select A1+B1 from R1 group by A1+B1";
  checkQueriesPlansAreTheSame(sql,sql_rewrote);
}
