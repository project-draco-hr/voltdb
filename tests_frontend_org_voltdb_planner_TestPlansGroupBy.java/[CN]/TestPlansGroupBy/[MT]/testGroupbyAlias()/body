{
  pns=compileToFragments("SELECT abs(PKEY) as sp, count(*) as ct FROM P1 GROUP BY sp");
  String explainStr1=printExplainPlan(pns);
  pns=compileToFragments("SELECT abs(PKEY) as sp, count(*) as ct FROM P1 GROUP BY abs(PKEY)");
  String explainStr2=printExplainPlan(pns);
  assertEquals(explainStr1,explainStr2);
}
