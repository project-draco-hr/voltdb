{
  failToCompile("SELECT 2*count(P1.PKEY) AS AAA FROM P1 GROUP BY AAA","invalid GROUP BY expression:  COUNT()");
  failToCompile("SELECT P1.PKEY AS AAA, P1.PKEY AS AAA FROM P1 GROUP BY AAA","Group by expression \"AAA\" is ambiguous");
  failToCompile("SELECT 2*count(P1.PKEY) AS AAA, P1.PKEY AS AAA FROM P1 GROUP BY AAA","Group by expression \"AAA\" is ambiguous");
  compile("SELECT P1.PKEY AS AAA FROM P1 GROUP BY AAA");
}
