{
  pns=compileToFragments("SELECT V_SUM_C1 FROM V_P1 GROUP by V_SUM_C1");
  checkMVReaggreateFeatureAggPushdown(pns,1,0,2,1,3,0);
  pns=compileToFragments("SELECT V_SUM_C1 FROM V_P1 GROUP by V_SUM_C1 " + "ORDER BY V_SUM_C1");
  checkMVReaggreateFeatureAggPushdown(pns,1,0,2,1,3,0);
  pns=compileToFragments("SELECT V_SUM_C1 FROM V_P1 GROUP by V_SUM_C1 " + "ORDER BY V_SUM_C1 LIMIT 5");
  checkMVReaggreateFeatureAggPushdown(pns,1,0,2,1,3,0);
  pns=compileToFragments("SELECT V_SUM_C1 FROM V_P1 GROUP by V_SUM_C1 LIMIT 5");
  checkMVReaggreateFeatureAggPushdown(pns,1,0,2,1,3,0);
  pns=compileToFragments("SELECT distinct V_SUM_C1 FROM V_P1 GROUP by V_SUM_C1 LIMIT 5");
  checkMVReaggreateFeatureAggPushdown(pns,1,0,2,1,3,0);
  pns=compileToFragments("SELECT V_SUM_C1, sum(V_CNT) FROM V_P1 GROUP by V_SUM_C1");
  checkMVReaggreateFeatureAggPushdown(pns,1,1,2,2,3,1);
  pns=compileToFragments("SELECT V_SUM_C1, sum(V_CNT) FROM V_P1 " + "GROUP by V_SUM_C1 ORDER BY V_SUM_C1");
  checkMVReaggreateFeatureAggPushdown(pns,1,1,2,2,3,1);
  pns=compileToFragments("SELECT V_SUM_C1, sum(V_CNT) FROM V_P1 " + "GROUP by V_SUM_C1 ORDER BY V_SUM_C1 limit 2");
  checkMVReaggreateFeatureAggPushdown(pns,1,1,2,2,3,1);
  pns=compileToFragments("SELECT V_SUM_C1, sum(distinct V_CNT) " + "FROM V_P1 GROUP by V_SUM_C1 ORDER BY V_SUM_C1");
  checkMVReaggreateFeatureAggPushdownFalse(pns,3,1,2,2);
  pns=compileToFragments("SELECT V_A1,V_B1, V_SUM_C1, sum(V_SUM_D1) FROM V_P1 " + "GROUP BY V_A1,V_B1, V_SUM_C1");
  checkMVReaggreateFeatureAggPushdown(pns,3,1,2,2,3,1);
  pns=compileToFragments("SELECT V_A1,V_B1, V_SUM_C1, sum(V_SUM_D1) FROM V_P1 " + "GROUP BY V_A1,V_B1, V_SUM_C1 ORDER BY V_A1,V_B1, V_SUM_C1");
  checkMVReaggreateFeatureAggPushdown(pns,3,1,2,2,3,1);
  pns=compileToFragments("SELECT V_A1,V_B1, V_SUM_C1, sum(V_SUM_D1) FROM V_P1 " + "GROUP BY V_A1,V_B1, V_SUM_C1 ORDER BY V_A1,V_B1, V_SUM_C1 LIMIT 5");
  checkMVReaggreateFeatureAggPushdown(pns,3,1,2,2,3,1);
  pns=compileToFragments("SELECT V_A1,V_B1, V_SUM_C1, sum(V_SUM_D1) FROM V_P1 " + "GROUP BY V_A1,V_B1, V_SUM_C1 ORDER BY V_A1, V_SUM_C1 LIMIT 5");
  checkMVReaggreateFeatureAggPushdown(pns,3,1,2,2,3,1);
  pns=compileToFragments("SELECT V_A1,V_B1, V_SUM_C1, sum( distinct V_SUM_D1) FROM V_P1 " + "GROUP BY V_A1,V_B1, V_SUM_C1 ORDER BY V_A1, V_SUM_C1 LIMIT 5");
  checkMVReaggreateFeatureAggPushdownFalse(pns,3,1,2,2);
}
