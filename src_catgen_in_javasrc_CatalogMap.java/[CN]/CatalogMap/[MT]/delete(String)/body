{
  try {
    if (m_items.containsKey(name) == false)     throw new CatalogException("Catalog item '" + name + "' doesn't exists in "+ m_parent);
    m_items.remove(name);
    updateVersioning();
    int index=1;
    for (    Entry<String,T> e : m_items.entrySet()) {
      e.getValue().m_relativeIndex=index++;
    }
  }
 catch (  Exception ex) {
    throw new RuntimeException(ex);
  }
}
