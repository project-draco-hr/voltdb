{
  try {
    String mapKey=name.toLowerCase();
    if (m_items.containsKey(mapKey) == false)     throw new CatalogException("Catalog item '" + mapKey + "' doesn't exists in "+ m_parent);
    m_items.remove(mapKey);
    int index=1;
    for (    Entry<String,T> e : m_items.entrySet()) {
      e.getValue().m_relativeIndex=index++;
    }
  }
 catch (  Exception ex) {
    throw new RuntimeException(ex);
  }
}
