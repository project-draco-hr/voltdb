{
  if (!in) {
    if (nLeft != null) {
      nLeft.nParent=null;
    }
    if (nRight != null) {
      nRight.nParent=null;
    }
    if (nParent != null) {
      if (iData == nParent.iLeft) {
        nParent.nLeft=null;
      }
 else {
        nParent.nRight=null;
      }
    }
    nLeft=nRight=nParent=null;
  }
}
