{
  int initiatorCount=0;
  for (  final Site s : m_context.siteTracker.getUpSites())   if (s.getIsexec() == false)   initiatorCount++;
  final int[] initiatorIds=new int[initiatorCount];
  int index=0;
  for (  final Site s : m_context.siteTracker.getUpSites())   if (s.getIsexec() == false)   initiatorIds[index++]=Integer.parseInt(s.getTypeName());
  assert(m_mailbox != null);
  RestrictedPriorityQueue retval=new RestrictedPriorityQueue(initiatorIds,siteId,m_mailbox,VoltDB.DTXN_MAILBOX_ID);
  return retval;
}
