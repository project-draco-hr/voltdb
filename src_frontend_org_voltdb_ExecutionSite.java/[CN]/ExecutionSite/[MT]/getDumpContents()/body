{
  final ExecutorContext context=new ExecutorContext();
  context.siteId=siteId;
  if (m_mailbox instanceof SiteMailbox)   context.mailboxHistory=((SiteMailbox)m_mailbox).getHistory();
  if (m_currentTransactionState != null)   context.activeTransaction=m_currentTransactionState.getDumpContents();
  m_transactionQueue.getDumpContents(context);
  return context;
}
