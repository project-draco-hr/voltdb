{
  tryFetchNewWork();
  TransactionState nextTxn=m_transactionQueue.peek();
  if (nextTxn == null)   return false;
  if (nextTxn instanceof SinglePartitionTxnState) {
    nextTxn=m_transactionQueue.peek();
    boolean success=nextTxn.doWork();
    assert(success);
    return true;
  }
  return false;
}
