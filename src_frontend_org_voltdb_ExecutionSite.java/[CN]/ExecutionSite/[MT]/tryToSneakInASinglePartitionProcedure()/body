{
  VoltMessage message=m_mailbox.recv();
  tick();
  if (message != null) {
    handleMailboxMessage(message);
    return true;
  }
 else {
    TransactionState nextTxn=null;
    if (nextTxn instanceof SinglePartitionTxnState) {
      boolean success=nextTxn.doWork(m_rejoining);
      assert(success);
      return true;
    }
 else {
      return false;
    }
  }
}
