{
  if (m_txnlog.isTraceEnabled()) {
    m_txnlog.trace("FUZZTEST beginNewTxn " + txnState.txnId + " "+ (txnState.isSinglePartition() ? "single" : "multi")+ " "+ (txnState.isReadOnly() ? "readonly" : "readwrite")+ " "+ (txnState.isCoordinator() ? "coord" : "part"));
  }
  if (!txnState.isReadOnly()) {
    assert(txnState.getBeginUndoToken() == kInvalidUndoToken);
    txnState.setBeginUndoToken(latestUndoToken);
    assert(txnState.getBeginUndoToken() != kInvalidUndoToken);
  }
}
