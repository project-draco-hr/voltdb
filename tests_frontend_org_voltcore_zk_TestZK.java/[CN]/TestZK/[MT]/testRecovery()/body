{
  ZooKeeper zk=getClient(0);
  zk.create("/foo",new byte[0],Ids.OPEN_ACL_UNSAFE,CreateMode.PERSISTENT);
  failSite(0);
  zk.close();
  for (int ii=1; ii < NUM_AGREEMENT_SITES; ii++) {
    zk=getClient(ii);
    assertEquals(zk.getData("/foo",false,null).length,0);
  }
  recoverSite(0);
  assertEquals(zk.getData("/foo",false,null).length,0);
  zk=getClient(0);
  assertEquals(zk.getData("/foo",false,null).length,0);
}
