{
  ExpressionType coveringExprType=coveringExpr.getExpressionType();
  AbstractExpression leftCoveringExpr=coveringExpr.getLeft();
  AbstractExpression otherExpr=coveringExpr.getRight();
  ExpressionType[] comparators=ExpressionType.getCoveringPartialIndexComparators(coveringExprType);
  if (comparators != null) {
    List<List<AbstractExpression>> coveredCandidateExprs=getCoveredCandidateExpressionFromFilters(comparators,leftCoveringExpr,tableScan,exprsToCover);
    assert(coveredCandidateExprs.size() == 2);
    List<AbstractExpression> nonExactMatchCoveredExprs=getCoveredExpressionFromFilters(coveringExprType,otherExpr,coveredCandidateExprs.get(0),coveredCandidateExprs.get(1));
    exprsToCover.removeAll(nonExactMatchCoveredExprs);
  }
  return exprsToCover;
}
