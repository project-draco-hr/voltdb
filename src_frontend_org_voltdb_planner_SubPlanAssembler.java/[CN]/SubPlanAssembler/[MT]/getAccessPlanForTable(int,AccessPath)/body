{
  assert(tableAliasIdx != StmtTableScan.NULL_ALIAS_INDEX);
  assert(path != null);
  AbstractPlanNode scanNode=null;
  if (path.index == null) {
    scanNode=getScanAccessPlanForTable(tableAliasIdx,path.otherExprs);
  }
 else {
    scanNode=getIndexAccessPlanForTable(tableAliasIdx,path);
  }
  return scanNode;
}
