{
  assert(table != null);
  assert(path != null);
  AbstractPlanNode scanNode=null;
  if (path.index == null) {
    scanNode=getScanAccessPlanForTable(table,path.otherExprs);
  }
 else {
    scanNode=getIndexAccessPlanForTable(table,path);
  }
  scanNode.generateOutputSchema(m_db);
  return scanNode;
}
