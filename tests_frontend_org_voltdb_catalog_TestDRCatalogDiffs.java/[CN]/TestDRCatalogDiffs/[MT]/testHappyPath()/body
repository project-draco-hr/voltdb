{
  String masterSchema="CREATE TABLE T1 (C1 INTEGER NOT NULL, C2 INTEGER, C3 VARCHAR(50) NOT NULL, " + "PRIMARY KEY (C1), LIMIT PARTITION ROWS 100 EXECUTE (DELETE FROM T1 WHERE C1 < 50));\n" + "PARTITION TABLE T1 ON COLUMN C1;\n"+ "CREATE UNIQUE INDEX foo ON T1 (C3, C1);\n"+ "DR TABLE T1;";
  String replicaSchema="CREATE TABLE T1 (C1 INTEGER NOT NULL, C2 INTEGER, C3 VARCHAR(50) NOT NULL, " + "PRIMARY KEY (C1), LIMIT PARTITION ROWS 100 EXECUTE (DELETE FROM T1 WHERE C1 < 50));\n" + "PARTITION TABLE T1 ON COLUMN C1;\n"+ "CREATE UNIQUE INDEX foo ON T1 (C3, C1);\n"+ "DR TABLE T1;";
  CatalogDiffEngine diff=runCatalogDiff(masterSchema,replicaSchema);
  assertTrue(diff.errors(),diff.supported());
}
