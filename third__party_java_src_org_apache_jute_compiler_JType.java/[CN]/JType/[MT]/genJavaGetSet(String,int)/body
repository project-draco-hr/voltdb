{
  String getFunc="  public " + mJavaName + " get"+ capitalize(fname)+ "() {\n";
  getFunc+="    return " + fname + ";\n";
  getFunc+="  }\n";
  String setFunc="  public void set" + capitalize(fname) + "("+ mJavaName+ " m_) {\n";
  setFunc+="    " + fname + "=m_;\n";
  setFunc+="  }\n";
  return getFunc + setFunc;
}
