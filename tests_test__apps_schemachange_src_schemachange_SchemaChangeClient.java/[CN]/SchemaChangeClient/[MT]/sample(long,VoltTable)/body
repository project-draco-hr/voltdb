{
  VoltTable t2=t.clone(4096 * 1024);
  ClientResponse cr=callROProcedureWithRetry("@AdHoc",String.format("select * from %s where pkey >= %d order by pkey limit 100;",TableHelper.getTableName(t),offset));
  assert(cr.getStatus() == ClientResponse.SUCCESS);
  VoltTable result=cr.getResults()[0];
  result.resetRowPosition();
  while (result.advanceRow()) {
    t2.add(result);
  }
  return t2;
}
