{
  if (t == null) {
    return 0;
  }
  ClientResponse cr=callROProcedureWithRetry("@AdHoc",String.format("select pkey from %s order by pkey desc limit 1;",TableHelper.getTableName(t)));
  assert(cr.getStatus() == ClientResponse.SUCCESS);
  VoltTable result=cr.getResults()[0];
  return result.getRowCount() > 0 ? result.asScalarLong() : 0;
}
