{
  boolean success=true;
  try {
    if (!this.isNOP) {
      this.catalogData=this.builder.compileToBytes();
      assert(this.catalogData != null);
      success=execUpdate(client,"@UpdateApplicationCatalog",catalogData,false);
    }
  }
  finally {
    this.builder=null;
    this.isNOP=true;
  }
  return success;
}
