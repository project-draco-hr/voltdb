{
  String ddlString=this.ddl.toString();
  boolean success=true;
  try {
    if (ddl.length() > 0) {
      log.info(_F("\n::: DDL Batch (BEGIN) :::\n%s\n::: DDL Batch (END) :::",ddlString));
      success=execLiveDDL(client,ddlString,false);
    }
  }
  finally {
    this.ddl=null;
  }
  return success;
}
