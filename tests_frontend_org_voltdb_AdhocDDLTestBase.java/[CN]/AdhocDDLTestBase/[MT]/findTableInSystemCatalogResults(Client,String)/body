{
  VoltTable tables=client.callProcedure("@SystemCatalog","TABLES").getResults()[0];
  boolean found=false;
  tables.resetRowPosition();
  while (tables.advanceRow()) {
    String thisTable=tables.getString("TABLE_NAME");
    if (thisTable.equalsIgnoreCase(table)) {
      found=true;
      break;
    }
  }
  return found;
}
