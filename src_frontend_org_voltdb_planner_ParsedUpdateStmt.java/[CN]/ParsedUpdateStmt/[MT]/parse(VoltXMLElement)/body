{
  String tableName=stmtNode.attributes.get("table");
  assert(tableName != null);
  tableName=tableName.trim();
  table=getTableFromDB(tableName);
  tableList.add(table);
  for (  VoltXMLElement child : stmtNode.children) {
    if (child.name.equalsIgnoreCase("columns"))     parseColumns(child);
 else     if (child.name.equalsIgnoreCase("condition"))     parseConditions(child);
  }
}
