{
  try {
    m_leaderCache.start(true);
  }
 catch (  Exception e) {
    VoltDB.crashLocalVoltDB("Unable to configure SpInitiator.",true,e);
  }
  super.configureCommon(backend,serializedCatalog,catalogContext,csp,numberOfPartitions,startAction,agent,memStats,cl);
  m_tickProducer.start();
  LeaderElector.createParticipantNode(m_messenger.getZK(),LeaderElector.electionDirForPartition(m_partitionId),Long.toString(getInitiatorHSId()),null);
  ((SpScheduler)m_scheduler).setDRGateway(PartitionDRGateway.getInstance(m_partitionId,nodeDRGateway,true));
}
