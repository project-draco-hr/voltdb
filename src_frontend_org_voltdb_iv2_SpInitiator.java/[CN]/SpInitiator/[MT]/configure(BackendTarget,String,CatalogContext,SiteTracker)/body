{
  m_executionSite=new Iv2ExecutionSite(m_scheduler,m_initiatorMailbox.getHSId(),backend,catalogContext,serializedCatalog,catalogContext.m_transactionId,m_partitionId,siteTracker.m_numberOfPartitions);
  ProcedureRunnerFactory prf=new ProcedureRunnerFactory();
  prf.configure(m_executionSite,null);
  m_procSet=new LoadedProcedureSet(m_executionSite,prf,m_initiatorMailbox.getHSId(),0,siteTracker.m_numberOfPartitions);
  m_procSet.loadProcedures(catalogContext,backend);
  m_initiatorMailbox.setProcedureSet(m_procSet);
  m_siteThread=new Thread(m_executionSite);
  m_siteThread.start();
}
