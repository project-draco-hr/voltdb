{
  m_leaderElector=new LeaderElector(m_messenger.getZK(),zkLeaderNode(),"sp",new byte[]{},this);
  try {
    m_leaderElector.start(true);
  }
 catch (  Exception ex) {
    VoltDB.crashLocalVoltDB("Partition " + m_partitionId + " failed to initialize "+ "leader elector. ",false,ex);
  }
  return m_leaderElector.isLeader();
}
