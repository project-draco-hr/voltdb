{
  try {
    tmLog.info("SP " + m_initiatorMailbox.getHSId() + " becoming leader for partition "+ m_partitionId);
    m_term=new Term(m_messenger.getZK(),m_partitionId,getInitiatorHSId(),m_initiatorMailbox);
    Future<?> inaugurated=m_term.start(m_kfactorForStartup);
    inaugurated.get();
    tmLog.info("SP " + m_initiatorMailbox.getHSId() + " is successfully the leader for partition "+ m_partitionId);
    m_repairLog.setLeaderState(true);
    m_scheduler.setLeaderState(true);
  }
 catch (  Exception e) {
    VoltDB.crashLocalVoltDB("Bad news.",true,e);
  }
}
