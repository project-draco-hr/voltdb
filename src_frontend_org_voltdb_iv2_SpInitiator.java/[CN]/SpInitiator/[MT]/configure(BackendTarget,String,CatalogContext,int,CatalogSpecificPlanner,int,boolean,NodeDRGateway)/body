{
  try {
    m_leaderCache.start(true);
  }
 catch (  Exception e) {
    VoltDB.crashLocalVoltDB("Unable to configure SpInitiator.",true,e);
  }
  super.configureCommon(backend,serializedCatalog,catalogContext,csp,numberOfPartitions,createForRejoin && isRejoinable(),nodeDRGateway);
  LeaderElector.createParticipantNode(m_messenger.getZK(),LeaderElector.electionDirForPartition(m_partitionId),Long.toString(getInitiatorHSId()),null);
  m_scheduler.setDRGateway(PartitionDRGateway.getInstance(m_partitionId,nodeDRGateway));
}
