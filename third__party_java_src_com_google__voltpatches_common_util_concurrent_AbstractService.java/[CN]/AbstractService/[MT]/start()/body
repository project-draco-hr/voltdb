{
  if (monitor.enterIf(isStartable)) {
    try {
      snapshot=new StateSnapshot(STARTING);
      starting();
      doStart();
    }
 catch (    Throwable startupFailure) {
      notifyFailed(startupFailure);
    }
 finally {
      monitor.leave();
      executeListeners();
    }
  }
  return startup;
}
