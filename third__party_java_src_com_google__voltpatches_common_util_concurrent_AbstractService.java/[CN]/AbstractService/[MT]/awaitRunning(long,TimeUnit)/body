{
  if (monitor.enterWhenUninterruptibly(hasReachedRunning,timeout,unit)) {
    try {
      checkCurrentState(RUNNING);
    }
  finally {
      monitor.leave();
    }
  }
 else {
    throw new TimeoutException("Timed out waiting for " + this + " to reach the RUNNING state. "+ "Current state: "+ state());
  }
}
