{
  checkColumnBounds(columnIndex);
  try {
    if (table.getColumnType(columnIndex - 1) != VoltType.STRING)     throw SQLError.get(SQLError.CONVERSION_NOT_FOUND,table.getColumnType(columnIndex - 1),"byte[]");
    return table.getStringAsBytes(columnIndex - 1);
  }
 catch (  SQLException x) {
    throw x;
  }
catch (  Exception x) {
    throw SQLError.get(x);
  }
}
