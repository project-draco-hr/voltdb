{
  System.err.println("Client started");
  try {
    m_voltClient.createConnection("localhost","program","none");
  }
 catch (  UnknownHostException e) {
    e.printStackTrace();
    System.exit(-1);
  }
catch (  ConnectException e) {
    e.printStackTrace();
    System.exit(-1);
  }
catch (  IOException e) {
    e.printStackTrace();
    System.exit(-1);
  }
}
