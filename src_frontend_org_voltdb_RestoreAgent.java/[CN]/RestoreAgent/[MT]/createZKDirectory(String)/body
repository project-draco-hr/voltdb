{
  try {
    try {
      m_zk.create(path,new byte[0],Ids.OPEN_ACL_UNSAFE,CreateMode.PERSISTENT);
    }
 catch (    KeeperException e) {
      if (e.code() != Code.NODEEXISTS) {
        throw e;
      }
    }
  }
 catch (  Exception e) {
    LOG.fatal("Failed to create Zookeeper node: " + e.getMessage());
    VoltDB.crashVoltDB();
  }
}
