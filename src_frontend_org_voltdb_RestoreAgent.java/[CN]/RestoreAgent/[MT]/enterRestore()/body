{
  try {
    Pair<String,Boolean> result=ZKUtil.createAndElectLeader(m_zk,RESTORE_BARRIER,new byte[0],null);
    m_zkBarrierNode=result.getFirst();
    m_isLeader=result.getSecond();
  }
 catch (  Exception e) {
    VoltDB.crashGlobalVoltDB("Failed to create Zookeeper node: " + e.getMessage(),false,e);
  }
}
