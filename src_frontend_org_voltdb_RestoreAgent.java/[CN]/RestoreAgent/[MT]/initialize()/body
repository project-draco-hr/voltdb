{
  try {
    Class<?> replayClass=MiscUtils.loadProClass("org.voltdb.CommandLogReinitiatorImpl","Command log replay",true);
    if (replayClass != null) {
      Constructor<?> constructor=replayClass.getConstructor(int.class,StartAction.class,ZooKeeper.class,int.class,String.class,int[].class,Set.class,long.class);
      m_replayAgent=(CommandLogReinitiator)constructor.newInstance(m_hostId,m_action,m_zk,m_allPartitions.length,m_clPath,m_allPartitions,m_liveHosts,RESTORE_TXNID + 1);
    }
  }
 catch (  Exception e) {
    VoltDB.crashGlobalVoltDB("Unable to instantiate command log reinitiator",true,e);
  }
  m_replayAgent.setCallback(this);
}
