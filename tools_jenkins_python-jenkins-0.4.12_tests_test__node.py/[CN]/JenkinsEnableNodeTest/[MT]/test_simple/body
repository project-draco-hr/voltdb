@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_simple(self, jenkins_mock):
    jenkins_mock.side_effect = [json.dumps(self.offline_node_info), None]
    self.j.enable_node('test node')
    self.assertEqual(jenkins_mock.call_args[0][0].get_full_url(), '{0}/computer/test%20node/toggleOffline?offlineMessage='.format(self.base_url))
    jenkins_mock.side_effect = [json.dumps(self.online_node_info)]
    node_info = self.j.get_node_info('test node')
    self.assertEqual(node_info, self.online_node_info)
    self._check_requests(jenkins_mock.call_args_list)
