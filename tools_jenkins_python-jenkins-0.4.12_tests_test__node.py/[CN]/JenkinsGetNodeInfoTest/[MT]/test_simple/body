@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_simple(self, jenkins_mock):
    jenkins_mock.side_effect = [json.dumps(self.node_info)]
    self.assertEqual(self.j.get_node_info('test node'), self.node_info)
    self.assertEqual(jenkins_mock.call_args[0][0].get_full_url(), self.make_url('computer/test%20node/api/json?depth=0'))
    self._check_requests(jenkins_mock.call_args_list)
