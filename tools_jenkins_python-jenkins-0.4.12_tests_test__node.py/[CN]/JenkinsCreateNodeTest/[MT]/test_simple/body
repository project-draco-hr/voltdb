@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_simple(self, jenkins_mock):
    jenkins_mock.side_effect = [None, None, json.dumps(self.node_info), json.dumps(self.node_info)]
    self.j.create_node('test node', exclusive=True)
    self.assertEqual(jenkins_mock.call_args_list[1][0][0].get_full_url().split('?')[0], self.make_url('computer/doCreateItem'))
    self.assertTrue(self.j.node_exists('test node'))
    self._check_requests(jenkins_mock.call_args_list)
