@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_failed(self, jenkins_mock):
    jenkins_mock.side_effect = [None, None, None, None]
    with self.assertRaises(jenkins.JenkinsException) as context_manager:
        self.j.create_node('test_node')
    self.assertEqual(jenkins_mock.call_args_list[1][0][0].get_full_url().split('?')[0], self.make_url('computer/doCreateItem'))
    self.assertEqual(str(context_manager.exception), 'create[test_node] failed')
    self._check_requests(jenkins_mock.call_args_list)
