{
  if (hostName == null) {
    hostName=src.hostName;
  }
  if (port == 0) {
    port=src.port;
  }
  if (identityFile == null) {
    identityFile=src.identityFile;
  }
  if (user == null) {
    user=src.user;
  }
  if (preferredAuthentications == null) {
    preferredAuthentications=src.preferredAuthentications;
  }
  if (batchMode == null) {
    batchMode=src.batchMode;
  }
}
