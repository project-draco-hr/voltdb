def test_jenkins_open_timeout(self):
    j = jenkins.Jenkins(('http://%s:%s' % self.server.server_address), None, None, timeout=0.1)
    request = jenkins.Request(('http://%s:%s/job/TestJob' % self.server.server_address))
    with testtools.ExpectedException(jenkins.TimeoutException):
        j.jenkins_open(request, add_crumb=False)
