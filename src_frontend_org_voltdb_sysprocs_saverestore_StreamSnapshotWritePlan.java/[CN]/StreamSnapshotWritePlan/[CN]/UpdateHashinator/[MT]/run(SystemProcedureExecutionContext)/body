{
  SNAP_LOG.debug("P" + context.getPartitionId() + " updating Java hashinator with new partitions: "+ m_newPartitions);
  TheHashinator.updateHashinator(TheHashinator.getConfiguredHashinatorType().hashinatorClass,m_txnId,m_javaHashinatorConfig);
  if (SNAP_LOG.isDebugEnabled()) {
    SNAP_LOG.debug("P" + context.getPartitionId() + " updated the hashinator with new partitions: "+ m_newPartitions);
  }
  Pair<TheHashinator.HashinatorType,byte[]> currentConfig=TheHashinator.getCurrentConfig();
  context.updateHashinator(currentConfig);
  context.setNumberOfPartitions(context.getNumberOfPartitions() + m_newPartitions.size());
}
