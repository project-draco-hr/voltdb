{
  String randomString="";
  if (exactLength) {
    for (int i=0; i < maxLength; i++)     randomString+=getChar();
  }
 else {
    double rand=Math.random();
    while ((double)randomString.length() / maxLength < rand) {
      randomString+=getChar();
      rand=Math.random();
    }
  }
  return randomString;
}
