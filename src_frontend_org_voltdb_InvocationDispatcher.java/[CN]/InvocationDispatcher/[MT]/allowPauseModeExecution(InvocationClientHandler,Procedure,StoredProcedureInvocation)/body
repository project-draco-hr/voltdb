{
  if (VoltDB.instance().getMode() != OperationMode.PAUSED || handler.isAdmin()) {
    return true;
  }
  if (procedure.getSystemproc() && ("@AdHoc".equals(invocation.procName) || "@AdHocSpForTest".equals(invocation.procName))) {
    return true;
  }
 else {
    return procedure.getReadonly();
  }
}
