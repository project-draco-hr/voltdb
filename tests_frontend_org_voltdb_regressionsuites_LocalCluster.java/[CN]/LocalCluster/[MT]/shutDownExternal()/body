{
  if (m_cluster != null) {
    for (    Process proc : m_cluster) {
      proc.destroy();
      int retval=proc.waitFor();
      if (retval != 0 && retval != 143) {
        System.out.println("External VoltDB process terminated abnormally with return: " + retval);
      }
    }
  }
  if (m_cluster != null)   m_cluster.clear();
}
