{
  Process proc=null;
  PipeToFile ptf=null;
synchronized (this) {
    proc=m_cluster.get(hostNum);
    ptf=m_pipes.get(hostNum);
    m_cluster.set(hostNum,null);
    m_pipes.set(hostNum,null);
  }
  if (proc != null)   proc.destroy();
  if (ptf != null) {
    new File(ptf.m_filename).delete();
  }
}
