{
  if ((path == null) || (path.length() == 0)) {
    return;
  }
  FileWriter fw=new FileWriter(path);
  fw.append(String.format("%d,%d,%d,%d,%d,%d,%d\n",stats.getStartTimestamp(),stats.getDuration(),stats.getInvocationsCompleted(),stats.kPercentileLatency(0.0),stats.kPercentileLatency(1.0),stats.kPercentileLatency(0.95),stats.kPercentileLatency(0.99)));
  fw.close();
}
