{
  if (callback instanceof ProcedureArgumentCacher) {
    ((ProcedureArgumentCacher)callback).setArgs(parameters);
  }
  ProcedureInvocation invocation=new ProcedureInvocation(originalTxnId,m_handle.getAndIncrement(),procName,parameters);
  return callProcedure(callback,m_expectedOutgoingMessageSize,invocation);
}
