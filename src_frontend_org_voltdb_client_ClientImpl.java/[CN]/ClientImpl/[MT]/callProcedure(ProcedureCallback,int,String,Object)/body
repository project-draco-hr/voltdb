{
  if (m_isShutdown) {
    return false;
  }
  if (callback == null) {
    callback=new NullCallback();
  }
 else   if (callback instanceof ProcedureArgumentCacher) {
    ((ProcedureArgumentCacher)callback).setArgs(parameters);
  }
  ProcedureInvocation invocation=new ProcedureInvocation(m_handle.getAndIncrement(),procName,parameters);
  if (m_blockingQueue) {
    while (!m_distributer.queue(invocation,callback,expectedSerializedSize,m_blessedThreadIds.contains(Thread.currentThread().getId()))) {
      try {
        backpressureBarrier();
      }
 catch (      InterruptedException e) {
        throw new java.io.InterruptedIOException("Interrupted while invoking procedure asynchronously");
      }
    }
    return true;
  }
 else {
    return m_distributer.queue(invocation,callback,expectedSerializedSize,m_blessedThreadIds.contains(Thread.currentThread().getId()));
  }
}
