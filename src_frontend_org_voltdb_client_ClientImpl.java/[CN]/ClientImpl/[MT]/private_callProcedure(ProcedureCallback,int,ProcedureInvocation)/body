{
  if (m_isShutdown) {
    return false;
  }
  if (callback == null) {
    callback=new NullCallback();
  }
  boolean isBlessed=m_blessedThreadIds.contains(Thread.currentThread().getId());
  if (m_blockingQueue) {
    while (!m_distributer.queue(invocation,callback,isBlessed)) {
      try {
        backpressureBarrier();
      }
 catch (      InterruptedException e) {
        throw new java.io.InterruptedIOException("Interrupted while invoking procedure asynchronously");
      }
    }
    return true;
  }
 else {
    return m_distributer.queue(invocation,callback,isBlessed);
  }
}
