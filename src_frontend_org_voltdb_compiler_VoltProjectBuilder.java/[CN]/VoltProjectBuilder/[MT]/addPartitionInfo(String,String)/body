{
  assert(m_partitionInfos.containsKey(tableName) == false);
  m_partitionInfos.put(tableName,partitionColumnName);
  String tempSQL="PARTITION TABLE " + tableName + " ON COLUMN "+ partitionColumnName+ ";";
  try {
    addLiteralSchema(tempSQL);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
