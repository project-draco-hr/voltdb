{
  Configuration config=new Configuration(args);
  try {
    if (!config.validate()) {
      config.usage();
      System.exit(-1);
    }
 else {
      initialize(config);
      instance().run();
    }
  }
 catch (  OutOfMemoryError e) {
    String errmsg="VoltDB Main thread: ran out of Java memory. This node will shut down.";
    VoltDB.crashLocalVoltDB(errmsg,false,e);
  }
}
