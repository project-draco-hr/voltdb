{
  return ImmutableList.<File>builder().add(new VoltFile(m_voltdbRoot,VoltDB.INITIALIZED_MARKER)).add(new VoltFile(m_voltdbRoot,VoltDB.INITIALIZED_PATHS)).add(new VoltFile(m_voltdbRoot,Constants.CONFIG_DIR)).add(new VoltFile(m_voltdbRoot,VoltDB.STAGED_MESH)).add(new VoltFile(m_voltdbRoot,VoltDB.TERMINUS_MARKER)).build();
