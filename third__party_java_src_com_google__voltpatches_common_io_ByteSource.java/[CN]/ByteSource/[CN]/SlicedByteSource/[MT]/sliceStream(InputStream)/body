{
  if (offset > 0) {
    try {
      ByteStreams.skipFully(in,offset);
    }
 catch (    Throwable e) {
      Closer closer=Closer.create();
      closer.register(in);
      try {
        throw closer.rethrow(e);
      }
  finally {
        closer.close();
      }
    }
  }
  return ByteStreams.limit(in,length);
}
