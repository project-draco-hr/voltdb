{
  Optional<Long> sizeIfKnown=sizeIfKnown();
  if (sizeIfKnown.isPresent() && sizeIfKnown.get() == 0L) {
    return true;
  }
  Closer closer=Closer.create();
  try {
    InputStream in=closer.register(openStream());
    return in.read() == -1;
  }
 catch (  Throwable e) {
    throw closer.rethrow(e);
  }
 finally {
    closer.close();
  }
}
