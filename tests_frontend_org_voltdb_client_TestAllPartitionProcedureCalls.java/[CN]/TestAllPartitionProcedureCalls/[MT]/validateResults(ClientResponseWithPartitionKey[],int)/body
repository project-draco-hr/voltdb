{
  assertTrue(responses.length == partitionCount);
  long total=0;
  for (  ClientResponseWithPartitionKey resp : responses) {
    VoltTable results=resp.response.getResults()[0];
    long count=results.fetchRow(0).getLong(0);
    assertTrue(count > 0);
    total+=count;
  }
  assertTrue(total == ROWS);
}
