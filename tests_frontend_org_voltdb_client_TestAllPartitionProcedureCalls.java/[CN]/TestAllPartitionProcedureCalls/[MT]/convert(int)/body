{
  if (number == 0) {
    return "zero";
  }
  String soFar;
  if (number % 100 < 20) {
    soFar=NUMS[number % 100];
    number/=100;
  }
 else {
    soFar=NUMS[number % 10];
    number/=10;
    soFar=TENS[number % 10] + soFar;
    number/=10;
  }
  if (number == 0) {
    return soFar.trim();
  }
  return (NUMS[number] + " hundred" + soFar).trim();
}
