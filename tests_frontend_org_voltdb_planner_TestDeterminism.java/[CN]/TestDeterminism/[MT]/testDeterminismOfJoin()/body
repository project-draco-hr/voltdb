{
  assertPlanDeterminism("select X.a, X.z, Y.z from tuniqcombo X, tunique Y order by X.a, X.c, X.b, Y.a;",ORDERED);
  assertPlanDeterminism("select X.a, X.z, Y.z from tuniqcombo X, tunique Y order by X.b, X.a, Y.a, X.c;",ORDERED);
  assertPlanDeterminism("select X.a, X.z, Y.z from tuniqcombo X, tunique Y order by X.z, X.a, X.c, X.b, Y.a;",ORDERED);
  assertPlanDeterminism("select X.a, X.z, Y.z, X.z + Y.z from tuniqcombo X, tunique Y order by 4, X.z, X.a, X.c, X.b, Y.a;",ORDERED);
}
