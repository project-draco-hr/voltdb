{
  for (  AbstractPlanNode child : m_children) {
    ArrayList<SchemaColumn> columns=child.getOutputSchema().getColumns();
    for (    SchemaColumn scol : columns) {
      if (scol.getType() == VoltType.STRING || scol.getType() == VoltType.VARBINARY) {
        if (scol.getSize() < 64) {
          return true;
        }
      }
    }
  }
  return false;
}
