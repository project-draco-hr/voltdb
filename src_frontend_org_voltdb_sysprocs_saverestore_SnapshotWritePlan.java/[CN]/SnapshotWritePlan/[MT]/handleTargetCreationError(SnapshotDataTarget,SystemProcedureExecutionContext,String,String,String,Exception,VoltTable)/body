{
  SNAP_LOG.warn("Failure creating snapshot target",ex);
  try {
    if (sdt != null) {
      m_targets.remove(sdt);
      sdt.close();
    }
  }
 catch (  Exception e) {
    SNAP_LOG.error("Failed to close snapshot data target after error: " + e.getMessage(),e);
  }
  StringWriter sw=new StringWriter();
  PrintWriter pw=new PrintWriter(sw);
  ex.printStackTrace(pw);
  pw.flush();
  result.addRow(context.getHostId(),hostname,tableName,"FAILURE","SNAPSHOT INITIATION OF " + file_nonce + "RESULTED IN IOException: \n"+ sw.toString());
}
