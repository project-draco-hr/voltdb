@staticmethod
def get(database_id):
    '\n        Get the deployment with specified database_id.\n        Args:\n            database_id (int): The first parameter.\n        Returns:\n            List of deployment information with specified database.\n        '
    if (database_id is None):
        return jsonify({'deployment': Global.DEPLOYMENT.values(), })
    else:
        deployment = Global.DEPLOYMENT.get(database_id)
        return jsonify({'deployment': deployment, })
