def get_jar_file_path():
    file_path = ''
    try:
        try:
            volt_jar = glob.glob(os.path.join(get_volt_jar_dir(), 'voltdb-*.jar'))
            if (len(volt_jar) > 0):
                file_path = volt_jar[0]
            else:
                print 'No voltdb jar file found.'
        except Exception as err:
            print err
    finally:
        return file_path
