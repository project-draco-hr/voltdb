def is_voltserver_running():
    for proc in psutil.process_iter():
        try:
            cmd = proc.cmdline()
            if (('-DVDMStarted=true' in cmd) and ('java' in cmd[0])):
                return True
        except (psutil.NoSuchProcess, psutil.ZombieProcess, psutil.AccessDenied) as e:
            pass
    return False
