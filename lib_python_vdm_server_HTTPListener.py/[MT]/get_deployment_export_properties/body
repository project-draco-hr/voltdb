def get_deployment_export_properties(export_xml, is_list):
    new_export = {}
    exports = []
    if (is_list is 'list'):
        for export in export_xml:
            new_export = {}
            for field in export:
                new_export[field] = export[field]
            exports.append(new_export)
    else:
        for field in export_xml:
            new_export[field] = export_xml[field]
        exports.append(new_export)
    return exports
