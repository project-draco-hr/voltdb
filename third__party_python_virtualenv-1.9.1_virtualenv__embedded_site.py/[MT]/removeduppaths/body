def removeduppaths():
    ' Remove duplicate entries from sys.path along with making them\n    absolute'
    L = []
    known_paths = set()
    for dir in sys.path:
        (dir, dircase) = makepath(dir)
        if (not (dircase in known_paths)):
            L.append(dir)
            known_paths.add(dircase)
    sys.path[:] = L
    return known_paths
