def setencoding():
    "Set the string encoding used by the Unicode implementation.  The\n    default is 'ascii', but if you're willing to experiment, you can\n    change this."
    encoding = 'ascii'
    if 0:
        import locale
        loc = locale.getdefaultlocale()
        if loc[1]:
            encoding = loc[1]
    if 0:
        encoding = 'undefined'
    if (encoding != 'ascii'):
        sys.setdefaultencoding(encoding)
