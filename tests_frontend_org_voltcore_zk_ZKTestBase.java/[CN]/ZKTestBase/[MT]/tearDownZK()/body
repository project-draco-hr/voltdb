{
  for (  ZooKeeper keeper : m_clients) {
    System.out.println("Shutting down client " + keeper);
    keeper.close();
  }
  m_clients.clear();
  for (  HostMessenger hm : m_messengers) {
    System.out.println("Shutting down messenger " + hm);
    if (hm != null) {
      hm.shutdown();
    }
  }
  m_messengers.clear();
}
