{
  if (m_socketChannel != null) {
    try {
      m_socketChannel.close();
    }
 catch (    final IOException e) {
      throw new RuntimeException(e);
    }
    m_socketChannel=null;
    m_socket=null;
  }
  if (m_eeProcess != null) {
    m_eeProcess.waitFor();
  }
  if (m_stdoutParser != null) {
    m_stdoutParser.join();
  }
}
