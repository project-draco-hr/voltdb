{
  m_data.clear();
  m_data.putInt(Commands.updateHashinator.m_id);
  m_data.putInt(type.typeId());
  m_data.putInt(config.length);
  m_data.put(config);
  try {
    m_data.flip();
    m_connection.write();
  }
 catch (  final Exception e) {
    System.out.println("Exception: " + e.getMessage());
    throw new RuntimeException(e);
  }
}
