{
  final ByteBuffer stringBytes=ByteBuffer.allocate(size);
  while (stringBytes.hasRemaining()) {
    int read=m_socketChannel.read(stringBytes);
    if (read == -1) {
      throw new EOFException();
    }
  }
  stringBytes.flip();
  final String retval=new String(stringBytes.array());
  return retval;
}
