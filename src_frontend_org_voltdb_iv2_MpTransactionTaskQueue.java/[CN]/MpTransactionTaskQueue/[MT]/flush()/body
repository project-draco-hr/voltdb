{
  int offered=0;
  if (m_backlog.isEmpty() || !m_backlog.getFirst().getTransactionState().isDone()) {
    return offered;
  }
  m_backlog.removeFirst();
  Iterator<TransactionTask> iter=m_backlog.iterator();
  if (iter.hasNext()) {
    TransactionTask task=iter.next();
    taskQueueOffer(task);
    ++offered;
  }
  return offered;
}
