{
  if (object instanceof BigDecimal) {
    final BigDecimal bd1=(BigDecimal)object;
    final BigDecimal bd2=ExportDecoderBase.decodeDecimal(fds);
    if (bd1.compareTo(bd2) != 0) {
      System.out.println("compare DECIMAL failed: " + bd1 + " != "+ bd2);
      return false;
    }
 else {
      return true;
    }
  }
  return false;
}
