{
  final String simpleSchema1="create table mytable  (pkey integer, column2_integer integer);\n" + "alter table mytable drop column pkey;\n" + "create procedure from class " + MyTableProcedure.class.getCanonicalName() + ";\n";
  final String projectPath=getSimpleProjectPathForDDL(simpleSchema1);
  final VoltCompiler compiler=new VoltCompiler();
  final boolean success=compiler.compileWithProjectXML(projectPath,testout_jar);
  assertTrue(success);
}
