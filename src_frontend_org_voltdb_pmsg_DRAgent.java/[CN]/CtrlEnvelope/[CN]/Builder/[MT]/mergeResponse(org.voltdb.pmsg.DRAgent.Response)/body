{
  if (responseBuilder_ == null) {
    if (((bitField0_ & 0x00000020) == 0x00000020) && response_ != org.voltdb.pmsg.DRAgent.Response.getDefaultInstance()) {
      response_=org.voltdb.pmsg.DRAgent.Response.newBuilder(response_).mergeFrom(value).buildPartial();
    }
 else {
      response_=value;
    }
    onChanged();
  }
 else {
    responseBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00000020;
  return this;
}
