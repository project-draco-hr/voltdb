{
  if (resetBuilder_ == null) {
    if (((bitField0_ & 0x00000008) == 0x00000008) && reset_ != org.voltdb.pmsg.DRAgent.Reset.getDefaultInstance()) {
      reset_=org.voltdb.pmsg.DRAgent.Reset.newBuilder(reset_).mergeFrom(value).buildPartial();
    }
 else {
      reset_=value;
    }
    onChanged();
  }
 else {
    resetBuilder_.mergeFrom(value);
  }
  bitField0_|=0x00000008;
  return this;
}
