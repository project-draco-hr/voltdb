{
  if (pauseBuilder_ == null) {
    if (((bitField0_ & 0x00000008) == 0x00000008) && pause_ != org.voltdb.pmsg.DRAgent.Pause.getDefaultInstance()) {
      pause_=org.voltdb.pmsg.DRAgent.Pause.newBuilder(pause_).mergeFrom(value).buildPartial();
