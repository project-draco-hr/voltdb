{
  getSerializedSize();
  if (((bitField0_ & 0x00000001) == 0x00000001)) {
    output.writeEnum(1,mode_.getNumber());
  }
  if (((bitField0_ & 0x00000002) == 0x00000002)) {
    output.writeFixed64(2,snapshotTimestamp_);
  }
  for (int i=0; i < nodeInfo_.size(); i++) {
    output.writeMessage(3,nodeInfo_.get(i));
  }
  if (((bitField0_ & 0x00000004) == 0x00000004)) {
    output.writeInt32(4,globalPartitionCount_);
  }
  for (int i=0; i < partitionInfo_.size(); i++) {
    output.writeMessage(5,partitionInfo_.get(i));
  }
  if (((bitField0_ & 0x00000008) == 0x00000008)) {
    output.writeInt32(6,status_);
  }
  getUnknownFields().writeTo(output);
}
