{
  rangeIndex=index;
  isJoinIndex=isJoin;
switch (e.getType()) {
case OpTypes.NOT:
    indexCondition=e;
  break;
case OpTypes.IS_NULL:
indexEndCondition=e;
break;
case OpTypes.EQUAL:
indexCondition=e;
indexEndCondition=indexCondition;
break;
case OpTypes.GREATER:
case OpTypes.GREATER_EQUAL:
indexCondition=e;
break;
case OpTypes.SMALLER:
case OpTypes.SMALLER_EQUAL:
indexEndCondition=e;
break;
default :
Error.runtimeError(ErrorCode.U_S0500,"Expression");
}
}
