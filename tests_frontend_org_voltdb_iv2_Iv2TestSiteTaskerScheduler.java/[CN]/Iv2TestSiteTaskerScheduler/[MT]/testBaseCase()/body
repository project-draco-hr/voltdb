{
  SiteTaskerScheduler sts=new SiteTaskerScheduler();
  Task p1=new Task(1);
  Task p2=new Task(2);
  sts.offer(p1);
  sts.offer(p2);
  assertSame("Basecase-1",sts.poll(),p1);
  assertSame("Basecase-2",sts.poll(),p2);
  sts.offer(p2);
  sts.offer(p1);
  assertSame("Basecase-3",sts.poll(),p1);
  assertSame("Basecase-4",sts.poll(),p2);
}
