{
  try {
    if (_requestLog != null)     _requestLog.stop();
  }
 catch (  Exception e) {
    Log.warn(e);
  }
  if (getServer() != null)   getServer().getContainer().update(this,_requestLog,requestLog,"logimpl",true);
  _requestLog=requestLog;
  try {
    if (isStarted() && (_requestLog != null))     _requestLog.start();
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
