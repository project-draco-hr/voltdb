{
  if (_requestLog != null) {
    if (getServer() != null && getServer() != server)     getServer().getContainer().update(this,_requestLog,null,"logimpl",true);
    super.setServer(server);
    if (server != null && server != getServer())     server.getContainer().update(this,null,_requestLog,"logimpl",true);
  }
 else   super.setServer(server);
}
