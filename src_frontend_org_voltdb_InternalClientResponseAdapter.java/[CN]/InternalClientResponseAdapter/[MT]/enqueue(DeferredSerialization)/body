{
  try {
    ByteBuffer buf=null;
synchronized (this) {
      final int serializedSize=ds.getSerializedSize();
      buf=ByteBuffer.allocate(serializedSize);
      ds.serialize(buf);
    }
    enqueue(buf);
  }
 catch (  IOException e) {
    VoltDB.crashLocalVoltDB("enqueue() in InternalClientResponseAdapter throw an exception",true,e);
  }
}
