{
synchronized (this) {
switch (_state) {
case __DISPATCHED:
case __REDISPATCHED:
      throw new IllegalStateException(getStatusString());
default :
    _state=__IDLE;
}
_initial=true;
_resumed=false;
_expired=false;
_responseWrapped=false;
cancelTimeout();
_timeoutMs=DEFAULT_TIMEOUT;
_continuationListeners=null;
}
}
