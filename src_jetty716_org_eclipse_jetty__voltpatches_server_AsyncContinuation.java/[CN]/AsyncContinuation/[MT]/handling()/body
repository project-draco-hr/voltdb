{
synchronized (this) {
    _responseWrapped=false;
switch (_state) {
case __IDLE:
      _initial=true;
    _state=__DISPATCHED;
  return true;
case __COMPLETING:
_state=__UNCOMPLETED;
return false;
case __ASYNCWAIT:
return false;
case __REDISPATCH:
_state=__REDISPATCHED;
return true;
default :
throw new IllegalStateException(this.getStatusString());
}
}
}
