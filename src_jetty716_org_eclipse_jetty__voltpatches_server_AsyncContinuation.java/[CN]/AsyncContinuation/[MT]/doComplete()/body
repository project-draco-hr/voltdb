{
  final List<ContinuationListener> listeners;
synchronized (this) {
switch (_state) {
case __UNCOMPLETED:
      _state=__COMPLETED;
    listeners=_continuationListeners;
  break;
default :
listeners=null;
throw new IllegalStateException(this.getStatusString());
}
}
if (listeners != null) {
for (int i=0; i < listeners.size(); i++) {
try {
listeners.get(i).onComplete(this);
}
 catch (Exception e) {
Log.warn(e);
}
}
}
}
