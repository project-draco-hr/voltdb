{
  channel.connect();
  channel.close();
  channel.connect();
  ByteBuffer buf=ByteBuffer.allocate(Integer.SIZE);
  buf.putInt(10);
  buf.position(0);
  channel.write(buf);
  channel.close();
  channel.connect();
  buf=ByteBuffer.allocate(Integer.SIZE);
  buf.putInt(-1);
  buf.position(0);
  channel.write(buf);
  channel.close();
}
