{
  long txnId=m_idManager.getNextUniqueTransactionId();
  for (  long initiatorId : m_hsIds) {
    HeartbeatMessage heartbeat=new HeartbeatMessage(m_hsId,txnId,m_safetyState.getNewestSafeTxnIdForExecutorBySiteId(initiatorId));
    try {
      m_mailbox.send(initiatorId,heartbeat);
    }
 catch (    MessagingException e) {
      throw new RuntimeException(e);
    }
  }
}
