{
  if (m_handledFailedSites.contains(rejoiningAgreementSite)) {
    m_recoveryLog.info("Igoring join of site " + MiscUtils.hsIdToString(rejoiningAgreementSite) + " because it is known to be failed");
    return;
  }
  m_safetyState.addState(rejoiningAgreementSite);
  m_txnQueue.ensureInitiatorIsKnown(rejoiningAgreementSite);
  m_hsIds.add(rejoiningAgreementSite);
  m_recoveryLog.info("Unfaulting site " + rejoiningAgreementSite + " known failed sites "+ MiscUtils.hsIdCollectionToString(m_pendingFailedSites)+ " handled failed sites "+ MiscUtils.hsIdCollectionToString(m_handledFailedSites)+ " active sites "+ MiscUtils.hsIdCollectionToString(m_hsIds));
}
