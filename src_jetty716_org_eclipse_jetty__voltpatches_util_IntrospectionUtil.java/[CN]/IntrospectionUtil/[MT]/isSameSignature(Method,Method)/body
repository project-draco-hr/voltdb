{
  if (methodA == null)   return false;
  if (methodB == null)   return false;
  List parameterTypesA=Arrays.asList(methodA.getParameterTypes());
  List parameterTypesB=Arrays.asList(methodB.getParameterTypes());
  if (methodA.getName().equals(methodB.getName()) && parameterTypesA.containsAll(parameterTypesB))   return true;
  return false;
}
