{
  return new AdHocPlannerWork(orig.replySiteId,orig.clientHandle,orig.connectionId,orig.adminConnection,(Connection)orig.clientData,orig.sqlBatchText,orig.sqlStatements,orig.userParamSet,null,orig.isExplainWork,orig.inferPartitioning,orig.userPartitionKey,orig.invocationType,orig.originalTxnId,orig.originalUniqueId,orig.onReplica,orig.useAdhocDDL,completionHandler);
}
