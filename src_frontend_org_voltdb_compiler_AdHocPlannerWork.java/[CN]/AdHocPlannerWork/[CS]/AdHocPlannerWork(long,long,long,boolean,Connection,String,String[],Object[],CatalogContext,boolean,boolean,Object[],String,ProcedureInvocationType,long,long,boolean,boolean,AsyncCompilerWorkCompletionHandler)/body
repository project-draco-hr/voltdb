{
  super(replySiteId,false,clientHandle,connectionId,clientConnection == null ? "" : clientConnection.getHostnameAndIPAndPort(),adminConnection,clientConnection,invocationName,type,originalTxnId,originalUniqueId,onReplica,useAdhocDDL,completionHandler);
  this.sqlBatchText=sqlBatchText;
  this.sqlStatements=sqlStatements;
  this.userParamSet=userParamSet;
  this.catalogContext=context;
  this.isExplainWork=isExplain;
  this.inferPartitioning=inferPartitioning;
  this.userPartitionKey=userPartitionKey;
}
