{
  int index=getTable(tableName).getColumns().size();
  String table_path=getTable(tableName).getPath();
  m_catalog.execute("add " + table_path + " columns "+ columnName);
  String column_path=getColumnFromTable(tableName,columnName).getPath();
  m_catalog.execute("set " + column_path + " index "+ index);
  m_catalog.execute("set " + column_path + " type "+ columnType.getValue());
  m_catalog.execute("set " + column_path + " nullable "+ String.valueOf(isNullable));
  m_catalog.execute("set " + column_path + " name \""+ columnName+ "\"");
  m_catalog.execute("set " + column_path + " defaultvalue \""+ defaultValue+ "\"");
  m_catalog.execute("set " + column_path + " defaulttype "+ defaultType.getValue());
  return m_catalog;
}
