{
  m_catalog.execute("add " + getCluster().getPath() + " sites "+ siteId);
  m_catalog.execute("set " + getSite(siteId).getPath() + " host "+ getHost(hostId).getPath());
  m_catalog.execute("set " + getSite(siteId).getPath() + " isexec "+ isExec);
  String partition_path="null";
  if (isExec) {
    partition_path=getPartition(partitionId).getPath();
  }
  m_catalog.execute("set " + getSite(siteId).getPath() + " partition "+ partition_path);
  return m_catalog;
}
