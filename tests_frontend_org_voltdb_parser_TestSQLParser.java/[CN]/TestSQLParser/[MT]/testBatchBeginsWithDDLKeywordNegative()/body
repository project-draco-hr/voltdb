{
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("insert into t values (47);"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("delete from t where i = 9;"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("upsert into t values (32);"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("update t set i = 70 where i > 69;"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("-- create table was done earlier...\n" + "update t set i = 70 where i > 69;"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("// create table was done earlier...\n" + "update t set i = 70 where i > 69;"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("/* create table was done earlier... */\n" + "update t set i = 70 where i > 69;"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("load classes foo-bar.jar"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("remove classes foo-bar.jar"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("exec SelectAllRowsWithKey 10;"));
  assertFalse(SQLParser.batchBeginsWithDDLKeyword("file \"mysqlcommands.sql\";"));
}
