{
  BufferedReader reader=m_clientReaders.get(partId);
  String line=null;
  do {
    if (reader == null) {
      reader=openNextClientFile(partId);
      m_clientReaders.put(partId,reader);
    }
    if (reader == null)     break;
    line=reader.readLine();
    if (line == null) {
      try {
        reader.close();
      }
 catch (      Exception ignoreIt) {
      }
      ;
      reader=null;
    }
  }
 while (line == null);
  return line;
}
