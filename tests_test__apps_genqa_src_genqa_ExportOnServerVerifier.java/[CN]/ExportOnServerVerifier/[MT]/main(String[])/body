{
  ExportOnServerVerifier verifier=new ExportOnServerVerifier();
  try {
    verifier.verify(args);
  }
 catch (  IOException e) {
    e.printStackTrace(System.err);
    System.exit(-1);
  }
catch (  ValidationErr e) {
    System.err.println("Validation error: " + e.toString());
    System.exit(-1);
  }
  System.exit(0);
}
