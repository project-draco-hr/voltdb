{
  if (m_tableName == null) {
    String tableName="";
    if (m_tableIdx != 0) {
      assert(m_tableIdx == 1);
      tableName+="inner table.";
    }
    return tableName + "column:" + m_columnIndex;
  }
  if (m_tableName.equals(impliedTableName)) {
    return m_columnName;
  }
 else {
    return m_tableName + "." + m_columnName;
  }
}
