{
  if (table == null) {
    return;
  }
  Column column=table.getColumns().getIgnoreCase(m_columnName);
  if (column == null) {
    return;
  }
  m_tableName=table.getTypeName();
  m_columnIndex=column.getIndex();
  setValueType(VoltType.get((byte)column.getType()));
  setValueSize(column.getSize());
  m_resolved=true;
}
