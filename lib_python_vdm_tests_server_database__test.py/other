import unittest
import requests
import socket
__host_name__ = socket.gethostname()
__host_or_ip__ = socket.gethostbyname(__host_name__)
__url__ = (('http://' + __host_or_ip__) + ':8000/api/1.0/databases/')
if (__name__ == '__main__'):
    unittest.main()
