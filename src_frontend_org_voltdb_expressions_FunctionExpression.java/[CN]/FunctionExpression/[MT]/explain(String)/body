{
  String result=m_name + "(";
  String connector;
  if (FunctionForVoltDB.isUnitFunction(m_functionId)) {
    result+=m_alias.substring(m_name.length() + 1);
    connector=", ";
  }
 else {
    connector="";
  }
  for (  AbstractExpression arg : m_args) {
    result+=connector + arg.explain(impliedTableName);
    connector=", ";
  }
  result+=")";
  return result;
}
