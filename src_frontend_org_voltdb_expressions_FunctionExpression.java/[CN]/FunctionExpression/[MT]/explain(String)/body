{
  assert(m_name != null);
  String result=m_name;
  if (!m_args.isEmpty()) {
    String connector="(";
    if (m_impliedArgument != null) {
      result+=connector + m_impliedArgument;
      connector=", ";
    }
    for (    AbstractExpression arg : m_args) {
      result+=connector + arg.explain(impliedTableName);
      connector=", ";
    }
    result+=")";
  }
  return result;
}
