{
  assert(m_sampler != null);
  m_shouldContinueSamplerThread.set(false);
  try {
    m_sampler.join();
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
  assert(!m_sampler.isAlive());
  m_sampling=false;
  try {
    Thread.sleep(1000);
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
}
