{
  ArrayList<Long> all_sites=new ArrayList<Long>();
  for (  int p : partitions) {
    ArrayList<Long> sites=getLiveSitesForPartition(p);
    for (    long site : sites) {
      all_sites.add(site);
    }
  }
  long[] retval=new long[all_sites.size()];
  for (int i=0; i < all_sites.size(); i++) {
    retval[i]=all_sites.get(i);
  }
  return retval;
}
