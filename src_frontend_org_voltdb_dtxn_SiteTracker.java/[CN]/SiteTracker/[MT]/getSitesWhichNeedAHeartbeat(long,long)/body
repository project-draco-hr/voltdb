{
  int index=0;
  for (  Entry<Integer,Long> e : m_lastHeartbeatTime.entrySet()) {
    long siteTime=e.getValue();
    if ((time - siteTime) >= timeout)     m_tempOldSitesScratch[index++]=e.getKey();
  }
  int[] retval=new int[index];
  for (int i=0; i < index; i++)   retval[i]=m_tempOldSitesScratch[i];
  return retval;
}
