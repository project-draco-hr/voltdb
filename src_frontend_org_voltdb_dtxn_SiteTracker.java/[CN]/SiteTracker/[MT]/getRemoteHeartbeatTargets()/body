{
  int localhost=VoltDB.instance().getHostMessenger().getHostId();
  Set<Integer> hosts=m_mailboxTracker.getAllHosts();
  long[][] retval=new long[hosts.size() - 1][];
  int i=0;
  for (  int host : hosts) {
    if (host != localhost) {
      retval[i++]=longListToArray(m_mailboxTracker.getSitesForHost(host));
    }
  }
  return retval;
}
