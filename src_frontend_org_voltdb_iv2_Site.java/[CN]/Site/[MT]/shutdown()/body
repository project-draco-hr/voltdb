{
  try {
    if (m_hsql != null) {
      HsqlBackend.shutdownInstance();
    }
    if (m_ee != null) {
      m_ee.release();
    }
    if (m_snapshotter != null) {
      m_snapshotter.shutdown();
    }
  }
 catch (  InterruptedException e) {
    hostLog.warn("Interrupted shutdown execution site.",e);
  }
}
