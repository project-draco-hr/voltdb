{
  setUpSites(1,2,1);
  WorkUnit w=new WorkUnit(work,new int[]{},false);
  assertTrue(w.allDependenciesSatisfied());
  assertEquals(work,w.getPayload());
  assertNull(w.getDependencies());
  assertNull(w.getDependency(0));
  w=new WorkUnit(work,null,false);
  assertTrue(w.allDependenciesSatisfied());
}
