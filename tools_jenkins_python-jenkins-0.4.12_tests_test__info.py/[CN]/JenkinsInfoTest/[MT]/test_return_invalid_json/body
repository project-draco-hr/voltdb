@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_return_invalid_json(self, jenkins_mock):
    jenkins_mock.return_value = 'not valid JSON'
    with self.assertRaises(jenkins.JenkinsException) as context_manager:
        self.j.get_info()
    self.assertEqual(jenkins_mock.call_args[0][0].get_full_url(), self.make_url('api/json'))
    self.assertEqual(str(context_manager.exception), 'Could not parse JSON info for server[{0}/]'.format(self.base_url))
    self._check_requests(jenkins_mock.call_args_list)
