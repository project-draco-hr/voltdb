{
  EndTime.set(System.currentTimeMillis());
  if (StartTime.get() == Long.MAX_VALUE)   StartTime.set(EndTime.get() - executionDuration);
  cnt.incrementAndGet();
  tot.addAndGet(executionDuration);
  if (min.get() > executionDuration)   min.set(executionDuration);
  if (max.get() < executionDuration)   max.set(executionDuration);
  lat.incrementAndGet(Math.min((int)(executionDuration / 25l),8));
  if (!success)   err.incrementAndGet();
}
