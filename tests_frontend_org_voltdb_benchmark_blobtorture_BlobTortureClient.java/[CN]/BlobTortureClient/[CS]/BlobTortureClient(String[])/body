{
  super(args);
  int blobSize=1048576;
  for (  String arg : args) {
    String[] parts=arg.split("=",2);
    if (parts.length == 1) {
      continue;
    }
 else     if (parts[1].startsWith("${")) {
      continue;
    }
 else     if (parts[0].equals("blobsize")) {
      blobSize=Integer.parseInt(parts[1]);
    }
  }
  m_blobSize=blobSize;
}
