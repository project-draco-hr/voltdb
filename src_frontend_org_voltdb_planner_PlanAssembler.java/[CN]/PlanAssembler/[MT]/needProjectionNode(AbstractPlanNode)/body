{
  if ((root.getPlanNodeType() == PlanNodeType.DISTINCT) || (root.getPlanNodeType() == PlanNodeType.PROJECTION)) {
    return false;
  }
  assert(!m_parsedSelect.hasComplexAgg());
  if ((root.getPlanNodeType() == PlanNodeType.AGGREGATE) || (root.getPlanNodeType() == PlanNodeType.HASHAGGREGATE)) {
    if (!m_parsedSelect.mvFixInfo.finalNeedFix) {
      return false;
    }
  }
  if (m_parsedSelect.hasComplexGroupby()) {
    return false;
  }
  return true;
}
