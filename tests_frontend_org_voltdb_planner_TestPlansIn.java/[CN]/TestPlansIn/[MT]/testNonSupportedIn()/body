{
  failToCompile("select * from new_order where no_w_id in ( )"," unexpected ");
  failToCompile("select * from new_order where no_w_id in ( ) and no_o_id > 1"," unexpected ");
  failToCompile("select * from new_order where no_w_id in ( );"," unexpected ");
  failToCompile("select * from new_order where no_w_id in ( ) and no_o_id > 1;"," unexpected ");
  failToCompile("select * from new_order where no_w_id in (select w_id from warehouse);","VoltDB does not support subqueries");
  failToCompile("select * from new_order where no_w_id <> (5, 7, 8);","row column count mismatch");
  failToCompile("select * from new_order where exists (select w_id from warehouse);","VoltDB does not support subqueries");
  failToCompile("select * from new_order where not exists (select w_id from warehouse);","VoltDB does not support subqueries");
}
