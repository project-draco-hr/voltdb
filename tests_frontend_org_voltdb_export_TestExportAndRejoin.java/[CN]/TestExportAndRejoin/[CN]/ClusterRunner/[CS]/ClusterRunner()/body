{
  m_cluster=new LocalCluster("exportandrejoin.jar",1,HOST_COUNT,1,BackendTarget.NATIVE_EE_JNI);
  m_cluster.setHasLocalServer(false);
  VoltProjectBuilder project=getBuilderForTest();
  boolean compile=m_cluster.compile(project);
  MiscUtils.copyFile(project.getPathToDeployment(),Configuration.getPathToCatalogForTest("export-ddl-sans-nonulls.xml"));
  assertTrue(compile);
  m_cluster.startUp();
}
