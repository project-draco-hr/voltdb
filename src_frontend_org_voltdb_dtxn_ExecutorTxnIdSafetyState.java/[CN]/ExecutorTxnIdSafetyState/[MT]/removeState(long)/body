{
  SiteState ss=m_stateBySite.remove(executorSiteId);
  if (ss == null)   return;
  PartitionState ps=ss.partition;
  for (  SiteState s : ps.sites) {
    if (s.hsId == ss.hsId) {
      ps.sites.remove(s);
      break;
    }
  }
  m_stateBySite.remove(executorSiteId);
  m_stateToPartitionMap.remove(executorSiteId);
}
