{
  try {
    for (    String host : hosts)     if (!host.equals(""))     client.createConnection(host);
  }
 catch (  UnknownHostException e) {
    e.printStackTrace();
    System.exit(-1);
  }
catch (  ConnectException e) {
    e.printStackTrace();
    System.exit(-1);
  }
catch (  IOException e) {
    e.printStackTrace();
    System.exit(-1);
  }
}
