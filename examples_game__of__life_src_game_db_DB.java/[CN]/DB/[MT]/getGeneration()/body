{
  try {
    int defaultPK=0;
    return client.callProcedure(GetGeneration.class.getSimpleName(),defaultPK).getResults()[0].fetchRow(0).getLong(0);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  ProcCallException e) {
    System.out.println("Getting generation failed: " + e.getMessage());
  }
  return 0;
}
