{
  try {
    VoltTable results=client.callProcedure(GetIDs.class.getSimpleName()).getResults()[0];
    boolean[] ids=new boolean[results.getRowCount()];
    for (int i=0; i < ids.length; i++)     if (results.fetchRow(i).getLong(1) == 0)     ids[(int)results.fetchRow(i).getLong(0)]=false;
 else     ids[(int)results.fetchRow(i).getLong(0)]=true;
    return ids;
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  ProcCallException e) {
    System.out.println("Getting ids failed: " + e.getMessage());
  }
  return null;
}
