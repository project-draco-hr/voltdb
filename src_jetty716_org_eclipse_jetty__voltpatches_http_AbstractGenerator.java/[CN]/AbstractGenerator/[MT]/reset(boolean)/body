{
  _state=STATE_HEADER;
  _status=0;
  _version=HttpVersions.HTTP_1_1_ORDINAL;
  _reason=null;
  _last=false;
  _head=false;
  _noContent=false;
  _persistent=null;
  _contentWritten=0;
  _contentLength=HttpTokens.UNKNOWN_CONTENT;
  _date=null;
  if (_buffer != null)   _buffers.returnBuffer(_buffer);
  _buffer=null;
  if (returnBuffers) {
    if (_header != null)     _buffers.returnBuffer(_header);
    _header=null;
  }
 else   if (_header != null)   _header.clear();
  _content=null;
  _method=null;
}
