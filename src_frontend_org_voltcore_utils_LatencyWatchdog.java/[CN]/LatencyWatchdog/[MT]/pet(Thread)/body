{
  if (!m_enable)   return;
  if (m_latencyMap.containsKey(t)) {
    m_latencyMap.put(t,System.currentTimeMillis());
  }
 else {
    m_latencyMap.put(t,System.currentTimeMillis());
    executor.scheduleWithFixedDelay(new WatchdogCallback(t),WATCHDOG_DELAY,WATCHDOG_DELAY,TimeUnit.MILLISECONDS);
  }
}
