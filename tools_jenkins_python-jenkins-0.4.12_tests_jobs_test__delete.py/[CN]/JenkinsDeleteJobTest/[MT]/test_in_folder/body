@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_in_folder(self, jenkins_mock):
    jenkins_mock.side_effect = [None, jenkins.NotFoundException()]
    self.j.delete_job(u'a Folder/Test Job')
    self.assertEqual(jenkins_mock.call_args_list[0][0][0].get_full_url(), self.make_url('job/a%20Folder/job/Test%20Job/doDelete'))
    self._check_requests(jenkins_mock.call_args_list)
