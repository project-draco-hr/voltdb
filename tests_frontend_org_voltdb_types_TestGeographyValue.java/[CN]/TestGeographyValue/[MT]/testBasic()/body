{
  GeographyValue geog;
  List<PointType> outerLoop=Arrays.asList(new PointType(32.305f,-64.751f),new PointType(25.244f,-80.437f),new PointType(18.476f,-66.371f),new PointType(32.305f,-64.751f));
  List<PointType> innerLoop=Arrays.asList(new PointType(28.066f,-68.874f),new PointType(25.361f,-68.855f),new PointType(28.376f,-73.381f),new PointType(28.066f,-68.874f));
  geog=new GeographyValue(Arrays.asList(outerLoop,innerLoop));
  assertEquals("POLYGON((32.305 -64.751, 25.244 -80.437, 18.476 -66.371, 32.305 -64.751), " + "(28.066 -68.874, 25.361 -68.855, 28.376 -73.381, 28.066 -68.874))",geog.toString());
  geog=new GeographyValue("POLYGON((32.305 -64.751, 25.244 -80.437, 18.476 -66.371, 32.305 -64.751), " + "(28.066 -68.874, 25.361 -68.855, 28.376 -73.381, 28.066 -68.874))");
  assertEquals("POLYGON((32.305 -64.751, 25.244 -80.437, 18.476 -66.371, 32.305 -64.751), " + "(28.066 -68.874, 25.361 -68.855, 28.376 -73.381, 28.066 -68.874))",geog.toString());
  assertEquals(76,geog.getLengthInBytes());
}
