{
  if (toType == VoltType.INVALID || fromType == VoltType.INVALID) {
    return false;
  }
 else   if (toType == fromType) {
    return true;
  }
  assert(toType != fromType);
  if (isArray) {
    if (arrayTypeImplicityConvertible(fromType,toType)) {
      return true;
    }
 else {
      if (fromType != VoltType.TINYINT) {
        return false;
      }
      fromType=VoltType.VARBINARY;
    }
  }
switch (toType) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case FLOAT:
case TIMESTAMP:
case DECIMAL:
switch (fromType) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case FLOAT:
case TIMESTAMP:
case DECIMAL:
case STRING:
      return true;
default :
    return false;
}
case STRING:
switch (fromType) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case FLOAT:
case TIMESTAMP:
case DECIMAL:
case VARBINARY:
return true;
default :
return false;
}
case VARBINARY:
case GEOGRAPHY:
case GEOGRAPHY_POINT:
default :
return false;
}
}
