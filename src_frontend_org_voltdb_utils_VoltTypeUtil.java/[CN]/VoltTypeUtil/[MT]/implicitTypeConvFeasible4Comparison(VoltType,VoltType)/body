{
  if (to == VoltType.INVALID || from == VoltType.INVALID) {
    return false;
  }
 else   if (to == from) {
    return true;
  }
  assert(to != from);
switch (to) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case FLOAT:
case TIMESTAMP:
case DECIMAL:
switch (from) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case FLOAT:
case TIMESTAMP:
case DECIMAL:
      return true;
default :
    return false;
}
case STRING:
case VARBINARY:
case GEOGRAPHY:
case GEOGRAPHY_POINT:
default :
return false;
}
}
