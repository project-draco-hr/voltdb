{
  if (toType == VoltType.INVALID || fromType == VoltType.INVALID) {
    return false;
  }
 else   if (toType == fromType) {
    return true;
  }
  assert(toType != fromType);
switch (toType) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case FLOAT:
case TIMESTAMP:
case DECIMAL:
switch (fromType) {
case TINYINT:
case SMALLINT:
case INTEGER:
case BIGINT:
case FLOAT:
case TIMESTAMP:
case DECIMAL:
      return true;
default :
    return false;
}
case STRING:
case VARBINARY:
case GEOGRAPHY:
case GEOGRAPHY_POINT:
default :
return false;
}
}
