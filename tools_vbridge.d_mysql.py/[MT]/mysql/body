@VOLT.Command(description='VoltDB quick start from a live MySQL database.', description2=('\nRun from a project directory where new files can be generated.\nEdit %s to specify database parameters and generation options.\nUse the "config" and "show" sub-commands to update and view the configuration.\n' % environment.config_name))
def mysql(runner):
    config = get_config(runner, 'uri', 'partition_table')
    mysqlutil.generate_schema(config.uri, config.partition_table)
