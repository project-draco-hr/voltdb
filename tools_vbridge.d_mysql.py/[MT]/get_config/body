def get_config(runner, *keys):


    class O(object):
        pass
    o = O()
    missing = []
    for name in keys:
        key = ('mysql.%s' % name)
        value = runner.config.get(key)
        if value:
            setattr(o, name, value)
        else:
            missing.append(key)
    if missing:
        utility.abort(('Configuration properties are missing from "%s".' % runner.config.path), 'Use the "config" and "show" subcommands to work with the configuration.', 'Missing properties:', missing)
    return o
