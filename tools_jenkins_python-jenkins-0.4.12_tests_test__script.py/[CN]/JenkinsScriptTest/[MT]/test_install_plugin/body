@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_install_plugin(self, jenkins_mock):
    'Installation of plugins is done with the run_script method\n        '
    j = jenkins.Jenkins(self.make_url(''), 'test', 'test')
    j.install_plugin('jabber')
    self.assertEqual(jenkins_mock.call_args[0][0].get_full_url(), self.make_url('scriptText'))
    self._check_requests(jenkins_mock.call_args_list)
