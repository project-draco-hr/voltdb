{
  System.out.print(HORIZONTAL_RULE);
  System.out.println(" Starting Benchmark");
  System.out.println(HORIZONTAL_RULE);
  state.scheduler.scheduleWithFixedDelay(reporter,state.config.displayinterval,state.config.displayinterval,TimeUnit.SECONDS);
  state.scheduler.scheduleWithFixedDelay(partitionTracker,1,1,TimeUnit.SECONDS);
  state.scheduler.execute(deleter);
  state.scheduler.scheduleWithFixedDelay(maxTracker,5000,10,TimeUnit.MILLISECONDS);
  inserter.run();
  state.shutdown();
}
