{
  sourceResultSet.checkColumnBounds(column);
  VoltType type=sourceResultSet.table.getColumnType(column);
switch (type) {
case TINYINT:
    return 1;
case SMALLINT:
  return 2;
case INTEGER:
return 4;
case BIGINT:
return 8;
case FLOAT:
return 8;
case DECIMAL:
return 38;
case TIMESTAMP:
return 32;
case STRING:
return 128;
default :
throw SQLError.get(SQLError.TRANSLATION_NOT_FOUND,type);
}
}
