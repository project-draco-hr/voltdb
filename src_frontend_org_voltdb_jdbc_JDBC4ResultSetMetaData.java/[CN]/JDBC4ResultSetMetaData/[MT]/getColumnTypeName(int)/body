{
  sourceResultSet.checkColumnBounds(column);
  VoltType type=sourceResultSet.table.getColumnType(column);
switch (type) {
case TINYINT:
    return "TINYINT";
case SMALLINT:
  return "SMALLINT";
case INTEGER:
return "INT";
case BIGINT:
return "BIGINT";
case FLOAT:
return "FLOAT";
case DECIMAL:
return "DECIMAL";
case TIMESTAMP:
return "TIMESTAMP";
case STRING:
return "VARCHAR";
default :
return "UNKNOWN";
}
}
