{
  DBBPool p1=new DBBPool();
  BBInputStream is=new BBInputStream();
  byte val=0;
  for (int i=0; i < 100; ++i) {
    DBBPool.BBContainer p1c=p1.acquire(4096);
    for (int j=0; j < 50; ++j) {
      p1c.b.put(val);
      if (val == 255) {
        val=0;
      }
 else {
        val++;
      }
    }
    p1c.b.flip();
    is.offer(p1c);
  }
  try {
    val=0;
    for (int i=0; i < 100; i++) {
      for (int j=0; j < 50; ++j) {
        assertEquals(val,is.read());
        if (val == 255) {
          val=0;
        }
 else {
          val++;
        }
      }
    }
  }
 catch (  Throwable e) {
    throw new RuntimeException(e);
  }
  p1.clear();
}
