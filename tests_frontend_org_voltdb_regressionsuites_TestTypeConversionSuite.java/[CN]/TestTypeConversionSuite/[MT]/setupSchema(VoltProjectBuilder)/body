{
  String literalSchema="CREATE TABLE T (\n" + "  ti TINYINT,\n" + "  si SMALLINT ,\n"+ "  int INTEGER NOT NULL ,\n"+ "  bi BIGINT ,\n"+ "  flt FLOAT ,\n"+ "  dec DECIMAL ,\n"+ "  ts TIMESTAMP ,\n"+ "  str VARCHAR(64),\n"+ "  bin VARBINARY(64),\n"+ "  pt GEOGRAPHY_POINT,\n"+ "  pol GEOGRAPHY \n"+ ");\n";
  project.addLiteralSchema(literalSchema);
  project.setUseDDLSchema(true);
}
