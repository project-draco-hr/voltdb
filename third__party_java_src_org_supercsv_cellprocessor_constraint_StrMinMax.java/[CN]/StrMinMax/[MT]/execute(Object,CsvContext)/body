{
  validateInputNotNull(value,context);
  final String stringValue=value.toString();
  final int length=stringValue.length();
  if (length < min || length > max) {
    throw new SuperCsvConstraintViolationException(String.format("the length (%d) of value '%s' does not lie between the min (%d) and max (%d) values (inclusive)",length,stringValue,min,max),context,this);
  }
  return next.execute(stringValue,context);
}
