@patch.object(jenkins.Jenkins, 'jenkins_open')
def test_raise_HTTPError(self, jenkins_mock):
    jenkins_mock.side_effect = jenkins.HTTPError(self.make_url('job/TestJob/api/json?depth=0'), code=401, msg='basic auth failed', hdrs=[], fp=None)
    with self.assertRaises(jenkins.JenkinsException) as context_manager:
        self.j.get_job_info(u'TestJob')
    self.assertEqual(jenkins_mock.call_args[0][0].get_full_url(), self.make_url('job/TestJob/api/json?depth=0'))
    self.assertEqual(str(context_manager.exception), 'job[TestJob] does not exist')
    self._check_requests(jenkins_mock.call_args_list)
