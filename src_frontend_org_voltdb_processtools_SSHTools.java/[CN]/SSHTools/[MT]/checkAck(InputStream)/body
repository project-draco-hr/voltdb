{
  int b=in.read();
  if (b == 0)   return b;
  if (b == -1)   return b;
  if (b == 1 || b == 2) {
    StringBuffer sb=new StringBuffer();
    int c;
    do {
      c=in.read();
      sb.append((char)c);
    }
 while (c != '\n');
    if (b == 1) {
      System.out.print(sb.toString());
    }
    if (b == 2) {
      System.out.print(sb.toString());
    }
  }
  return b;
}
